{"ast":null,"code":"import { computed } from 'vue';\nimport { XMarkIcon } from '@heroicons/vue/24/outline';\nimport { SquaresPlusIcon, MagnifyingGlassCircleIcon, FireIcon, BoltIcon, ServerIcon } from '@heroicons/vue/24/solid';\nimport { useRouter } from 'vue-router';\nimport vipBg from '@/assets/images/vip.png';\nexport default {\n  __name: 'ProDialog',\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n    const router = useRouter();\n    const dialogVisible = computed({\n      get: () => props.modelValue,\n      set: value => emit('update:modelValue', value)\n    });\n    const closeDialog = () => {\n      dialogVisible.value = false;\n    };\n    const handleUpgrade = () => {\n      closeDialog();\n      router.push('/pro');\n    };\n    const __returned__ = {\n      props,\n      emit,\n      router,\n      dialogVisible,\n      closeDialog,\n      handleUpgrade,\n      computed,\n      get XMarkIcon() {\n        return XMarkIcon;\n      },\n      get SquaresPlusIcon() {\n        return SquaresPlusIcon;\n      },\n      get MagnifyingGlassCircleIcon() {\n        return MagnifyingGlassCircleIcon;\n      },\n      get FireIcon() {\n        return FireIcon;\n      },\n      get BoltIcon() {\n        return BoltIcon;\n      },\n      get ServerIcon() {\n        return ServerIcon;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      get vipBg() {\n        return vipBg;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","XMarkIcon","SquaresPlusIcon","MagnifyingGlassCircleIcon","FireIcon","BoltIcon","ServerIcon","useRouter","vipBg","props","__props","emit","__emit","router","dialogV