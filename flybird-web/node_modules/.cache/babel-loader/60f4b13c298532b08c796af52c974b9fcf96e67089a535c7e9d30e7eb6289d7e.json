30 rounded-xl px-2 py-3\">\n                  <div class=\"w-12 h-12 mx-auto mb-0.5 rounded-lg flex items-center justify-center transition-all duration-300 group-hover:scale-110 group-hover:shadow-lg group-hover:shadow-yellow-400/20\">\n                    <ServerIcon class=\"w-7 h-7 text-yellow-600 transition-all duration-300 group-hover:text-yellow-500\" />\n                  </div>\n                  <span class=\"text-xs text-yellow-600 transition-colors duration-300 group-hover:text-yellow-500\">超大云存储</span>\n                </div>\n              </div>\n            </div>\n\n            <!-- 按钮 -->\n            <div class=\"absolute bottom-6 left-8 right-8\">\n              <button \n                @click=\"handleUpgrade\"\n                class=\"w-full py-3 bg-gradient-to-b from-[#EEA852] to-[#EC6833] text-white font-medium rounded-full hover:opacity-90 transition-all duration-300 transform hover:scale-105\"\n              >\n                立即开通\n              </button>\n            </div>\n          </div>\n        </div>\n\n        <!-- 关闭按钮 -->\n        <button \n          @click=\"closeDialog\"\n          class=\"mt-8 w-8 h-8 rounded-full border border-[#343A53] flex items-center justify-center hover:bg-white/10 transition-colors\"\n        >\n          <XMarkIcon class=\"w-5 h-5 text-[#343A53]\" />\n        </button>\n      </div>\n    </div>\n  </Teleport>\n</template>\n\n<script setup>\nimport { computed } from 'vue'\nimport { XMarkIcon } from '@heroicons/vue/24/outline'\nimport { \n  SquaresPlusIcon,\n  MagnifyingGlassCircleIcon,\n  FireIcon,\n  BoltIcon,\n  ServerIcon\n} from '@heroicons/vue/24/solid'\nimport { useRouter } from 'vue-router'\nimport vipBg from '@/assets/images/vip.png'\n\nconst props = defineProps({\n  modelValue: {\n    type: Boolean,\n    default: false\n  }\n})\n\nconst emit = defineEmits(['update:modelValue'])\nconst router = useRouter()\n\nconst dialogVisible = computed({\n  get: () => props.modelValue,\n  set: (value) => emit