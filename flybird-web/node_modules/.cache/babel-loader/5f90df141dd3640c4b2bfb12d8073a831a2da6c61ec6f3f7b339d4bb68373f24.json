{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport PageBanner from '@/components/common/PageBanner.vue';\nimport PortfolioNavigation from '@/components/portfolio/PortfolioNavigation.vue';\nimport { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nimport { EyeIcon, HeartIcon, PlusIcon, UserIcon, PlayIcon, FireIcon, ClockIcon, ChevronDownIcon, ArrowsUpDownIcon } from '@heroicons/vue/24/outline';\nimport { Switch } from '@headlessui/vue';\nexport default {\n  __name: 'DesignView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const route = useRoute();\n\n    // 使用 Vuex store 的 isAuthenticated 状态\n    const isAuthenticated = computed(() => store.state.isAuthenticated);\n\n    // 主导航数据\n    const mainNavs = [{\n      name: '发现',\n      path: '/portfolio'\n    }, {\n      name: '设计',\n      path: '/portfolio/design'\n    }, {\n      name: '视频',\n      path: '/portfolio/video'\n    }, {\n      name: '动画',\n      path: '/portfolio/animation'\n    }, {\n      name: '摄影',\n      path: '/portfolio/photo'\n    }];\n\n    // 当前选中的分类\n    const currentCategory = ref('all');\n    const currentSort = ref('popular');\n\n    // 设计分类数据\n    const designCategories = [{\n      id: 'all',\n      name: '全部'\n    }, {\n      id: 'ui',\n      name: 'UI设计'\n    }, {\n      id: 'graphic',\n      name: '平面设计'\n    }, {\n      id: 'brand',\n      name: '品牌设计'\n    }, {\n      id: 'web',\n      name: '网页设计'\n    }, {\n      id: 'illustration',\n      name: '插画'\n    }, {\n      id: 'icon',\n      name: '图标设计'\n    }];\n\n    // 模拟作品数据\n    const works = ref([{\n      id: 1,\n      title: '电商App设计',\n      type: 'UI设计',\n      description: '一个现代简约风格的电商App设计，包含完整的用户流程和交互设计方案...',\n      cover: 'https://picsum.photos/600/400?random=1',\n      author: {\n        name: '张小明',\n        avatar: 'https://picsum.photos/32/32?random=1',\n        title: '高级UI设计师'\n      },\n      views: 1234,\n      likes: 89\n    }, {\n      id: 2,\n      title: '品牌VI设计',\n      type: '品牌设计',\n      description: '科技公司品牌视觉识别系统设计，包含logo、色彩、字体等规范...',\n      cover: 'https://picsum.photos/600/400?random=2',\n      author: {\n        name: '李小红',\n        avatar: 'https://picsum.photos/32/32?random=2',\n        title: '品牌设计师'\n      },\n      views: 856,\n      likes: 67\n    }\n    // 可以添加更多作品数据...\n    ]);\n\n    // 排序相关\n    const showSortMenu = ref(false);\n    const showPremium = ref(false);\n\n    // 排序选项\n    const sortOptions = {\n      popular: {\n        label: '最受欢迎',\n        icon: FireIcon\n      },\n      newest: {\n        label: '最新发布',\n        icon: ClockIcon\n      },\n      views: {\n        label: '最多浏览',\n        icon: EyeIcon\n      },\n      likes: {\n        label: '最多点赞',\n        icon: HeartIcon\n      }\n    };\n\n    // 处理排序\n    const handleSort = value => {\n      currentSort.value = value;\n      showSortMenu.value = false;\n    };\n\n    // 根据分类过滤作品\n    const filteredWorks = computed(() => {\n      let result = [...works.value];\n\n      // 应用分类过滤\n      if (currentCategory.value !== 'all') {\n        result = result.filter(work => work.type === currentCategory.value);\n      }\n\n      // 应用会员筛选\n      if (showPremium.value) {\n        result = result.filter(work => work.isPremium);\n      }\n\n      // 应用排序\n      switch (currentSort.value) {\n        case 'popular':\n          result.sort((a, b) => b.views + b.likes - (a.views + a.likes));\n          break;\n        case 'newest':\n          result.sort((a, b) => new Date(b.createdAt) - new Date(a.createdAt));\n          break;\n        case 'views':\n          result.sort((a, b) => b.views - a.views);\n          break;\n        case 'likes':\n          result.sort((a, b) => b.likes - a.likes);\n          break;\n      }\n      return result;\n    });\n    const __returned__ = {\n      store,\n      route,\n      isAuthenticated,\n      mainNavs,\n      currentCategory,\n      currentSort,\n      designCategories,\n      works,\n      showSortMenu,\n      showPremium,\n      sortOptions,\n      handleSort,\n      filteredWorks,\n      PageBanner,\n      PortfolioNavigation,\n      ref,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get EyeIcon() {\n        return EyeIcon;\n      },\n      get HeartIcon() {\n        return HeartIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      },\n      get UserIcon() {\n        return UserIcon;\n      },\n      get PlayIcon() {\n        return PlayIcon;\n      },\n      get FireIcon() {\n        return FireIcon;\n      },\n      get ClockIcon() {\n        return ClockIcon;\n      },\n      get ChevronDownIcon() {\n        return ChevronDownIcon;\n      },\n      get ArrowsUpDownIcon() {\n        return ArrowsUpDownIcon;\n      },\n      get Switch() {\n        return Switch;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["PageBanner","PortfolioNavigation","ref","computed","useStore","useRoute","EyeIcon","HeartIcon","PlusIcon","UserIcon","PlayIcon","FireIcon","ClockIcon","ChevronDownIcon","ArrowsUpDownIcon","Switch","store","route","isAuthenticated","state","mainNavs","name","path","currentCategory","currentSort","designCategories","id","works","title","type","description","cover","author","avatar","views","likes","showSortMenu","showPremium","sortOptions","popular","label","icon","newest","handleSort","value","filteredWorks","result","filter","work","isPremium","sort","a","b","Date","createdAt"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/portfolio/DesignView.vue"],"sourcesContent":["<template>\n  <div class=\"py-4 lg:py-6 mt-[72px]\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅 -->\n      <PageBanner theme=\"blue\">\n        <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">设计作品</h1>\n        <p class=\"text-gray-600 text-lg max-w-2xl\">分享你的设计灵感，探索创意无限</p>\n      </PageBanner>\n\n      <!-- 使用导航组件 -->\n      <PortfolioNavigation v-model:currentCategory=\"currentCategory\" v-model:currentSort=\"currentSort\" />\n\n      <!-- 筛选和排序工具栏 -->\n      <div class=\"flex items-center justify-between mb-6\">\n        <!-- 会员模板开关 -->\n        <div class=\"flex items-center gap-2\">\n          <Switch\n            v-model=\"showPremium\"\n            class=\"relative inline-flex h-6 w-11 items-center rounded-full bg-gray-200\"\n            :class=\"[showPremium ? 'bg-blue-600' : 'bg-gray-200']\"\n          >\n            <span class=\"sr-only\">显示会员模板</span>\n            <span\n              class=\"inline-block h-4 w-4 transform rounded-full bg-white transition\"\n              :class=\"[showPremium ? 'translate-x-6' : 'translate-x-1']\"\n            />\n          </Switch>\n          <span class=\"text-sm text-gray-600\">会员模板</span>\n        </div>\n\n        <!-- 排序下拉菜单 -->\n        <div class=\"relative\">\n          <button \n            @click=\"showSortMenu = !showSortMenu\"\n            class=\"flex items-center gap-2 px-3 py-2 text-sm text-gray-600 hover:text-gray-900 transition-colors\"\n          >\n            <component :is=\"sortOptions[currentSort].icon\" class=\"w-4 h-4\" />\n            {{ sortOptions[currentSort].label }}\n            <ChevronDownIcon class=\"w-4 h-4\" />\n          </button>\n\n          <!-- 排序选项菜单 -->\n          <div v-if=\"showSortMenu\"\n            class=\"absolute right-0 mt-1 w-40 bg-white rounded-lg shadow-lg border border-gray-100 py-1 z-10\"\n          >\n            <button\n              v-for=\"(option, key) in sortOptions\"\n              :key=\"key\"\n              @click=\"handleSort(key)\"\n              class=\"w-full px-4 py-2 text-left text-sm hover:bg-gray-50 flex items-center gap-2\"\n              :class=\"{ 'text-blue-600': currentSort === key }\"\n            >\n              <component :is=\"option.icon\" class=\"w-4 h-4\" />\n              {{ option.label }}\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <!-- 作品列表 -->\n      <div class=\"grid grid-cols-2 lg:grid-cols-4 gap-4 lg:gap-6 mt-6\">\n        <div v-for=\"work in filteredWorks\" :key=\"work.id\"\n          class=\"group bg-white rounded-xl border border-gray-100 overflow-hidden hover:shadow-lg transition-all duration-300\"\n        >\n          <!-- 作品封面 -->\n          <div class=\"relative aspect-w-1 aspect-h-1 overflow-hidden\">\n            <img \n              :src=\"work.cover\" \n              class=\"w-full h-full object-cover transform group-hover:scale-105 transition-transform duration-500\"\n              alt=\"\"\n            >\n            <!-- 类型标签 -->\n            <div class=\"absolute top-4 left-4\">\n              <span class=\"px-3 py-1 bg-white/90 backdrop-blur-sm rounded-full text-xs font-medium text-gray-900\">\n                {{ work.type }}\n              </span>\n            </div>\n          </div>\n\n          <!-- 作品信息 -->\n          <div class=\"p-3 lg:p-4\">\n            <h3 class=\"text-sm font-medium text-gray-900 mb-2 line-clamp-1\">{{ work.title }}</h3>\n            \n            <!-- 作者信息和统计 -->\n            <div class=\"flex items-center justify-between\">\n              <div class=\"flex items-center space-x-2\">\n                <img :src=\"work.author.avatar\" class=\"w-5 h-5 rounded-full\">\n                <span class=\"text-sm text-gray-600\">{{ work.author.name }}</span>\n              </div>\n              <div class=\"flex items-center space-x-3 text-sm text-gray-500\">\n                <span class=\"flex items-center\">\n                  <EyeIcon class=\"w-4 h-4 mr-1\" />{{ work.views }}\n                </span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n  \n    </div>\n  </div>\n</template>\n\n<script setup>\nimport PageBanner from '@/components/common/PageBanner.vue'\nimport PortfolioNavigation from '@/components/portfolio/PortfolioNavigation.vue'\nimport { ref, computed } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute } from 'vue-router'\nimport { \n  EyeIcon, \n  HeartIcon,\n  PlusIcon,\n  UserIcon,\n  PlayIcon,\n  FireIcon,\n  ClockIcon,\n  ChevronDownIcon,\n  ArrowsUpDownIcon\n} from '@heroicons/vue/24/outline'\nimport { Switch } from '@headlessui/vue'\n\nconst store = useStore()\nconst route = useRoute()\n\n// 使用 Vuex store 的 isAuthenticated 状态\nconst isAuthenticated = computed(() => store.state.isAuthenticated)\n\n// 主导航数据\nconst mainNavs = [\n  { name: '发现', path: '/portfolio' },\n  { name: '设计', path: '/portfolio/design' },\n  { name: '视频', path: '/portfolio/video' },\n  { name: '动画', path: '/portfolio/animation' },\n  { name: '摄影', path: '/portfolio/photo' }\n]\n\n// 当前选中的分类\nconst currentCategory = ref('all')\nconst currentSort = ref('popular')\n\n// 设计分类数据\nconst designCategories = [\n  { id: 'all', name: '全部' },\n  { id: 'ui', name: 'UI设计' },\n  { id: 'graphic', name: '平面设计' },\n  { id: 'brand', name: '品牌设计' },\n  { id: 'web', name: '网页设计' },\n  { id: 'illustration', name: '插画' },\n  { id: 'icon', name: '图标设计' }\n]\n\n// 模拟作品数据\nconst works = ref([\n  {\n    id: 1,\n    title: '电商App设计',\n    type: 'UI设计',\n    description: '一个现代简约风格的电商App设计，包含完整的用户流程和交互设计方案...',\n    cover: 'https://picsum.photos/600/400?random=1',\n    author: {\n      name: '张小明',\n      avatar: 'https://picsum.photos/32/32?random=1',\n      title: '高级UI设计师'\n    },\n    views: 1234,\n    likes: 89\n  },\n  {\n    id: 2,\n    title: '品牌VI设计',\n    type: '品牌设计',\n    description: '科技公司品牌视觉识别系统设计，包含logo、色彩、字体等规范...',\n    cover: 'https://picsum.photos/600/400?random=2',\n    author: {\n      name: '李小红',\n      avatar: 'https://picsum.photos/32/32?random=2',\n      title: '品牌设计师'\n    },\n    views: 856,\n    likes: 67\n  },\n  // 可以添加更多作品数据...\n])\n\n// 排序相关\nconst showSortMenu = ref(false)\nconst showPremium = ref(false)\n\n// 排序选项\nconst sortOptions = {\n  popular: { label: '最受欢迎', icon: FireIcon },\n  newest: { label: '最新发布', icon: ClockIcon },\n  views: { label: '最多浏览', icon: EyeIcon },\n  likes: { label: '最多点赞', icon: HeartIcon }\n}\n\n// 处理排序\nconst handleSort = (value) => {\n  currentSort.value = value\n  showSortMenu.value = false\n}\n\n// 根据分类过滤作品\nconst filteredWorks = computed(() => {\n  let result = [...works.value]\n  \n  // 应用分类过滤\n  if (currentCategory.value !== 'all') {\n    result = result.filter(work => work.type === currentCategory.value)\n  }\n  \n  // 应用会员筛选\n  if (showPremium.value) {\n    result = result.filter(work => work.isPremium)\n  }\n\n  // 应用排序\n  switch (currentSort.value) {\n    case 'popular':\n      result.sort((a, b) => (b.views + b.likes) - (a.views + a.likes))\n      break\n    case 'newest':\n      result.sort((a, b) => new Date(b.createdAt) - new Date(a.createdAt))\n      break\n    case 'views':\n      result.sort((a, b) => b.views - a.views)\n      break\n    case 'likes':\n      result.sort((a, b) => b.likes - a.likes)\n      break\n  }\n\n  return result\n})\n</script> "],"mappings":";;AAwGA,OAAOA,UAAU,MAAM,oCAAmC;AAC1D,OAAOC,mBAAmB,MAAM,gDAA+C;AAC/E,SAASC,GAAG,EAAEC,QAAQ,QAAQ,KAAI;AAClC,SAASC,QAAQ,QAAQ,MAAK;AAC9B,SAASC,QAAQ,QAAQ,YAAW;AACpC,SACEC,OAAO,EACPC,SAAS,EACTC,QAAQ,EACRC,QAAQ,EACRC,QAAQ,EACRC,QAAQ,EACRC,SAAS,EACTC,eAAe,EACfC,gBAAe,QACV,2BAA0B;AACjC,SAASC,MAAM,QAAQ,iBAAgB;;;;;;;IAEvC,MAAMC,KAAK,GAAGZ,QAAQ,CAAC;IACvB,MAAMa,KAAK,GAAGZ,QAAQ,CAAC;;IAEvB;IACA,MAAMa,eAAe,GAAGf,QAAQ,CAAC,MAAMa,KAAK,CAACG,KAAK,CAACD,eAAe;;IAElE;IACA,MAAME,QAAQ,GAAG,CACf;MAAEC,IAAI,EAAE,IAAI;MAAEC,IAAI,EAAE;IAAa,CAAC,EAClC;MAAED,IAAI,EAAE,IAAI;MAAEC,IAAI,EAAE;IAAoB,CAAC,EACzC;MAAED,IAAI,EAAE,IAAI;MAAEC,IAAI,EAAE;IAAmB,CAAC,EACxC;MAAED,IAAI,EAAE,IAAI;MAAEC,IAAI,EAAE;IAAuB,CAAC,