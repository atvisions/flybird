{"ast":null,"code":"import { useCssVars as _useCssVars, unref as _unref } from 'vue';\nimport { ref, onMounted, onUnmounted, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  __name: 'index',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    _useCssVars(_ctx => ({\n      \"2c047126-scale\\ \\>\\=\\ 1\\ \\?\\ \\\"auto\\\"\\ \\:\\ \\\"hidden\\\"\": scale.value >= 1 ? \"auto\" : \"hidden\",\n      \"2c047126-scale\\ \\>\\=\\ 1\\ \\?\\ \\\"40px\\\"\\ \\:\\ \\\"0\\\"\": scale.value >= 1 ? \"40px\" : \"0\"\n    }));\n    const router = useRouter();\n    const scale = ref(1);\n    let isDragging = false;\n    let startX = 0;\n    let startScale = 0;\n    const activeTab = ref('layout');\n\n    // 组件分类和组件列表\n    const categories = ref([]);\n    const currentCategory = ref(null);\n    const components = ref([]);\n    const loading = ref(false);\n\n    // 获取组件分类\n    const fetchCategories = async () => {\n      try {\n        const response = await axios.get('/api/v1/resume/component-categories/');\n        categories.value = response.data;\n        if (categories.value.length > 0) {\n          currentCategory.value = categories.value[0].id;\n          fetchComponents(currentCategory.value);\n        }\n      } catch (error) {\n        console.error('获取组件分类失败:', error);\n      }\n    };\n\n    // 获取组件列表\n    const fetchComponents = async categoryId => {\n      if (!categoryId) return;\n      loading.value = true;\n      try {\n        const response = await axios.get(`/api/v1/resume/component-categories/${categoryId}/components/`);\n        components.value = response.data;\n      } catch (error) {\n        console.error('获取组件列表失败:', error);\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // 监听分类变化\n    watch(currentCategory, newCategoryId => {\n      if (newCategoryId) {\n        fetchComponents(newCategoryId);\n      }\n    });\n    onMounted(() => {\n      fetchCategories();\n    });\n\n    // 历史记录状态\n    const canUndo = ref(true); // 暂时设为 true 以显示按钮\n    const canRedo = ref(true); // 暂时设为 true 以显示按钮\n\n    // 历史记录相关方法\n    const handleUndo = () => {\n      // TODO: 实现撤销功能\n      console.log('undo');\n    };\n    const handleRedo = () => {\n      // TODO: 实现重做功能\n      console.log('redo');\n    };\n    const startDrag = e => {\n      isDragging = true;\n      startX = e.clientX;\n      startScale = scale.value;\n      document.addEventListener('mousemove', onDrag);\n      document.addEventListener('mouseup', stopDrag);\n    };\n    const onDrag = e => {\n      if (!isDragging) return;\n      const track = document.querySelector('.zoom-track');\n      const trackRect = track.getBoundingClientRect();\n      const trackWidth = trackRect.width;\n      const deltaX = e.clientX - startX;\n      const deltaScale = deltaX / trackWidth * 1.5; // 1.5 是范围 (2 - 0.5)\n\n      let newScale = startScale + deltaScale;\n      newScale = Math.min(2, Math.max(0.5, newScale));\n      newScale = Math.round(newScale * 10) / 10;\n      scale.value = newScale;\n    };\n    const stopDrag = () => {\n      isDragging = false;\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    };\n    onUnmounted(() => {\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    });\n    const goBack = () => {\n      router.back();\n    };\n    const handleExit = () => {\n      router.push('/resumes');\n    };\n\n    // 组件分类数据\n    const componentCategories = [{\n      text: '文本',\n      value: 'text',\n      icon: 'mdi-format-text'\n    }, {\n      text: '图片',\n      value: 'image',\n      icon: 'mdi-image'\n    }, {\n      text: '列表',\n      value: 'list',\n      icon: 'mdi-format-list-bulleted'\n    }, {\n      text: '表格',\n      value: 'table',\n      icon: 'mdi-table'\n    }, {\n      text: '时间线',\n      value: 'timeline',\n      icon: 'mdi-timeline'\n    }, {\n      text: '标题',\n      value: 'heading',\n      icon: 'mdi-format-header-1'\n    }, {\n      text: '段落',\n      value: 'paragraph',\n      icon: 'mdi-text'\n    }];\n    const __returned__ = {\n      router,\n      scale,\n      get isDragging() {\n        return isDragging;\n      },\n      set isDragging(v) {\n        isDragging = v;\n      },\n      get startX() {\n        return startX;\n      },\n      set startX(v) {\n        startX = v;\n      },\n      get startScale() {\n        return startScale;\n      },\n      set startScale(v) {\n        startScale = v;\n      },\n      activeTab,\n      categories,\n      currentCategory,\n      components,\n      loading,\n      fetchCategories,\n      fetchComponents,\n      canUndo,\n      canRedo,\n      handleUndo,\n      handleRedo,\n      startDrag,\n      onDrag,\n      stopDrag,\n      goBack,\n      handleExit,\n      componentCategories,\n      ref,\n      onMounted,\n      onUnmounted,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","watch","useRouter","axios","router","scale","isDragging","startX","startScale","activeTab","categories","currentCategory","components","loading","fetchCategories","response","get","value","data","length","id","fetchComponents","error","console","categoryId","newCategoryId","canUndo","canRedo","handleUndo","log","handleRedo","startDrag","e","clientX","document","addEventListener","onDrag","stopDrag","track","querySelector","trackRect","getBoundingClientRect","trackWidth","width","deltaX","deltaScale","newScale","Math","min","max","round","removeEventListener","goBack","back","handleExit","push","componentCategories","text","icon"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/resume-editor/index.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"resume-editor\">\n      <!-- 顶部栏 -->\n      <v-app-bar class=\"editor-header\" elevation=\"0\" height=\"60\">\n        <!-- 左侧：退出箭头和简历名称 -->\n        <div class=\"d-flex align-center justify-space-between w-100\">\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"返回\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon @click=\"handleExit\" v-bind=\"props\">\n                  <v-icon>mdi-arrow-left</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-divider vertical class=\"mx-3\"></v-divider>\n            <span class=\"text-h6\">我的简历</span>\n          </div>\n\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"撤销\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" :disabled=\"!canUndo\" v-bind=\"props\" @click=\"handleUndo\">\n                  <v-icon>mdi-undo</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"重做\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon :disabled=\"!canRedo\" v-bind=\"props\" @click=\"handleRedo\">\n                  <v-icon>mdi-redo</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n          </div>\n\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"草稿箱\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\">\n                  <v-icon>mdi-file-document-outline</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"打印预览\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\">\n                  <v-icon>mdi-printer</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"保存\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon v-bind=\"props\">\n                  <v-icon>mdi-content-save</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n          </div>\n        </div>\n      </v-app-bar>\n\n      <!-- 主要内容区域 -->\n      <div class=\"editor-main\">\n        <!-- 左侧工具栏 -->\n        <div class=\"editor-toolbar\">\n          <div class=\"d-flex\">\n            <v-card class=\"h-100 d-flex flex-column toolbar-content\">\n              <!-- 布局面板 -->\n              <v-window v-model=\"activeTab\">\n                <v-window-item value=\"layout\">\n                  <v-card-title class=\"text-body-1 py-2\">布局</v-card-title>\n                  <v-divider></v-divider>\n                  <v-card-text class=\"flex-grow-1 overflow-y-auto pa-2\">\n                    <div class=\"layout-tip\">拖动以下布局到画布中开始设计简历</div>\n                    <div class=\"layout-blocks\">\n                      <div class=\"layout-block\">\n                        <div class=\"block-icon\">\n                          <v-icon size=\"24\">mdi-view-sequential</v-icon>\n                        </div>\n                        <div class=\"block-content\">\n                          <div class=\"block-title\">单栏布局</div>\n                          <div class=\"block-desc\">适用于简洁的个人简历</div>\n                        </div>\n                        <v-icon size=\"18\" color=\"rgba(0,0,0,0.4)\" class=\"block-arrow\">mdi-chevron-right</v-icon>\n                      </div>\n\n                      <div class=\"layout-block\">\n                        <div class=\"block-icon\">\n                          <v-icon size=\"24\">mdi-view-parallel</v-icon>\n                        </div>\n                        <div class=\"block-content\">\n                          <div class=\"block-title\">双栏布局</div>\n                          <div class=\"block-desc\">适用于信息较多的简历</div>\n                        </div>\n                        <v-icon size=\"18\" color=\"rgba(0,0,0,0.4)\" class=\"block-arrow\">mdi-chevron-right</v-icon>\n                      </div>\n\n                      <div class=\"layout-block\">\n                        <div class=\"block-icon\">\n                          <v-icon size=\"24\">mdi-view-column</v-icon>\n                        </div>\n                        <div class=\"block-content\">\n                          <div class=\"block-title\">三栏布局</div>\n                          <div class=\"block-desc\">适用于内容丰富的简历</div>\n                        </div>\n                        <v-icon size=\"18\" color=\"rgba(0,0,0,0.4)\" class=\"block-arrow\">mdi-chevron-right</v-icon>\n                      </div>\n                    </div>\n                  </v-card-text>\n                </v-window-item>\n\n                <!-- 组件面板 -->\n                <v-window-item value=\"components\">\n                  <v-card-title class=\"text-body-1 py-2\">组件</v-card-title>\n                  <v-divider></v-divider>\n                  <v-card-text class=\"flex-grow-1 overflow-hidden pa-0\">\n                    <div class=\"component-container\">\n                      <!-- 左侧分类导航 -->\n                      <div class=\"component-nav\">\n                        <div class=\"component-nav-item\" \n                          v-for=\"(category, index) in componentCategories\" \n                          :key=\"index\"\n                          :class=\"{ active: currentCategory === category.value }\"\n                          @click=\"currentCategory = category.value\">\n                          <v-icon size=\"20\" class=\"mb-1\">{{ category.icon }}</v-icon>\n                          <div class=\"nav-text\">{{ category.text }}</div>\n                        </div>\n                      </div>\n\n                      <!-- 右侧组件列表 -->\n                      <div class=\"component-list\">\n                        <div class=\"component-tip px-4 pt-2\">拖动组件到画布中编辑简历内容</div>\n                        <div class=\"component-scroll\">\n                          <div class=\"component-blocks px-4\">\n                            <div class=\"component-block\" v-for=\"n in 10\" :key=\"n\">\n                              <div class=\"block-icon\">\n                                <v-icon size=\"18\">mdi-format-title</v-icon>\n                              </div>\n                              <div class=\"block-content\">\n                                <div class=\"block-title\">标题样式 {{ n }}</div>\n                              </div>\n                            </div>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </v-card-text>\n                </v-window-item>\n\n                <!-- 档案面板 -->\n                <v-window-item value=\"files\">\n                  <v-card-title class=\"text-body-1 py-2\">档案</v-card-title>\n                  <v-divider></v-divider>\n                  <v-card-text class=\"flex-grow-1 overflow-y-auto pa-2\">\n                    <v-list density=\"compact\">\n                      <v-list-item>\n                        <v-list-item-title>我的模板</v-list-item-title>\n                      </v-list-item>\n                      <v-list-item>\n                        <v-list-item-title>历史版本</v-list-item-title>\n                      </v-list-item>\n                    </v-list>\n                  </v-card-text>\n                </v-window-item>\n              </v-window>\n            </v-card>\n\n            <!-- 竖向标签页 -->\n            <div class=\"vertical-tabs\">\n              <div class=\"tab-items\">\n                <div class=\"tab-item\" \n                  :class=\"{ active: activeTab === 'layout' }\"\n                  @click=\"activeTab = 'layout'\">\n                  布局\n                </div>\n                <div class=\"tab-item\" \n                  :class=\"{ active: activeTab === 'components' }\"\n                  @click=\"activeTab = 'components'\">\n                  组件\n                </div>\n                <div class=\"tab-item\" \n                  :class=\"{ active: activeTab === 'files' }\"\n                  @click=\"activeTab = 'files'\">\n                  档案\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 中间画布区域 -->\n        <div class=\"editor-canvas\">\n          <v-card class=\"h-100\">\n            <v-card-text class=\"fill-height d-flex align-center justify-center pa-0\">\n              <div class=\"canvas-container\">\n                <div class=\"canvas-wrapper\">\n                  <div class=\"canvas-content\" :style=\"{ transform: `scale(${scale})` }\">\n                    画布区域\n                  </div>\n                </div>\n              </div>\n            </v-card-text>\n          </v-card>\n        </div>\n\n        <!-- 右侧操作栏 -->\n        <div class=\"editor-properties\">\n          <v-card class=\"h-100\">\n            <v-card-title class=\"text-body-1 py-2\">属性设置</v-card-title>\n            <v-divider></v-divider>\n            <v-card-text class=\"flex-grow-1 overflow-y-auto pa-2\">\n              <!-- 这里将放置属性设置面板 -->\n            </v-card-text>\n          </v-card>\n        </div>\n      </div>\n\n      <!-- 底部通栏 -->\n      <div class=\"editor-footer\">\n        <div class=\"d-flex align-center justify-center h-100\">\n          <div class=\"zoom-control\">\n            <span class=\"zoom-label zoom-label-left\">50%</span>\n            <div class=\"zoom-track\">\n              <div class=\"zoom-slider\" \n                :style=\"{ left: ((scale - 0.5) / 1.5 * 100) + '%' }\"\n                @mousedown=\"startDrag\">\n                <div class=\"zoom-value\">{{ Math.round(scale * 100) }}%</div>\n              </div>\n              <div class=\"zoom-line\"></div>\n            </div>\n            <span class=\"zoom-label zoom-label-right\">200%</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </v-app>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport axios from 'axios'\n\nconst router = useRouter()\nconst scale = ref(1)\nlet isDragging = false\nlet startX = 0\nlet startScale = 0\nconst activeTab = ref('layout')\n\n// 组件分类和组件列表\nconst categories = ref([])\nconst currentCategory = ref(null)\nconst components = ref([])\nconst loading = ref(false)\n\n// 获取组件分类\nconst fetchCategories = async () => {\n  try {\n    const response = await axios.get('/api/v1/resume/component-categories/')\n    categories.value = response.data\n    if (categories.value.length > 0) {\n      currentCategory.value = categories.value[0].id\n      fetchComponents(currentCategory.value)\n    }\n  } catch (error) {\n    console.error('获取组件分类失败:', error)\n  }\n}\n\n// 获取组件列表\nconst fetchComponents = async (categoryId) => {\n  if (!categoryId) return\n  loading.value = true\n  try {\n    const response = await axios.get(`/api/v1/resume/component-categories/${categoryId}/components/`)\n    components.value = response.data\n  } catch (error) {\n    console.error('获取组件列表失败:', error)\n  } finally {\n    loading.value = false\n  }\n}\n\n// 监听分类变化\nwatch(currentCategory, (newCategoryId) => {\n  if (newCategoryId) {\n    fetchComponents(newCategoryId)\n  }\n})\n\nonMounted(() => {\n  fetchCategories()\n})\n\n// 历史记录状态\nconst canUndo = ref(true)  // 暂时设为 true 以显示按钮\nconst canRedo = ref(true)  // 暂时设为 true 以显示按钮\n\n// 历史记录相关方法\nconst handleUndo = () => {\n  // TODO: 实现撤销功能\n  console.log('undo')\n}\n\nconst handleRedo = () => {\n  // TODO: 实现重做功能\n  console.log('redo'