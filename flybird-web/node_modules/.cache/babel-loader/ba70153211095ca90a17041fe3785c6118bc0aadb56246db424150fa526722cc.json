{"ast":null,"code":"import { computed, ref } from 'vue';\nimport HeadView from '@/components/HeadView.vue';\nimport { useRegister } from '@/composables/useRegister';\nimport { auth } from '@/api/auth';\nimport { showToast } from '@/components/ToastMessage';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nexport default {\n  __name: 'RegisterView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const router = useRouter();\n    const {\n      form,\n      loading,\n      sendingCode,\n      agreed,\n      showPassword\n    } = useRegister();\n\n    // 添加倒计时状态\n    const countdown = ref(0);\n\n    // 倒计时管理\n    const startCountdown = () => {\n      countdown.value = 60;\n      const timer = setInterval(() => {\n        countdown.value--;\n        if (countdown.value <= 0) {\n          clearInterval(timer);\n        }\n      }, 1000);\n    };\n\n    // 表单是否有效\n    const isFormValid = computed(() => {\n      return form.value.phone && form.value.code && form.value.password && form.value.confirmPassword && agreed.value;\n    });\n\n    // 切换密码显示状态\n    const togglePassword = () => {\n      showPassword.value = !showPassword.value;\n    };\n\n    // 添加手机号验证\n    const validatePhone = () => {\n      if (!form.value.phone) {\n        showToast('请输入手机号', 'warning');\n        return false;\n      }\n      const phoneRegex = /^1[3-9]\\d{9}$/;\n      if (!phoneRegex.test(form.value.phone)) {\n        showToast('请输入正确的手机号格式', 'warning');\n        return false;\n      }\n      return true;\n    };\n\n    // 发送验证码\n    const handleSendCode = async () => {\n      if (!validatePhone()) {\n        return;\n      }\n      try {\n        sendingCode.value = true;\n        const response = await auth.sendVerifyCode({\n          phone: form.value.phone,\n          scene: 'register'\n        });\n        if (response?.data?.code === 200) {\n          startCountdown();\n          showToast(response.data.message, 'success');\n        } else {\n          throw new Error(response.data.message || '发送验证码失败');\n        }\n      } catch (error) {\n        console.error('发送验证码失败:', error);\n        if (error.response) {\n          console.error('完整错误响应:', {\n            status: error.response.status,\n            statusText: error.response.statusText,\n            data: error.response.data,\n            headers: error.response.headers,\n            config: {\n              url: error.response.config.url,\n              method: error.response.config.method,\n              data: error.response.config.data\n            }\n          });\n        }\n        const errorMsg = error.response?.data?.message || error.response?.data?.detail || error.message || '发送验证码失败';\n        showToast(errorMsg, 'error');\n      } finally {\n        sendingCode.value = false;\n      }\n    };\n\n    // 处理注册\n    const handleRegister = async () => {\n      try {\n        loading.value = true;\n        const response = await auth.register({\n          phone: form.value.phone,\n          code: form.value.code,\n          password: form.value.password,\n          confirmPassword: form.value.confirmPassword\n        });\n        if (response?.data?.code === 200) {\n          // 保存 token\n          localStorage.setItem('token', response.data.data.access);\n          localStorage.setItem('refresh_token', response.data.data.refresh);\n\n          // 更新登录状态\n          store.commit('SET_LOGGED_IN', true);\n          await store.dispatch('getUserInfo');\n          showToast('注册成功', 'success');\n          router.push('/user?tab=home');\n        }\n      } catch (error) {\n        console.error('注册失败:', error);\n        const errorMsg = error.response?.data?.message || error.response?.data?.detail || error.message || '注册失败';\n        showToast(errorMsg, 'error');\n      } finally {\n        loading.value = false;\n      }\n    };\n    const __returned__ = {\n      store,\n      router,\n      form,\n      loading,\n      sendingCode,\n      agreed,\n      showPassword,\n      countdown,\n      startCountdown,\n      isFormValid,\n      togglePassword,\n      validatePhone,\n      handleSendCode,\n      handleRegister,\n      computed,\n      ref,\n      HeadView,\n      get useRegister() {\n        return useRegister;\n      },\n      get auth() {\n        return auth;\n      },\n      get showToast() {\n        return showToast;\n      },\n      get useStore() {\n        return useStore;\n      },\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","ref","HeadView","useRegister","auth","showToast","useStore","useRouter","store","router","form","loading","sendingCode","agreed","showPassword","countdown","startCountdown","value","timer","setInterval","clearInterval","isFormValid","phone","code","password","confirmPassword","togglePassword","validatePhone","phoneRegex","test","handleSendCode","response","sendVerifyCode","scene","data","message","Error","error","console","status","statusText","headers","config","url","method","errorMsg","detail","handleRegister","register","localStorage","setItem","access","refresh","commit","dispatch","push"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col min-h-screen\">\n  <HeadView />\n  <main class=\"flex-1 pt-14 relative z-[1] bg-gray-50\">\n    <div class=\"sm:mx-auto sm:w-full sm:max-w-md pt-8\">\n      <h2 class=\"mt-6 text-center text-2xl font-bold leading-9 tracking-tight text-gray-900\">注册账号</h2>\n    </div>\n    <div class=\"mt-10 sm:mx-auto sm:w-full sm:max-w-[480px]\">\n      <div class=\"bg-white px-6 py-12 shadow sm:rounded-lg sm:px-12\">\n        <form class=\"space-y-6\" @submit.prevent=\"handleRegister\">\n          <!-- 手机号输入 -->\n          <div>\n            <label for=\"phone\" class=\"block text-sm font-medium leading-6 text-gray-900\">手机号</label>\n            <div class=\"mt-2\">\n              <input \n                id=\"phone\" \n                v-model=\"form.phone\" \n                type=\"text\" \n                required\n                placeholder=\"请输入手机号\"\n                class=\"block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\" \n              />\n            </div>\n          </div>\n\n          <!-- 验证码输入 -->\n          <div>\n            <label for=\"code\" class=\"block text-sm font-medium leading-6 text-gray-900\">验证码</label>\n            <div class=\"mt-2 flex\">\n              <div class=\"flex-grow\">\n                <input \n                  id=\"code\" \n                  v-model=\"form.code\" \n                  type=\"text\" \n                  required\n                  placeholder=\"请输入验证码\"\n                  class=\"block w-full rounded-l-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\" \n                />\n              </div>\n              <button \n                type=\"button\"\n                @click=\"handleSendCode\"\n                :disabled=\"sendingCode || countdown > 0\"\n                class=\"relative -ml-px whitespace-nowrap inline-flex items-center rounded-r-md px-6 py-2 text-sm font-semibold ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-10 w-[140px]\"\n                :class=\"sendingCode ? 'text-gray-400 bg-gray-50' : 'text-indigo-600'\"\n              >\n                <svg v-if=\"sendingCode\" class=\"animate-spin -ml-1 mr-2 h-4 w-4\" fill=\"none\" viewBox=\"0 0 24 24\">\n                  <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"></circle>\n                  <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z\"></path>\n                </svg>\n                <span class=\"w-full text-center\">\n                  {{ countdown > 0 ? `${countdown}s` : '获取验证码' }}\n                </span>\n              </button>\n            </div>\n          </div>\n\n          <!-- 密码输入 -->\n          <div>\n            <label for=\"password\" class=\"block text-sm font-medium leading-6 text-gray-900\">密码</label>\n            <div class=\"mt-2 relative\">\n              <input \n                id=\"password\" \n                v-model=\"form.password\" \n                :type=\"showPassword ? 'text' : 'password'\" \n                required\n                placeholder=\"请输入密码\"\n                class=\"block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\" \n              />\n              <button \n                type=\"button\" \n                @click=\"togglePassword\"\n                class=\"absolute inset-y-0 right-0 pr-3 flex items-center\"\n              >\n                <svg v-if=\"showPassword\" class=\"h-5 w-5 text-gray-500\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 12a3 3 0 11-6 0 3 3 0 016 0z\" />\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z\" />\n                </svg>\n                <svg v-else class=\"h-5 w-5 text-gray-500\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M13.875 18.825A10.05 10.05 0 0112 19c-4.478 0-8.268-2.943-9.543-7a9.97 9.97 0 011.563-3.029m5.858.908a3 3 0 114.243 4.243M9.878 9.878l4.242 4.242M9.88 9.88l-3.29-3.29m7.532 7.532l3.29 3.29M3 3l3.59 3.59m0 0A9.953 9.953 0 0112 5c4.478 0 8.268 2.943 9.543 7a10.025 10.025 0 01-4.132 5.411m0 0L21 21\" />\n                </svg>\n              </button>\n            </div>\n          </div>\n\n          <!-- 确认密码输入 -->\n          <div>\n            <label for=\"confirmPassword\" class=\"block text-sm font-medium leading-6 text-gray-900\">确认密码</label>\n            <div class=\"mt-2 relative\">\n              <input \n                id=\"confirmPassword\" \n                v-model=\"form.confirmPassword\"\n                :type=\"showPassword ? 'text' : 'password'\"\n                required\n                placeholder=\"请确认密码\"\n                class=\"block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\" \n              />\n              <button \n                type=\"button\" \n                @click=\"togglePassword\"\n                class=\"absolute inset-y-0 right-0 pr-3 flex items-center\"\n              >\n                <svg v-if=\"showPassword\" class=\"h-5 w-5 text-gray-500\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 12a3 3 0 11-6 0 3 3 0 016 0z\" />\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z\" />\n                </svg>\n                <svg v-else class=\"h-5 w-5 text-gray-500\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M13.875 18.825A10.05 10.05 0 0112 19c-4.478 0-8.268-2.943-9.543-7a9.97 9.97 0 011.563-3.029m5.858.908a3 3 0 114.243 4.243M9.878 9.878l4.242 4.242M9.88 9.88l-3.29-3.29m7.532 7.532l3.29 3.29M3 3l3.59 3.59m0 0A9.953 9.953 0 0112 5c4.478 0 8.268 2.943 9.543 7a10.025 10.025 0 01-4.132 5.411m0 0L21 21\" />\n                </svg>\n              </button>\n            </div>\n          </div>\n\n          <!-- 服务条款 -->\n          <div class=\"flex items-center\">\n            <input \n              id=\"agree-terms\" \n              v-model=\"agreed\" \n              type=\"checkbox\" \n              required\n              class=\"h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600\" \n            />\n            <label for=\"agree-terms\" class=\"ml-3 block text-sm leading-6 text-gray-900\">\n              我已阅读并同意\n              <a href=\"#\" class=\"font-semibold text-indigo-600 hover:text-indigo-500\">服务条款</a>\n              和\n              <a href=\"#\" class=\"font-semibold text-indigo-600 hover:text-indigo-500\">隐私政策</a>\n            </label>\n          </div>\n\n          <!-- 注册按钮 -->\n          <div>\n            <button \n              type=\"submit\" \n              :disabled=\"!isFormValid || loading\"\n              class=\"flex w-full justify-center rounded-md bg-indigo-600 px-3 py-1.5 text-sm font-semibold leading-6 text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600 disabled:opacity-50 disabled:cursor-not-allowed\"\n            >\n              <svg v-if=\"loading\" class=\"animate-spin -ml-1 mr-3 h-5 w-5 text-white\" fill=\"none\" viewBox=\"0 0 24 24\">\n                <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"></circle>\n                <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z\"></path>\n              </svg>\n              {{ loading ? '注册中...' : '注册' }}\n            </button>\n          </div>\n        </form>\n\n        <!-- 登录链接 -->\n        <div>\n          <div class=\"relative mt-10\">\n            <div class=\"absolute inset-0 flex items-center\" aria-hidden=\"true\">\n              <div class=\"w-full border-t border-gray-200\"></div>\n            </div>\n            <div class=\"relative flex justify-center text-sm font-medium leading-6\">\n              <span class=\"bg-white px-6 text-gray-900\">已有账号?</span>\n            </div>\n          </div>\n\n          <div class=\"mt-6\">\n            <router-link \n              to=\"/login\"\n              class=\"flex w-full justify-center rounded-md bg-white px-3 py-1.5 text-sm font-semibold leading-6 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600\"\n            >\n              登录\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </div>\n\n</main>\n</div>\n</template>\n\n<script setup>\nimport { computed, ref } from 'vue'\nimport HeadView from '@/components/HeadView.vue'\nimport { useRegister } from '@/composables/useRegister'\nimport { auth } from '@/api/auth'\nimport { showToast } from '@/components/ToastMessage'\nimport { useStore } from 'vuex'\nimport { useRouter } from 'vue-router'\n\nconst store = useStore()\nconst router = useRouter()\n\nconst {\n  form,\n  loading,\n  sendingCode,\n  agreed,\n  showPassword\n} = useRegister()\n\n// 添加倒计时状态\nconst countdown = ref(0)\n\n// 倒计时管理\nconst startCountdown = () => {\n  countdown.value = 60\n  const timer = setInterval(() => {\n    countdown.value--\n    if (countdown.value <= 0) {\n      clearInterval(timer)\n    }\n  }, 1000)\n}\n\n// 表单是否有效\nconst isFormValid = computed(() => {\n  return form.value.phone && \n         form.value.code && \n         form.value.password && \n         form.value.confirmPassword && \n         agreed.value\n})\n\n// 切换密码显示状态\nconst togglePassword = () => {\n  showPassword.value = !showPassword.value\n}\n\n// 添加手机号验证\nconst validatePhone = () => {\n  if (!form.value.phone) {\n    showToast('请输入手机号', 'warning')\n    return false\n  }\n  const phoneRegex = /^1[3-9]\\d{9}$/\n  if (!phoneRegex.test(form.value.phone)) {\n    showToast('请输入正确的手机号格式', 'warning')\n    return false\n  }\n  return true\n}\n\n// 发送验证码\nconst handleSendCode = async () => {\n  if (!validatePhone()) {\n    return\n  }\n\n  try {\n    sendingCode.value = true\n    const response = await auth.sendVerifyCode({\n      phone: form.value.phone,\n      scene: 'register'\n    })\n    \n    if (response?.data?.code === 200) {\n      startCountdown()\n      showToast(response.data.message, 'success')\n    } else {\n      throw new Error(response.data.message || '发送验证码失败')\n    }\n  } catch (error) {\n    console.error('发送验证码失败:', error)\n    if (error.response) {\n      console.error('完整错误响应:', {\n        status: error.response.status,\n        statusText: error.response.statusText,\n        data: error.response.data,\n        headers: error.response.headers,\n        config: {\n          url: error.response.config.url,\n          method: error.response.config.method,\n          data: error.response.config.data\n        }\n      })\n    }\n    const errorMsg = error.response?.data?.message || \n                    error.response?.data?.detail ||\n                    error.message || \n                    '发送验证码失败'\n    showToast(errorMsg, 'error')\n  } finally {\n    sendingCode.value = false\n  }\n}\n\n// 处理注册\nconst handleRegister = async () => {\n  try {\n    loading.value = true\n    const response = await auth.register({\n      phone: form.value.phone,\n      code: form.value.code,\n      password: form.value.password,\n      confirmPassword: form.value.confirmPassword\n    })\n\n    if (response?.data?.code === 200) {\n      // 保存 token\n      localStorage.setItem('token', response.data.data.access)\n      localStorage.setItem('refresh_token', response.data.data.refresh)\n      \n      // 更新登录状态\n      store.commit('SET_LOGGED_IN', true)\n      await store.dispatch('getUserInfo')\n      \n      showToast('注册成功', 'success')\n      router.push('/user?tab=home')\n    }\n  } catch (error) {\n    console.error('注册失败:', error)\n    const errorMsg = error.response?.data?.message || \n                    error.response?.data?.detail ||\n                    error.message || \n                    '注册失败'\n    showToast(errorMsg, 'error')\n  } finally {\n    loading.value = false\n  }\n}\n</script>"],"mappings":"AA8KA,SAASA,QAAQ,EAAEC,GAAG,QAAQ,KAAI;AAClC,OAAOC,QAAQ,MAAM,2BAA0B;AAC/C,SAASC,WAAW,QAAQ,2BAA0B;AACtD,SAASC,IAAI,QAAQ,YAAW;AAChC,SAASC,SAAS,QAAQ,2BAA0B;AACpD,SAASC,QAAQ,QAAQ,MAAK;AAC9B,SAASC,SAAS,QAAQ,YAAW;;;;;;;IAErC,MAAMC,KAAK,GAAGF,QAAQ,CAAC;IACvB,MAAMG,MAAM,GAAGF,SAAS,CAAC;IAEzB,MAAM;MACJG,IAAI;MACJC,OAAO;MACPC,WAAW;MACXC,MAAM;MACNC;IACF,CAAC,GAAGX,WAAW,CAAC;;IAEhB;IACA,MAAMY,SAAS,GAAGd,GAAG,CAAC,CAAC;;IAEvB;IACA,MAAMe,cAAc,GAAGA,CAAA,KAAM;MAC3BD,SAAS,CAACE,KAAK,GAAG,EAAC;MACnB,MAAMC,KAAK,GAAGC,WAAW,CAAC,MAAM;QAC9BJ,SAAS,CAACE,KAAK,EAAC;QAChB,IAAIF,SAAS,CAACE,KAAK,IAAI,CAAC,EAAE;UACxBG,aAAa,CAACF,KAAK;QACrB;MACF,CAAC,EAAE,IAAI;IACT;;IAEA;IACA,MAAMG,WAAW,GAAGrB,QAAQ,CAAC,MAAM;MACjC,OAAOU,IAAI,CAACO,KAAK,CAACK,KAAK,IAChBZ,IAAI,CAACO,KAAK,CAACM,IAAI,IACfb,IAAI,CAACO,KAAK,CAACO,QAAQ,IACnBd,IAAI,CAACO,KAAK,CAACQ,eAAe,IAC1BZ,MAAM,CAACI,KAAI;IACpB,CAAC;;IAED;IACA,MAAMS,cAAc,GAAGA,CAAA,KAAM;MAC3BZ,YAAY,CAACG,KAAK,GAAG,CAACH,YAAY,CAACG,KAAI;IACzC;;IAEA;IACA,MAAMU,aAAa,GAAGA,CAAA,KAAM;MAC1B,IAAI,CAACjB,IAAI,CAACO,KAAK,CAACK,KAAK,EAAE;QACrBjB,SAAS,CAAC,QAAQ,EAAE,SAAS;QAC7B,OAAO,KAAI;MACb;MACA,MAAMuB,UAAU,GAAG,eAAc;MACjC,IAAI,CAACA,UAAU,CAACC,IAAI,CAACnB,IAAI,CAACO,KAA