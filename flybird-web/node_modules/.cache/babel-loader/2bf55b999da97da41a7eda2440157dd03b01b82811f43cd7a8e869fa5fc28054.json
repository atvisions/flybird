{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport { ref, computed, watch } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nimport MobileTabBar from '@/components/MobileTabBar.vue';\nimport PageBanner from '@/components/common/PageBanner.vue';\nimport PortfolioNavigation from '@/components/portfolio/PortfolioNavigation.vue';\nimport CategoryMenu from '@/components/portfolio/CategoryMenu.vue';\nimport { portfolioCategories, generateMenuGroups } from '@/config/portfolioCategories';\nimport { EyeIcon, HeartIcon, ChevronRightIcon, PlayIcon, XMarkIcon, ChevronLeftIcon, FireIcon, CalendarIcon, ShareIcon } from '@heroicons/vue/24/outline';\nexport default {\n  __name: 'HomeView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const route = useRoute();\n\n    // 使用统一的分类配置\n    const categoryConfig = portfolioCategories.discover;\n    const menuGroups = generateMenuGroups('discover');\n\n    // 使用 Vuex store 的 isAuthenticated 状态\n    const isAuthenticated = computed(() => store.state.isAuthenticated);\n\n    // 当前选中的分类\n    const currentCategory = ref('all');\n    const showCategoryMenu = ref(false);\n    const showSortMenu = ref(false);\n    const currentSort = ref('popular');\n\n    // 排序选项\n    const sortOptions = {\n      popular: {\n        label: '最受欢迎',\n        icon: FireIcon\n      },\n      newest: {\n        label: '最新发布',\n        icon: CalendarIcon\n      },\n      views: {\n        label: '最多浏览',\n        icon: ShareIcon\n      },\n      likes: {\n        label: '最多点赞',\n        icon: HeartIcon\n      }\n    };\n\n    // 模拟作品数据\n    const works = ref([{\n      id: 1,\n      title: '智能家居App UI设计',\n      type: 'ui',\n      cover: 'https://picsum.photos/600/400?random=1',\n      author: {\n        name: '张小明',\n        avatar: 'https://picsum.photos/32/32?random=1'\n      },\n      views: 1234,\n      likes: 89,\n      isVideo: false\n    }, {\n      id: 2,\n      title: '2024科技峰会品牌设计',\n      type: 'graphic',\n      cover: 'https://picsum.photos/600/400?random=2',\n      author: {\n        name: '李晓华',\n        avatar: 'https://picsum.photos/32/32?random=2'\n      },\n      views: 856,\n      likes: 67,\n      isVideo: false\n    }, {\n      id: 3,\n      title: '产品宣传创意视频',\n      type: 'video',\n      cover: 'https://picsum.photos/600/400?random=3',\n      author: {\n        name: '王大力',\n        avatar: 'https://picsum.photos/32/32?random=3'\n      },\n      views: 2341,\n      likes: 178,\n      isVideo: true,\n      duration: '2:35'\n    }, {\n      id: 4,\n      title: '企业品牌动画',\n      type: 'animation',\n      cover: 'https://picsum.photos/600/400?random=4',\n      author: {\n        name: '赵明',\n        avatar: 'https://picsum.photos/32/32?random=4'\n      },\n      views: 1567,\n      likes: 92,\n      isVideo: true,\n      duration: '1:45'\n    }, {\n      id: 5,\n      title: '电商网站设计',\n      type: 'web',\n      cover: 'https://picsum.photos/600/400?random=5',\n      author: {\n        name: '陈小红',\n        avatar: 'https://picsum.photos/32/32?random=5'\n      },\n      views: 987,\n      likes: 45,\n      isVideo: false\n    }, {\n      id: 6,\n      title: '旅行类App界面设计',\n      type: 'ui',\n      cover: 'https://picsum.photos/600/400?random=6',\n      author: {\n        name: '刘艺',\n        avatar: 'https://picsum.photos/32/32?random=6'\n      },\n      views: 765,\n      likes: 34,\n      isVideo: false\n    }, {\n      id: 7,\n      title: '音乐节创意海报',\n      type: 'graphic',\n      cover: 'https://picsum.photos/600/400?random=7',\n      author: {\n        name: '周设计',\n        avatar: 'https://picsum.photos/32/32?random=7'\n      },\n      views: 543,\n      likes: 28,\n      isVideo: false\n    }, {\n      id: 8,\n      title: '游戏角色原画设计',\n      type: 'illustration',\n      cover: 'https://picsum.photos/600/400?random=8',\n      author: {\n        name: '林画师',\n        avatar: 'https://picsum.photos/32/32?random=8'\n      },\n      views: 876,\n      likes: 56,\n      isVideo: false\n    }]);\n\n    // 根据分类和排序过滤作品\n    const filteredWorks = computed(() => {\n      let result = [...works.value];\n\n      // 根据选中的分类层级进行筛选\n      if (currentMainCategory.value !== 'all') {\n        result = result.filter(work => {\n          if (currentThirdCategory.value) {\n            return work.thirdCategory === currentThirdCategory.value;\n          }\n          if (currentSubCategory.value) {\n            return work.subCategory === currentSubCategory.value;\n          }\n          return work.type === currentMainCategory.value;\n        });\n      }\n\n      // 应用排序\n      switch (currentSort.value) {\n        case 'popular':\n          result.sort((a, b) => b.views + b.likes - (a.views + a.likes));\n          break;\n        case 'newest':\n          result.sort((a, b) => new Date(b.date) - new Date(a.date));\n          break;\n        case 'views':\n          result.sort((a, b) => b.views - a.views);\n          break;\n        case 'likes':\n          result.sort((a, b) => b.likes - a.likes);\n          break;\n      }\n      return result;\n    });\n\n    // 处理排序\n    const handleSort = value => {\n      currentSort.value = value;\n      showSortMenu.value = false;\n    };\n\n    // 当前选中的分类层级\n    const currentLevel = ref('main'); // 'main' | 'sub' | 'third'\n    const currentMainCategory = ref('all');\n    const currentSubCategory = ref('');\n    const currentThirdCategory = ref('');\n\n    // 获取当前分类的子分类\n    const currentSubCategories = computed(() => {\n      if (currentMainCategory.value === 'all') return [];\n      const mainCategory = categoryConfig.categories.find(c => c.id === currentMainCategory.value);\n      return mainCategory?.children || [];\n    });\n\n    // 处理分类切换\n    const handleCategoryChange = (categoryId, level = 'main') => {\n      if (level === 'main') {\n        currentMainCategory.value = categoryId;\n        currentSubCategory.value = '';\n        currentThirdCategory.value = '';\n        currentLevel.value = categoryId === 'all' ? 'main' : 'sub';\n      } else if (level === 'sub') {\n        currentSubCategory.value = categoryId;\n        currentThirdCategory.value = '';\n        currentLevel.value = 'third';\n      } else {\n        currentThirdCategory.value = categoryId;\n        showCategoryMenu.value = false;\n      }\n    };\n\n    // 获取当前显示的分类名称\n    const currentCategoryName = computed(() => {\n      if (currentMainCategory.value === 'all') return '全部';\n      const mainCategory = categoryConfig.categories.find(c => c.id === currentMainCategory.value);\n      if (!currentSubCategory.value) return mainCategory?.name;\n      const subCategory = mainCategory?.children?.find(c => c.id === currentSubCategory.value);\n      return subCategory?.name;\n    });\n\n    // 添加获取分类名称的函数\n    const getCategoryName = type => {\n      for (const key in portfolioCategories) {\n        const category = portfolioCategories[key].categories.find(c => c.id === type);\n        if (category) return category.name;\n      }\n      return type;\n    };\n    const __returned__ = {\n      store,\n      route,\n      categoryConfig,\n      menuGroups,\n      isAuthenticated,\n      currentCategory,\n      showCategoryMenu,\n      showSortMenu,\n      currentSort,\n      sortOptions,\n      works,\n      filteredWorks,\n      handleSort,\n      currentLevel,\n      currentMainCategory,\n      currentSubCategory,\n      currentThirdCategory,\n      currentSubCategories,\n      handleCategoryChange,\n      currentCategoryName,\n      getCategoryName,\n      ref,\n      computed,\n      watch,\n      get useStore() {\n        return useStore;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      MobileTabBar,\n      PageBanner,\n      PortfolioNavigation,\n      CategoryMenu,\n      get portfolioCategories() {\n        return portfolioCategories;\n      },\n      get generateMenuGroups() {\n        return generateMenuGroups;\n      },\n      get EyeIcon() {\n        return EyeIcon;\n      },\n      get HeartIcon() {\n        return HeartIcon;\n      },\n      get ChevronRightIcon() {\n        return ChevronRightIcon;\n      },\n      get PlayIcon() {\n        return PlayIcon;\n      },\n      get XMarkIcon() {\n        return XMarkIcon;\n      },\n      get ChevronLeftIcon() {\n        return ChevronLeftIcon;\n      },\n      get FireIcon() {\n        return FireIcon;\n      },\n      get CalendarIcon() {\n        return CalendarIcon;\n      },\n      get ShareIcon() {\n        return ShareIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","watch","useStore","useRoute","MobileTabBar","PageBanner","PortfolioNavigation","CategoryMenu","portfolioCategories","generateMenuGroups","EyeIcon","HeartIcon","ChevronRightIcon","PlayIcon","XMarkIcon","ChevronLeftIcon","FireIcon","CalendarIcon","ShareIcon","store","route","categoryConfig","discover","menuGroups","isAuthenticated","state","currentCategory","showCategoryMenu","showSortMenu","currentSort","sortOptions","popular","label","icon","newest","views","likes","works","id","title","type","cover","author","name","avatar","isVideo","duration","filteredWorks","result","value","currentMainCategory","filter","work","currentThirdCategory","thirdCategory","currentSubCategory","subCategory","sort","a","b","Date","date","handleSort","currentLevel","currentSubCategories","mainCategory","categories","find","c","children","handleCategoryChange","categoryId","level","currentCategoryName","getCategoryName","key","category"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/portfolio/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"py-4 lg:py-6 mt-[72px]\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅 -->\n      <PageBanner theme=\"blue\">\n        <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">{{ categoryConfig.title }}</h1>\n        <p class=\"text-gray-600 text-lg max-w-2xl\">{{ categoryConfig.description }}</p>\n      </PageBanner>\n\n      <!-- 移动端当前分类显示 -->\n      <div class=\"flex items-center justify-between py-3 sm:hidden\">\n        <div class=\"flex items-center space-x-2\">\n          <span class=\"text-sm text-gray-500\">当前：</span>\n          <span class=\"text-sm font-medium text-gray-900\">\n            {{ currentCategoryName }}\n          </span>\n        </div>\n        <button \n          class=\"text-sm text-gray-500 hover:text-gray-900 flex items-center\"\n          @click=\"showCategoryMenu = true\"\n        >\n          切换分类\n          <ChevronRightIcon class=\"w-4 h-4 ml-1\" />\n        </button>\n      </div>\n\n      <!-- 使用导航组件 -->\n      <PortfolioNavigation v-model:currentCategory=\"currentCategory\" class=\"hidden sm:block\" />\n\n      <!-- 作品列表 -->\n      <div class=\"max-w-7xl mx-auto mt-6\">\n        <div class=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 lg:gap-6\">\n          <div v-for=\"work in filteredWorks\" :key=\"work.id\"\n            class=\"bg-white rounded-lg lg:rounded-xl border border-gray-100 overflow-hidden hover:shadow-lg transition-all duration-300\"\n          >\n            <!-- 作品封面 -->\n            <div class=\"aspect-w-16 aspect-h-9 sm:aspect-w-1 sm:aspect-h-1 bg-gray-100 relative group overflow-hidden\">\n              <img \n                :src=\"work.cover\" \n                class=\"w-full h-full object-cover transform group-hover:scale-110 transition-all duration-500\"\n                alt=\"\"\n              >\n              <!-- 视频时长标记 -->\n              <div v-if=\"work.isVideo\" \n                class=\"absolute bottom-2 right-2 px-2 py-1 bg-black/70 rounded-md text-white text-xs flex items-center\"\n              >\n                <PlayIcon class=\"w-3 h-3 mr-1\" />\n                {{ work.duration }}\n              </div>\n              <!-- 悬浮遮罩 -->\n              <div class=\"absolute inset-0 bg-black/60 invisible group-hover:visible opacity-0 group-hover:opacity-100 transition-all duration-300 flex flex-col items-center justify-center transform group-hover:scale-110\">\n                <span class=\"text-white font-medium mb-2\">{{ getCategoryName(work.type) }}</span>\n                <div class=\"flex items-center gap-3\">\n                  <span class=\"flex items-center text-gray-300 text-sm\">\n                    <EyeIcon class=\"w-4 h-4 mr-1\" />{{ work.views }}\n                  </span>\n                  <span class=\"flex items-center text-gray-300 text-sm\">\n                    <HeartIcon class=\"w-4 h-4 mr-1\" />{{ work.likes }}\n                  </span>\n                </div>\n              </div>\n            </div>\n\n            <!-- 作品信息 -->\n            <div class=\"p-4 lg:p-4\">\n              <h3 class=\"text-sm font-medium text-gray-900 hover:text-blue-600 transition-colors line-clamp-1 mb-2\">\n                {{ work.title }}\n              </h3>\n              <div class=\"flex items-center justify-between\">\n                <div class=\"flex items-center space-x-2\">\n                  <img :src=\"work.author.avatar\" class=\"w-5 h-5 rounded-full\">\n                  <span class=\"text-sm text-gray-600\">{{ work.author.name }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 移动端分类切换菜单 -->\n    <div v-if=\"showCategoryMenu\" \n      class=\"fixed inset-0 bg-black/50 z-[100] sm:hidden flex items-end\"\n      @click=\"showCategoryMenu = false\"\n    >\n      <div \n        class=\"absolute bottom-0 left-0 right-0 bg-white rounded-t-2xl p-6 max-h-[85vh] overflow-y-auto\"\n        @click.stop\n      >\n        <div class=\"flex items-center justify-between mb-4\">\n          <div class=\"flex items-center space-x-2\">\n            <button \n              v-if=\"currentLevel !== 'main'\"\n              @click=\"currentLevel = currentLevel === 'third' ? 'sub' : 'main'\"\n              class=\"text-gray-400 hover:text-gray-500\"\n            >\n              <ChevronLeftIcon class=\"w-5 h-5\" />\n            </button>\n            <h3 class=\"text-lg font-medium text-gray-900\">\n              {{ \n                currentLevel === 'main' ? '选择分类' : \n                currentLevel === 'sub' ? portfolioCategories.find(c => c.id === currentMainCategory)?.name :\n                currentSubCategories.find(c => c.id === currentSubCategory)?.name\n              }}\n            </h3>\n          </div>\n          <button \n            @click=\"showCategoryMenu = false\"\n            class=\"text-gray-400 hover:text-gray-500\"\n          >\n            <XMarkIcon class=\"w-5 h-5\" />\n          </button>\n        </div>\n        <div class=\"grid grid-cols-2 gap-3 pb-6\">\n          <!-- 主分类 -->\n          <template v-if=\"currentLevel === 'main'\">\n            <button\n              v-for=\"category in categoryConfig.categories\"\n              :key=\"category.id\"\n              @click=\"handleCategoryChange(category.id, 'main')\"\n              class=\"flex items-center justify-center px-4 py-4 rounded-xl text-sm font-medium transition-colors\"\n              :class=\"[\n                currentMainCategory === category.id\n                  ? 'bg-blue-50 text-blue-600'\n                  : 'bg-gray-50 text-gray-600 hover:bg-gray-100'\n              ]\"\n            >\n              {{ category.name }}\n            </button>\n          </template>\n\n          <!-- 子分类 -->\n          <template v-else-if=\"currentLevel === 'sub'\">\n            <button\n              v-for=\"category in currentSubCategories\"\n              :key=\"category.id\"\n              @click=\"handleCategoryChange(category.id, 'sub')\"\n              class=\"flex items-center justify-center px-4 py-4 rounded-xl text-sm font-medium transition-colors\"\n              :class=\"[\n                currentSubCategory === category.id\n                  ? 'bg-blue-50 text-blue-600'\n                  : 'bg-gray-50 text-gray-600 hover:bg-gray-100'\n              ]\"\n            >\n              {{ category.name }}\n            </button>\n          </template>\n        </div>\n      </div>\n    </div>\n\n    <!-- 分类切换菜单 -->\n    <CategoryMenu v-if=\"showCategoryMenu\" \n      v-model:show=\"showCategoryMenu\"\n      v-model:currentLevel=\"currentLevel\"\n      v-model:currentMainCategory=\"currentMainCategory\"\n      v-model:currentSubCategory=\"currentSubCategory\"\n      v-model:currentThirdCategory=\"currentThirdCategory\"\n      type=\"discover\"\n      @category-change=\"handleCategoryChange\"\n    />\n\n    <!-- 使用移动端底部导航栏 -->\n    <MobileTabBar :menu-groups=\"menuGroups\" />\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, watch } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute } from 'vue-router'\nimport MobileTabBar from '@/components/MobileTabBar.vue'\nimport PageBanner from '@/components/common/PageBanner.vue'\nimport PortfolioNavigation from '@/components/portfolio/PortfolioNavigation.vue'\nimport CategoryMenu from '@/components/portfolio/CategoryMenu.vue'\nimport { portfolioCategories, generateMenuGroups } from '@/config/portfolioCategories'\nimport {\n  EyeIcon,\n  HeartIcon,\n  ChevronRightIcon,\n  PlayIcon,\n  XMarkIcon,\n  ChevronLeftIcon,\n  FireIcon,\n  CalendarIcon,\n  ShareIcon\n} from '@heroicons/vue/24/outline'\n\nconst store = useStore()\nconst route = useRoute()\n\n// 使用统一的分类配置\nconst categoryConfig = portfolioCategories.discover\nconst menuGroups = generateMenuGroups('discover')\n\n// 使用 Vuex store 的 isAuthenticated 状态\nconst isAuthenticated = computed(() => store.state.isAuthenticated)\n\n// 当前