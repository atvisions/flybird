nsole.log('redo');\n    };\n\n    // 缩放相关方法\n    const startDrag = e => {\n      isDragging = true;\n      startX = e.clientX;\n      startScale = scale.value;\n      document.addEventListener('mousemove', onDrag);\n      document.addEventListener('mouseup', stopDrag);\n    };\n    const onDrag = e => {\n      if (!isDragging) return;\n      const track = document.querySelector('.zoom-track');\n      const trackRect = track.getBoundingClientRect();\n      const trackWidth = trackRect.width;\n      const deltaX = e.clientX - startX;\n      const deltaScale = deltaX / trackWidth * 1.5;\n      let newScale = startScale + deltaScale;\n      newScale = Math.min(2, Math.max(0.5, newScale));\n      newScale = Math.round(newScale * 10) / 10;\n      scale.value = newScale;\n    };\n    const stopDrag = () => {\n      isDragging = false;\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    };\n    onUnmounted(() => {\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    });\n    const handleExit = () => {\n      router.push('/resumes');\n    };\n    const __returned__ = {\n      router,\n      scale,\n      get isDragging() {\n        return isDragging;\n      },\n      set isDragging(v) {\n        isDragging = v;\n      },\n      get startX() {\n        return startX;\n      },\n      set startX(v) {\n        startX = v;\n      },\n      get startScale() {\n        return startScale;\n      },\n      set startScale(v) {\n        startScale = v;\n      },\n      activeTab,\n      toolbarTabs,\n      fetchMainCategories,\n      canUndo,\n      canRedo,\n      handleUndo,\n      handleRedo,\n      startDrag,\n      onDrag,\n      stopDrag,\n      handleExit,\n      ref,\n      onMounted,\n      onUnmounted,\n      get useRouter() {\n        return useRouter;\n      },\n      get axios() {\n        return axios;\n      },\n      ToolbarPanel,\n  