{"ast":null,"code":"import { PencilSquareIcon, TrashIcon, PlusIcon, ExclamationTriangleIcon, DocumentTextIcon, TrophyIcon } from '@heroicons/vue/24/outline';\nimport { Dialog, DialogPanel, DialogTitle, TransitionRoot, TransitionChild } from '@headlessui/vue';\nimport { ref } from 'vue';\nimport { ElMessage } from 'element-plus';\nexport default {\n  __name: 'ProjectContent',\n  props: {\n    data: {\n      type: Array,\n      required: true\n    }\n  },\n  emits: ['edit', 'delete', 'add'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n\n    // 格式化日期显示\n    const formatDate = dateString => {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      return `${date.getFullYear()}.${String(date.getMonth() + 1).padStart(2, '0')}`;\n    };\n\n    // 删除相关\n    const showDeleteConfirm = ref(false);\n    const deleteId = ref(null);\n    const handleDelete = id => {\n      deleteId.value = id;\n      showDeleteConfirm.value = true;\n    };\n    const confirmDelete = async () => {\n      try {\n        await emit('delete', deleteId.value);\n        showDeleteConfirm.value = false;\n      } catch (error) {\n        console.error('删除失败:', error);\n        ElMessage.error('删除失败');\n      }\n    };\n    const __returned__ = {\n      props,\n      emit,\n      formatDate,\n      showDeleteConfirm,\n      deleteId,\n      handleDelete,\n      confirmDelete,\n      get PencilSquareIcon() {\n        return PencilSquareIcon;\n      },\n      get TrashIcon() {\n        return TrashIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      },\n      get ExclamationTriangleIcon() {\n        return ExclamationTriangleIcon;\n      },\n      get DocumentTextIcon() {\n        return DocumentTextIcon;\n      },\n      get TrophyIcon() {\n        return TrophyIcon;\n      },\n      get Dialog() {\n        return Dialog;\n      },\n      get DialogPanel() {\n        return DialogPanel;\n      },\n      get DialogTitle() {\n        return DialogTitle;\n      },\n      get TransitionRoot() {\n        return TransitionRoot;\n      },\n      get TransitionChild() {\n        return TransitionChild;\n      },\n      ref,\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["PencilSquareIcon","TrashIcon","PlusIcon","ExclamationTriangleIcon","DocumentTextIcon","TrophyIcon","Dialog","DialogPanel","DialogTitle","TransitionRoot","TransitionChild","ref","ElMessage","props","__props","emit","__emit","formatDate","dateString","date","Date","getFullYear","String","getMonth","padStart","showDeleteConfirm","deleteId","handleDelete","id","value","confirmDelete","error","console"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/user/MyProfile/components/ProjectContent.vue"],"sourcesContent":["<template>\n  <div class=\"relative space-y-0\">\n    <!-- 时间线 -->\n    <div class=\"absolute left-[0.5625rem] top-3 bottom-3 w-px bg-gray-200\"></div>\n\n    <!-- 项目经历列表 -->\n    <div \n      v-for=\"(project, index) in data\" \n      :key=\"index\" \n      class=\"relative pl-12 pb-6\"\n    >\n      <!-- 时间节点 -->\n      <div class=\"absolute left-[0.5625rem] -translate-x-1/2 top-2 w-3 h-3 rounded-full bg-white border-2 border-blue-500\"></div>\n      \n      <!-- 内容卡片 -->\n      <div class=\"bg-gray-50 rounded-lg border border-gray-100 p-4 hover:shadow-sm transition-shadow\">\n        <!-- 头部信息 -->\n        <div class=\"flex flex-col space-y-2\">\n          <!-- 项目名称和操作按钮 -->\n          <div class=\"flex items-start justify-between\">\n            <div class=\"flex-1 min-w-0\">\n              <h4 class=\"text-base font-medium text-gray-900 break-words\">{{ project.name }}</h4>\n              <div class=\"text-sm text-gray-500 mt-1\">\n                {{ formatDate(project.start_date) }} - {{ project.is_current ? '至今' : formatDate(project.end_date) }}\n              </div>\n            </div>\n            <div class=\"flex items-center space-x-2\">\n              <button\n                @click=\"$emit('edit', project)\"\n                class=\"p-1 hover:bg-white rounded-full transition-colors\"\n              >\n                <PencilSquareIcon class=\"w-4 h-4 text-gray-400\" />\n              </button>\n              <button\n                @click=\"handleDelete(project.id)\"\n                class=\"p-1 hover:bg-white rounded-full transition-colors\"\n              >\n                <TrashIcon class=\"w-4 h-4 text-gray-400\" />\n              </button>\n            </div>\n          </div>\n\n          <!-- 角色 -->\n          <div class=\"text-sm font-medium text-blue-600 break-words\">\n            {{ project.role }}\n          </div>\n\n          <!-- 项目描述 -->\n          <div class=\"mt-3 space-y-2\">\n            <div class=\"flex items-center space-x-2\">\n              <DocumentTextIcon class=\"w-4 h-4 text-gray-400 flex-shrink-0\" />\n              <span class=\"text-sm text-gray-500\">项目描述</span>\n            </div>\n            <div class=\"text-sm leading-relaxed text-gray-600 pl-6 break-words whitespace-pre-wrap\">\n              {{ project.description }}\n            </div>\n          </div>\n\n          <!-- 项目成就 -->\n          <div v-if=\"project.achievement\" class=\"mt-3 space-y-2\">\n            <div class=\"flex items-center space-x-2\">\n              <TrophyIcon class=\"w-4 h-4 text-yellow-500 flex-shrink-0\" />\n              <span class=\"text-sm text-gray-500\">项目成就</span>\n            </div>\n            <div class=\"text-sm leading-relaxed text-gray-600 pl-6 break-words whitespace-pre-wrap\">\n              {{ project.achievement }}\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 添加按钮 -->\n    <div class=\"relative pl-12\">\n      <div class=\"absolute left-[0.5625rem] -translate-x-1/2 top-2 w-3 h-3 rounded-full bg-white border-2 border-gray-300\"></div>\n      <button\n        @click=\"$emit('add')\"\n        class=\"w-full flex items-center justify-center px-4 py-2 border border-gray-200 rounded-lg text-sm text-gray-600 hover:bg-gray-50 transition-all\"\n      >\n        <PlusIcon class=\"w-4 h-4 mr-2\" />\n        添加项目经历\n      </button>\n    </div>\n\n    <!-- 删除确认弹窗 -->\n    <TransitionRoot appear :show=\"showDeleteConfirm\" as=\"template\">\n      <Dialog as=\"div\" class=\"relative z-50\" @close=\"showDeleteConfirm = false\">\n        <TransitionChild\n          as=\"template\"\n          enter=\"duration-300 ease-out\"\n          enter-from=\"opacity-0\"\n          enter-to=\"opacity-100\"\n          leave=\"duration-200 ease-in\"\n          leave-from=\"opacity-100\"\n          leave-to=\"opacity-0\"\n        >\n          <div class=\"fixed inset-0 bg-black/25\" />\n        </TransitionChild>\n        \n        <div class=\"fixed inset-0 overflow-y-auto\">\n          <div class=\"flex min-h-full items-center justify-center p-4 text-center\">\n            <DialogPanel class=\"w-full max-w-md transform overflow-hidden rounded-xl bg-white p-6 text-left align-middle shadow-xl transition-all\">\n              <div>\n                <div class=\"flex items-start space-x-3\">\n                  <div class=\"p-2 bg-red-50 rounded-full flex-shrink-0\">\n                    <ExclamationTriangleIcon class=\"w-6 h-6 text-red-600\" />\n                  </div>\n                  <div class=\"flex-1\">\n                    <DialogTitle as=\"h3\" class=\"text-lg font-medium text-gray-900\">\n                      确认删除项目经历？\n                    </DialogTitle>\n                    <p class=\"mt-2 text-sm text-gray-500\">\n                      删除后将无法恢复，请确认是否继续。\n                    </p>\n                  </div>\n                </div>\n                \n                <div class=\"mt-6 flex justify-end space-x-3\">\n                  <button\n                    type=\"button\"\n                    @click=\"showDeleteConfirm = false\"\n                    class=\"px-4 py-2 text-sm font-medium text-gray-700 bg-white border border-gray-300 rounded-lg hover:bg-gray-50\"\n                  >\n                    取消\n                  </button>\n                  <button\n                    type=\"button\"\n                    @click=\"confirmDelete\"\n                    class=\"px-4 py-2 text-sm font-medium text-white bg-red-600 border border-transparent rounded-lg hover:bg-red-700\"\n                  >\n                    确定删除\n                  </button>\n                </div>\n              </div>\n            </DialogPanel>\n          </div>\n        </div>\n      </Dialog>\n    </TransitionRoot>\n  </div>\n</template>\n\n<script setup>\nimport { \n  PencilSquareIcon, \n  TrashIcon, \n  PlusIcon,\n  ExclamationTriangleIcon,\n  DocumentTextIcon,\n  TrophyIcon\n} from '@heroicons/vue/24/outline'\nimport { \n  Dialog,\n  DialogPanel,\n  DialogTitle,\n  TransitionRoot,\n  TransitionChild\n} from '@headlessui/vue'\nimport { ref } from 'vue'\nimport { ElMessage } from 'element-plus'\n\nconst props = defineProps({\n  data: {\n    type: Array,\n    required: true\n  }\n})\n\nconst emit = defineEmits(['edit', 'delete', 'add'])\n\n// 格式化日期显示\nconst formatDate = (dateString) => {\n  if (!dateString) return ''\n  const date = new Date(dateString)\n  return `${date.getFullYear()}.${String(date.getMonth() + 1).padStart(2, '0')}`\n}\n\n// 删除相关\nconst showDeleteConfirm = ref(false)\nconst deleteId = ref(null)\n\nconst handleDelete = (id) => {\n  deleteId.value = id\n  showDeleteConfirm.value = true\n}\n\nconst confirmDelete = async () => {\n  try {\n    await emit('delete', deleteId.value)\n    showDeleteConfirm.value = false\n  } catch (error) {\n    console.error('删除失败:', error)\n    ElMessage.error('删除失败')\n  }\n}\n</script> "],"mappings":"AA+IA,SACEA,gBAAg