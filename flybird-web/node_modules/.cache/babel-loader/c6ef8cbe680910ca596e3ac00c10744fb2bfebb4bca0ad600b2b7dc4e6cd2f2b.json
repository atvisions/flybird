ion: '记录在日本京都的传统文化探索之旅，展现当地独特的人文风景...',\n      cover: 'https://picsum.photos/600/400?random=11',\n      author: {\n        name: '李小红',\n        avatar: 'https://picsum.photos/32/32?random=11',\n        title: 'Vlogger'\n      },\n      views: 1567,\n      likes: 98\n    }]);\n\n    // 根据分类过滤作品\n    const filteredWorks = computed(() => {\n      if (currentCategory.value === 'all') {\n        return works.value;\n      }\n      return works.value.filter(work => work.type === currentCategory.value);\n    });\n    const __returned__ = {\n      store,\n      route,\n      isAuthenticated,\n      mainNavs,\n      currentCategory,\n      videoCategories,\n      works,\n      filteredWorks,\n      ref,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get EyeIcon() {\n        return EyeIcon;\n      },\n      get HeartIcon() {\n        return HeartIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      },\n      get UserIcon() {\n        return UserIcon;\n      },\n      get PlayIcon() {\n        return PlayIcon;\n      },\n      PageBanner,\n      PortfolioNavigation\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","useRoute","EyeIcon","HeartIcon","PlusIcon","UserIcon","PlayIcon","PageBanner","PortfolioNavigation","store","route","isAuthenticated","state","mainNavs","name","path","currentCategory","videoCategories","id","works","title","type","description","cover","author","avatar","views","likes","filteredWorks","value","filter","work"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/portfolio/VideoView.vue"],"sourcesContent":["<template>\n  <div class=\"py-4 lg:py-6 mt-[72px]\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅 -->\n      <PageBanner theme=\"blue\">\n        <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">视频作品</h1>\n        <p class=\"text-gray-600 text-lg max-w-2xl\">分享你的视频创作，展现影像魅力</p>\n      </PageBanner>\n\n      <!-- 使用导航组件 -->\n      <PortfolioNavigation v-model:currentCategory=\"currentCategory\" />\n\n      <!-- 移动端分类显示 -->\n      <div class=\"md:hidden mb-6\">\n        <div class=\"flex items-center justify-between\">\n          <h1 class=\"text-xl font-bold text-gray-900\">视频作品</h1>\n          <span class=\"text-sm text-gray-500\">{{ works.length }}个作品</span>\n        </div>\n      </div>\n\n      <!-- 主导航区域 -->\n      <div class=\"bg-white rounded-xl border border-gray-100 p-4 hidden md:block\">\n        <div class=\"flex flex-col sm:flex-row sm:items-center justify-between gap-4\">\n          <!-- 左侧主导航 -->\n          <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n            <router-link\n              v-for=\"nav in mainNavs\"\n              :key=\"nav.path\"\n              :to=\"nav.path\"\n              class=\"px-4 sm:px-8 py-3 mx-1 text-sm font-medium transition-colors relative group whitespace-nowrap\"\n              :class=\"[\n                $route.path === nav.path\n                  ? 'text-gray-900'\n                  : 'text-gray-500 hover:text-gray-700'\n              ]\"\n            >\n              {{ nav.name }}\n              <!-- 活跃状态指示器 -->\n              <div \n                class=\"absolute left-1/2 -translate-x-1/2 bottom-4.5 w-1.5 h-1.5 rounded-full transition-colors\"\n                :class=\"$route.path === nav.path ? 'bg-gray-900' : 'bg-transparent group-hover:bg-gray-200'\"\n              ></div>\n            </router-link>\n          </div>\n\n          <!-- 右侧操作按钮 -->\n          <div class=\"flex items-center gap-2 sm:gap-3\">\n            <template v-if=\"isAuthenticated\">\n              <!-- 发布按钮 -->\n              <button class=\"h-9 px-4 sm:px-5 bg-gradient-to-r from-blue-600 to-indigo-600 text-whit