{"ast":null,"code":"import { computed } from 'vue';\nimport { XMarkIcon, CheckCircleIcon } from '@heroicons/vue/24/outline';\nimport { useRouter } from 'vue-router';\nimport vipBg from '@/assets/images/vip.png';\nexport default {\n  __name: 'ProDialog',\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n    const router = useRouter();\n    const dialogVisible = computed({\n      get: () => props.modelValue,\n      set: value => emit('update:modelValue', value)\n    });\n    const benefits = ['无限次导入简历', 'AI智能优化简历', '专业简历模板', '一键导出多种格式', '简历在线托管'];\n    const closeDialog = () => {\n      dialogVisible.value = false;\n    };\n    const handleUpgrade = () => {\n      closeDialog();\n      router.push('/pro');\n    };\n    const __returned__ = {\n      props,\n      emit,\n      router,\n      dialogVisible,\n      benefits,\n      closeDialog,\n      handleUpgrade,\n      computed,\n      get XMarkIcon() {\n        return XMarkIcon;\n      },\n      get CheckCircleIcon() {\n        return CheckCircleIcon;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      get vipBg() {\n        return vipBg;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","XMarkIcon","CheckCircleIcon","useRouter","vipBg","props","__props","emit","__emit","router","dialogVisible","get","modelValue","set","value","benefits","closeDialog","handleUpgrade","push"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/components/dialogs/ProDialog.vue"],"sourcesContent":["<template>\n  <Teleport to=\"body\">\n    <div v-if=\"dialogVisible\" class=\"fixed inset-0 z-50 flex items-center justify-center\">\n      <!-- 遮罩层 -->\n      <div class=\"absolute inset-0 bg-black/60\" @click=\"closeDialog\"></div>\n      \n      <!-- 弹窗内容 -->\n      <div class=\"relative w-[370px] h-[392px] rounded-2xl overflow-hidden\">\n        <!-- 背景图片 -->\n        <div class=\"absolute inset-0 bg-cover bg-center bg-no-repeat\" :style=\"{ backgroundImage: `url(${vipBg})` }\"></div>\n        \n        <!-- 内容区域 -->\n        <div class=\"relative h-full\">\n          <!-- 关闭按钮 -->\n          <button \n            @click=\"closeDialog\"\n            class=\"absolute top-4 right-4 p-1 rounded-full hover:bg-black/10 transition-colors\"\n          >\n            <XMarkIcon class=\"w-6 h-6 text-white\" />\n          </button>\n\n          <!-- 价格信息 -->\n          <div class=\"text-left pl-[30px] pt-[30px]\">\n            <h2 class=\"text-3xl font-bold text-black mb-1\">超值会员尊享价</h2>\n            <p class=\"text-sm text-black\">现在开通低至 0.8 元/天</p>\n          </div>\n\n          <!-- 标题 -->\n          <div class=\"text-center mt-12 mb-8\">\n            <h3 class=\"text-2xl font-bold text-white mb-2\">开通会员</h3>\n            <p class=\"text-white/80\">解锁所有高级功能</p>\n          </div>\n\n          <!-- 会员权益列表 -->\n          <div class=\"space-y-4 mb-8 px-8\">\n            <div v-for=\"(benefit, index) in benefits\" :key=\"index\" \n              class=\"flex items-center space-x-3 text-white\"\n            >\n              <CheckCircleIcon class=\"w-5 h-5 text-yellow-400 flex-shrink-0\" />\n              <span>{{ benefit }}</span>\n            </div>\n          </div>\n\n          <!-- 按钮 -->\n          <div class=\"space-y-4 px-8\">\n            <button \n              @click=\"handleUpgrade\"\n              class=\"w-full py-3 bg-gradient-to-r from-yellow-400 to-yellow-500 text-black font-medium rounded-lg hover:from-yellow-500 hover:to-yellow-600 transition-all duration-300 transform hover:scale-105\"\n            >\n              立即开通\n            </button>\n            <button \n              @click=\"closeDialog\"\n              class=\"w-full py-3 bg-white/10 text-white font-medium rounded-lg hover:bg-white/20 transition-colors\"\n            >\n              暂不开通\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </Teleport>\n</template>\n\n<script setup>\nimport { computed } from 'vue'\nimport { XMarkIcon, CheckCircleIcon } from '@heroicons/vue/24/outline'\nimport { useRouter } from 'vue-router'\nimport vipBg from '@/assets/images/vip.png'\n\nconst props = defineProps({\n  modelValue: {\n    type: Boolean,\n    default: false\n  }\n})\n\nconst emit = defineEmits(['update:modelValue'])\nconst router = useRouter()\n\nconst dialogVisible = computed({\n  get: () => props.modelValue,\n  set: (value) => emit('update:modelValue', value)\n})\n\nconst benefits = [\n  '无限次导入简历',\n  'AI智能优化简历',\n  '专业简历模板',\n  '一键导出多种格式',\n  '简历在线托管'\n]\n\nconst closeDialog = () => {\n  dialogVisible.value = false\n}\n\nconst handleUpgrade = () => {\n  closeDialog()\n  router.push('/pro')\n}\n</script>\n\n<style scoped>\n.dialog-enter-active,\n.dialog-leave-active {\n  transition: opacity 0.3s ease;\n}\n\n.dialog-enter-from,\n.dialog-leave-to {\n  opacity: 0;\n}\n</style> "],"mappings":"AAiEA,SAASA,QAAQ,QAAQ,KAAI;AAC7B,SAASC,SAAS,EAAEC,eAAe,QAAQ,2BAA0B;AACrE,SAASC,SAAS,QAAQ,YAAW;AACrC,OAAOC,KAAK,MAAM,yBAAwB;;;;;;;;;;;;;;;IAE1C,MAAMC,KAAK,GAAGC,OAAA;IAOd,MAAMC,IAAI,GAAGC,MAAA;IACb,MAAMC,MAAM,GAAGN,SAAS,CAAC;IAEzB,MAAMO,aAAa,GAAGV,QAAQ,CAAC;MAC7BW,GAAG,EAAEA,CAAA,KAAMN,KAAK,CAACO,UAAU;MAC3BC,GAAG,EAAGC,KAAK,IAAKP,IAAI,CAAC,mBAAmB,EAAEO,KAAK;IACjD,CAAC;IAED,MAAMC,QAAQ,GAAG,CACf,SAAS,EACT,UAAU,EACV,QAAQ,EACR,UAAU,EACV,QAAO,CACT;IAEA,MAAMC,WAAW,GAAGA,CAAA,KAAM;MACxBN,aAAa,CAACI,KAAK,GAAG,KAAI;IAC5B;IAEA,MAAMG,aAAa,GAAGA,CAAA,KAAM;MAC1BD,WAAW,CAAC;MACZP,MAAM,CAACS,IAAI,CAAC,MAAM;IACpB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}