{"ast":null,"code":"import { ref } from 'vue';\n\n// 主导航数据\n\nexport default {\n  __name: 'CampusView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const mainNavs = [{\n      name: '全部模板',\n      path: '/templates'\n    }, {\n      name: '校园简历',\n      path: '/templates/campus'\n    }, {\n      name: '社会简历',\n      path: '/templates/professional'\n    }, {\n      name: '英文简历',\n      path: '/templates/english'\n    }];\n\n    // 当前选中的分类\n    const currentCategory = ref('all');\n\n    // 模板分类\n    const templateCategories = [{\n      id: 'all',\n      name: '全部'\n    }, {\n      id: 'internship',\n      name: '实习生'\n    }, {\n      id: 'graduate',\n      name: '应届生'\n    }, {\n      id: 'research',\n      name: '研究生'\n    }, {\n      id: 'academic',\n      name: '学术简历'\n    }, {\n      id: 'competition',\n      name: '竞赛简历'\n    }];\n    const __returned__ = {\n      mainNavs,\n      currentCategory,\n      templateCategories,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","mainNavs","name","path","currentCategory","templateCategories","id"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/templates/CampusView.vue"],"sourcesContent":["<template>\n  <div class=\"py-16 mt-8\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅区域 -->\n      <div class=\"relative overflow-hidden rounded-2xl bg-gradient-to-r from-rose-50/90 via-pink-50/80 to-orange-50/70 p-8 sm:p-12 min-h-[320px]\">\n        <div class=\"relative z-10\">\n          <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">校园简历模板</h1>\n          <p class=\"text-gray-600 text-lg max-w-2xl\">为在校生和应届生量身定制的简历模板</p>\n        </div>\n      </div>\n\n      <!-- 主导航区域 -->\n      <div class=\"mt-8 bg-white rounded-xl border border-gray-100 p-4 hidden md:block\">\n        <div class=\"flex flex-col gap-4\">\n          <!-- 左侧主导航和右侧按钮 -->\n          <div class=\"flex flex-row sm:items-center justify-between\">\n            <!-- 主导航链接 -->\n            <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n              <router-link\n                v-for=\"nav in mainNavs\"\n                :key=\"nav.path\"\n                :to=\"nav.path\"\n                class=\"px-4 sm:px-8 py-3 mx-1 text-sm font-medium transition-colors relative group whitespace-nowrap\"\n                :class=\"[\n                  $route.path === nav.path\n                    ? 'text-gray-900'\n                    : 'text-gray-500 hover:text-gray-700'\n                ]\"\n              >\n                {{ nav.name }}\n                <!-- 活跃状态指示器 -->\n                <div \n                  class=\"absolute left-1/2 -translate-x-1/2 bottom-4.5 w-1.5 h-1.5 rounded-full transition-colors\"\n                  :class=\"$route.path === nav.path ? 'bg-gray-900' : 'bg-transparent group-hover:bg-gray-200'\"\n                ></div>\n              </router-link>\n            </div>\n          </div>\n\n          <!-- 分隔线 -->\n          <div class=\"h-px bg-gray-200\"></div>\n          \n          <!-- 二级分类标签 -->\n          <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n            <button\n              v-for=\"category in templateCategories\"\n              :key=\"category.id\"\n              @click=\"currentCategory = category.id\"\n              class=\"px-4 py-2 mx-1 rounded-full text-sm font-medium transition-colors whitespace-nowrap\"\n              :class=\"[\n                currentCategory === category.id\n                  ? 'bg-gray-900 text-white'\n                  : 'bg-gray-100 text-gray-600 hover:bg-gray-200'\n              ]\"\n            >\n              {{ category.name }}\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <!-- 模板列表 -->\n      <div class=\"mt-6\">\n        <!-- 模板列表内容 -->\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from 'vue'\n\n// 主导航数据\nconst mainNavs = [\n  { name: '全部模板', path: '/templates' },\n  { name: '校园简历', path: '/templates/campus' },\n  { name: '社会简历', path: '/templates/professional' },\n  { name: '英文简历', path: '/templates/english' }\n]\n\n// 当前选中的分类\nconst currentCategory = ref('all')\n\n// 模板分类\nconst templateCategories = [\n  { id: 'all', name: '全部' },\n  { id: 'internship', name: '实习生' },\n  { id: 'graduate', name: '应届生' },\n  { id: 'research', name: '研究生' },\n  { id: 'academic', name: '学术简历' },\n  { id: 'competition', name: '竞赛简历' }\n]\n</script> "],"mappings":"AAsEA,SAASA,GAAG,QAAQ,KAAI;;AAExB;;;;;;;;IACA,MAAMC,QAAQ,GAAG,CACf;MAAEC,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAa,CAAC,EACpC;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAoB,CAAC,EAC3C;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAA0B,CAAC,EACjD;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAqB,EAC7C;;IAEA;IACA,MAAMC,eAAe,GAAGJ,GAAG,CAAC,KAAK;;IAEjC;IACA,MAAMK,kBAAkB,GAAG,CACzB;MAAEC,EAAE,EAAE,KAAK;MAAEJ,IAAI,EAAE;IAAK,CAAC,EACzB;MAAEI,EAAE,EAAE,YAAY;MAAEJ,IAAI,EAAE;IAAM,CAAC,EACjC;MAAEI,EAAE,EAAE,UAAU;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAC/B;MAAEI,EAAE,EAAE,UAAU;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAC/B;MAAEI,EAAE,EAAE,UAAU;MAAEJ,IAAI,EAAE;IAAO,CAAC,EAChC;MAAEI,EAAE,EAAE,aAAa;MAAEJ,IAAI,EAAE;IAAO,EACpC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}