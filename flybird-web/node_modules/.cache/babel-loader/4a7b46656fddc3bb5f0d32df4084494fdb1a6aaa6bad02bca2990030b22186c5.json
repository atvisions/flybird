{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport { ref, onMounted, onUnmounted, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport ToolbarPanel from './components/ToolbarPanel.vue';\nimport Canvas from './components/Canvas.vue';\nimport { VExpansionPanels, VExpansionPanel, VExpansionPanelTitle, VExpansionPanelText } from 'vuetify/components';\nimport PropertyPanel from './components/PropertyPanel.vue';\nimport { useProfileStore } from '@/stores/profile';\nexport default {\n  __name: 'index',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const scale = ref(1);\n    let isDragging = false;\n    let startX = 0;\n    let startScale = 0;\n    const activeTab = ref('layout');\n\n    // 工具栏 tabs 数据\n    const toolbarTabs = ref([{\n      value: 'layout',\n      text: '布局',\n      icon: 'mdi-view-grid'\n    }, {\n      value: 'components',\n      text: '组件',\n      icon: 'mdi-puzzle'\n    }, {\n      value: 'files',\n      text: '档案',\n      icon: 'mdi-file'\n    }]);\n\n    // 获取主分类\n    const fetchMainCategories = async () => {\n      try {\n        const response = await axios.get('/api/v1/resume/component-categories/main_categories/');\n        const mainCategories = response.data.map(category => ({\n          value: category.code,\n          text: category.name,\n          icon: category.icon || 'mdi-view-grid'\n        }));\n        if (mainCategories.length > 0) {\n          toolbarTabs.value = mainCategories;\n          // 如果当前选中的 tab 不在新的分类列表中，则选择第一个分类\n          if (!mainCategories.find(tab => tab.value === activeTab.value)) {\n            activeTab.value = mainCategories[0].value;\n          }\n        }\n      } catch (error) {\n        console.error('获取主分类失败:', error);\n      }\n    };\n\n    // 获取档案数据\n    const profileStore = useProfileStore();\n    const profileData = ref(null);\n\n    // 获取用户档案数据\n    const fetchProfileData = async () => {\n      try {\n        const response = await profileStore.fetchProfile();\n        profileData.value = response.data;\n      } catch (error) {\n        console.error('获取档案数据失败:', error);\n      }\n    };\n    onMounted(() => {\n      fetchMainCategories();\n      fetchProfileData();\n    });\n\n    // 历史记录状态\n    const canUndo = ref(true);\n    const canRedo = ref(true);\n\n    // 历史记录相关方法\n    const handleUndo = () => {\n      console.log('undo');\n    };\n    const handleRedo = () => {\n      console.log('redo');\n    };\n\n    // 缩放相关方法\n    const startDrag = e => {\n      isDragging = true;\n      startX = e.clientX;\n      startScale = scale.value;\n      document.addEventListener('mousemove', onDrag);\n      document.addEventListener('mouseup', stopDrag);\n    };\n    const onDrag = e => {\n      if (!isDragging) return;\n      const track = document.querySelector('.zoom-track');\n      const trackRect = track.getBoundingClientRect();\n      const trackWidth = trackRect.width;\n      const deltaX = e.clientX - startX;\n      const deltaScale = deltaX / trackWidth * 1.5;\n      let newScale = startScale + deltaScale;\n      newScale = Math.min(2, Math.max(0.5, newScale));\n      newScale = Math.round(newScale * 10) / 10;\n      scale.value = newScale;\n    };\n    const stopDrag = () => {\n      isDragging = false;\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    };\n    onUnmounted(() => {\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    });\n    const handleExit = () => {\n      router.push('/resumes');\n    };\n\n    // 当前模板数据\n    const currentTemplate = ref(null);\n\n    // 处理模板变更\n    const handleTemplateChange = async template => {\n      console.log('选择的模板:', template);\n\n      // 如果还没有档案数据，先获取\n      if (!profileData.value) {\n        await fetchProfileData();\n      }\n\n      // 将模板数据和档案数据传递给 Canvas 组件\n      if (canvasRef.value) {\n        currentTemplate.value = template;\n        canvasRef.value.loadTemplate(template, profileData.value);\n      }\n    };\n\n    // 处理属性面板的更新\n    const handlePropertyUpdate = updatedTemplate => {\n      console.log('属性更新:', updatedTemplate);\n      if (canvasRef.value) {\n        canvasRef.value.loadTemplate(updatedTemplate);\n      }\n    };\n\n    // 监听模板属性变化\n    watch(currentTemplate, newTemplate => {\n      if (newTemplate && canvasRef.value) {\n        canvasRef.value.loadTemplate(newTemplate);\n      }\n    }, {\n      deep: true\n    });\n\n    // 获取 Canvas 组件的引用\n    const canvasRef = ref(null);\n\n    // 更新 Canvas 组件的引用\n    const updateCanvasRef = el => {\n      canvasRef.value = el;\n    };\n    const __returned__ = {\n      router,\n      scale,\n      get isDragging() {\n        return isDragging;\n      },\n      set isDragging(v) {\n        isDragging = v;\n      },\n      get startX() {\n        return startX;\n      },\n      set startX(v) {\n        startX = v;\n      },\n      get startScale() {\n        return startScale;\n      },\n      set startScale(v) {\n        startScale = v;\n      },\n      activeTab,\n      toolbarTabs,\n      fetchMainCategories,\n      profileStore,\n      profileData,\n      fetchProfileData,\n      canUndo,\n      canRedo,\n      handleUndo,\n      handleRedo,\n      startDrag,\n      onDrag,\n      stopDrag,\n      handleExit,\n      currentTemplate,\n      handleTemplateChange,\n      handlePropertyUpdate,\n      canvasRef,\n      updateCanvasRef,\n      ref,\n      onMounted,\n      onUnmounted,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get axios() {\n        return axios;\n      },\n      ToolbarPanel,\n      Canvas,\n      get VExpansionPanels() {\n        return VExpansionPanels;\n      },\n      get VExpansionPanel() {\n        return VExpansionPanel;\n      },\n      get VExpansionPanelTitle() {\n        return VExpansionPanelTitle;\n      },\n      get VExpansionPanelText() {\n        return VExpansionPanelText;\n      },\n      PropertyPanel,\n      get useProfileStore() {\n        return useProfileStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","watch","useRouter","axios","ToolbarPanel","Canvas","VExpansionPanels","VExpansionPanel","VExpansionPanelTitle","VExpansionPanelText","PropertyPanel","useProfileStore","router","scale","isDragging","startX","startScale","activeTab","toolbarTabs","value","text","icon","fetchMainCategories","response","get","mainCategories","data","map","category","code","name","length","find","tab","error","console","profileStore","profileData","fetchProfileData","fetchProfile","canUndo","canRedo","handleUndo","log","handleRedo","startDrag","e","clientX","document","addEventListener","onDrag","stopDrag","track","querySelector","trackRect","getBoundingClientRect","trackWidth","width","deltaX","deltaScale","newScale","Math","min","max","round","removeEventListener","handleExit","push","currentTemplate","handleTemplateChange","template","canvasRef","loadTemplate","handlePropertyUpdate","updatedTemplate","newTemplate","deep","updateCanvasRef","el"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/resume-editor/index.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"resume-editor\">\n      <!-- 顶部栏 -->\n      <v-app-bar class=\"editor-header\" elevation=\"0\" height=\"60\">\n        <!-- 左侧：退出箭头和简历名称 -->\n        <div class=\"d-flex align-center justify-space-between w-100\">\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"返回\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon @click=\"handleExit\" v-bind=\"props\">\n                  <v-icon>mdi-arrow-left</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-divider vertical class=\"mx-3\"></v-divider>\n            <span class=\"text-h6\">我的简历</span>\n          </div>\n\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"撤销\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" :disabled=\"!canUndo\" v-bind=\"props\" @click=\"handleUndo\">\n                  <v-icon>mdi-undo</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"重做\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon :disabled=\"!canRedo\" v-bind=\"props\" @click=\"handleRedo\">\n                  <v-icon>mdi-redo</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n          </div>\n\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"草稿箱\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\">\n                  <v-icon>mdi-file-document-outline</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"打印预览\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\">\n                  <v-icon>mdi-printer</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"保存\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon v-bind=\"props\">\n                  <v-icon>mdi-content-save</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n          </div>\n        </div>\n      </v-app-bar>\n\n      <!-- 主要内容区域 -->\n      <div class=\"editor-main\">\n        <!-- 左侧工具栏 -->\n        <div class=\"editor-toolbar\">\n          <div class=\"d-flex\">\n            <ToolbarPanel \n              v-model=\"activeTab\" \n              @template-change=\"handleTemplateChange\"\n            />\n\n            <!-- 竖向标签页 -->\n            <div class=\"vertical-tabs\">\n              <div class=\"tab-items\">\n                <div class=\"tab-item\" \n                  v-for=\"tab in toolbarTabs\"\n                  :key=\"tab.value\"\n                  :class=\"{ active: activeTab === tab.value }\"\n                  @click=\"activeTab = tab.value\">\n                  <span class=\"tab-text\">{{ tab.text }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 中间画布区域 -->\n        <div class=\"editor-canvas\">\n          <Canvas ref=\"canvasRef\" :scale=\"scale\" />\n        </div>\n\n        <!-- 右侧操作栏 -->\n        <div class=\"editor-properties\">\n          <PropertyPanel \n            v-model=\"currentTemplate\"\n            @update:model-value=\"handlePropertyUpdate\"\n          />\n        </div>\n      </div>\n\n      <!-- 底部通栏 -->\n      <div class=\"editor-footer\">\n        <div class=\"d-flex align-center justify-center h-100\">\n          <div class=\"zoom-control\">\n            <span class=\"zoom-label zoom-label-left\">50%</span>\n            <div class=\"zoom-track\">\n              <div class=\"zoom-slider\" \n                :style=\"{ left: ((scale - 0.5) / 1.5 * 100) + '%' }\"\n                @mousedown=\"startDrag\">\n                <div class=\"zoom-value\">{{ Math.round(scale * 100) }}%</div>\n              </div>\n              <div class=\"zoom-line\"></div>\n            </div>\n            <span class=\"zoom-label zoom-label-right\">200%</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </v-app>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport axios from 'axios'\nimport ToolbarPanel from './components/ToolbarPanel.vue'\nimport Canvas from './components/Canvas.vue'\nimport { VExpansionPanels, VExpansionPanel, VExpansionPanelTitle, VExpansionPanelText } from 'vuetify/components'\nimport PropertyPanel from './components/PropertyPanel.vue'\nimport { useProfileStore } from '@/stores/profile'\n\nconst router = useRouter()\nconst scale = ref(1)\nlet isDragging = false\nlet startX = 0\nlet startScale = 0\nconst activeTab = ref('layout')\n\n// 工具栏 tabs 数据\nconst toolbarTabs = ref([\n  { value: 'layout', text: '布局', icon: 'mdi-view-grid' },\n  { value: 'components', text: '组件', icon: 'mdi-puzzle' },\n  { value: 'files', text: '档案', icon: 'mdi-file' }\n])\n\n// 获取主分类\nconst fetchMainCategories = async () => {\n  try {\n    const response = await axios.get('/api/v1/resume/component-categories/main_categories/')\n    const mainCategories = response.data.map(category => ({\n      value: category.code,\n      text: category.name,\n      icon: category.icon || 'mdi-view-grid'\n    }))\n    \n    if (mainCategories.length > 0) {\n      toolbarTabs.value = mainCategories\n      // 如果当前选中的 tab 不在新的分类列表中，则选择第一个分类\n      if (!mainCategories.find(tab => tab.value === activeTab.value)) {\n        activeTab.value = mainCategories[0].value\n      }\n    }\n  } catch (error) {\n    console.error('获取主分类失败:', error)\n  }\n}\n\n// 获取档案数据\nconst profileStore = useProfileStore()\nconst profileData = ref(null)\n\n// 获取用户档案数据\nconst fetchProfileData = async () => {\n  try {\n    const response = await profileStore.fetchProfile()\n    profileData.value = response.data\n  } catch (error) {\n    console.error('获取档案数据失败:', error)\n  }\n}\n\nonMounted(() => {\n  fetchMainCategories()\n  fetchProfileData()\n})\n\n// 历史记录状态\nconst canUndo = ref(true)\nconst canRedo = ref(true)\n\n// 历史记录相关方法\nconst handleUndo = () => {\n  console.log('undo')\n}\n\nconst handleRedo = () => {\n  console.log('redo')\n}\n\n// 缩放相关方法\nconst startDrag = (e) => {\n  isDragging = true\n  startX = e.clientX\n  startScale = scale.value\n  document.addEventListener('mousemove', onDrag)\n  document.addEventListener('mouseup', stopDrag)\n}\n\nconst onDrag = (e) => {\n  if (!isDragging) return\n  \n  const track = document.querySelector('.zoom-track')\n  const trackRect = track.getBoundingClientRect()\n  const trackWidth = trackRect.width\n  const deltaX = e.clientX - startX\n  const deltaScale = (deltaX / trackWidth) * 1.5\n\n  let newScale = startScale + deltaScale\n  newScale = Math.min(2, Math.max(0.5, newScale))\n  newScale = Math.round(newScale * 10) / 10\n  \n  scale.value = newScale\n}\n\nconst stopDrag = () => {\n  isDragging = false\n  document.removeEventListener('mousemove', onDrag)\n  document.removeEventListener('mouseup', stopDrag)\n}\n\nonUnmounted(() => {\n  document.removeEventListener('mousemove', onDrag)\n  document.removeEventListener('mouseup', stopDrag)\n})\n\nconst handleExit = () => {\n  router.push('/resumes')\n}\n\n// 当前模板数据\nconst currentTemplate = ref(null)\n\n// 处理模板变更\nconst handleTemplateChange = async (template) => {\n  console.log('选择的模板:', template)\n  \n  // 如果还没有档案数据，先获取\n  if (!profileData.value) {\n    await fetchProfileData()\n  }\n  \n  // 将模板数据和档案数据传递给 Canvas 组件\n  if (canvasRef.value) {\n    currentTemplate.value = template\n    canvasRef.value.loadTemplate(template, profileData.value)\n  }\n}\n\n// 处理属性面板的更新\nconst handlePropertyUpdate = (updatedTemplate) => {\n  console.log('属性更新:', updatedTemplate)\n  if (canvasRef.value) {\n    canvasRef.value.loadTemplate(updatedTemplate)\n  }\n}\n\n// 监听模板属性变化\nwatch(currentTemplate, (newTemplate) => {\n  if (newTemplate && canvasRef.value) {\n    canvasRef.value.loadTemplate(newTemplate)\n  }\n}, { deep: true })\n\n// 获取 Canvas 组件的引用\nconst canvasRef = ref(null)\n\n// 更新 Canvas 组件的引用\nconst updateCanvasRef = (el) => {\n  canvasRef.value = el\n}\n</script>\n\n<style scoped>\n.resume-editor {\n  height: 100vh;\n  width: 100vw;\n  display: flex;\n  flex-direction: column;\n  background-color: #f5f5f5;\n  padding: 60px 0 0; /* 只设置顶部padding，为顶部栏留出空间 */\n}\n\n.editor-header {\n  background-color: white !important;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.12);\n  padding: 0 20px;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  z-index: 100;\n}\n\n:deep(.editor-header .v-btn--icon) {\n  width: 40px !important;\n  height: 40px !important;\n  color: rgba(0, 0, 0, 0.87);\n}\n\n:deep(.editor-header .v-btn--icon .v-icon) {\n  font-size: 24px;\n}\n\n:deep(.editor-header .v-btn--icon:hover) {\n  background-color: rgba(0, 0, 0, 0.04);\n}\n\n:deep(.editor-header .v-btn--icon.v-btn--density-default) {\n  width: 40px !important;\n  height: 40px !important;\n}\n\n:deep(.editor-header .text-h6) {\n  font-size: 16px;\n  font-weight: 500;\n  color: rgba(0, 0, 0, 0.87);\n}\n\n:deep(.v-toolbar__content) {\n  height: 60px !important;\n  min-height: 60px !important;\n}\n\n.editor-main {\n  flex: 1;\n  display: flex;\n  height: calc(100vh - 120px); /* 减去顶部栏60px和底部栏60px */\n  min-height: 0;\n  gap: 8px;\n  padding: 8px;\n}\n\n.editor-toolbar {\n  width: 260px;\n  flex: none;\n  min-height: 0;\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n}\n\n.editor-toolbar > .d-flex {\n  height: 100%;\n}\n\n.editor-canvas {\n  flex: 1;\n  min-width: 0;\n  min-height: 0;\n  display: flex;\n  flex-direction: column;\n}\n\n:deep(.editor-canvas .v-card) {\n  border: none;\n  box-shadow: none;\n}\n\n.editor-properties {\n  width: 300px;\n  flex: none;\n  min-height: 0; /* 防止内容溢出 */\n  display: flex;\n  flex-direction: column;\n}\n\n.editor-footer {\n  height: 60px;\n  background-color: white;\n  border-top: 1px solid rgba(0, 0, 0, 0.12);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n:deep(.v-card) {\n  border: none;\n  box-shado