v class=\"text-lg font-medium text-gray-900\">深受欢迎</div>\n                <div class=\"text-gray-500 text-sm\">4.2k+ 用户收藏</div>\n              </div>\n            </div>\n          </div>\n        </div>\n        \n        <!-- 右侧装饰作品 -->\n        <div class=\"absolute right-0 top-0 bottom-0 w-1/3 hidden lg:block pointer-events-none\">\n          <!-- 装饰方块组 1 -->\n          <div class=\"absolute right-24 top-12 w-48 h-36 rotate-6 transform hover:-translate-y-1 transition-transform\">\n            <div class=\"w-full h-full rounded-xl overflow-hidden shadow-lg bg-gradient-to-br from-orange-400/10 to-amber-400/10 p-4\">\n              <div class=\"w-full h-full rounded-lg bg-white/40 backdrop-blur-sm border border-white/60 flex flex-col justify-center items-center\">\n                <DocumentIcon class=\"w-8 h-8 text-orange-500/50 mb-2\" />\n                <div class=\"text-xs text-gray-600\">精选模板</div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 装饰方块组 2 -->\n          <div class=\"absolute right-48 top-32 w-40 h-32 -rotate-3 transform hover:-translate-y-1 transition-transform\">\n            <div class=\"w-full h-full rounded-xl overflow-hidden shadow-lg bg-gradient-to-br from-amber-400/10 to-yellow-400/10 p-4\">\n              <div class=\"w-full h-full rounded-lg bg-white/40 backdrop-blur-sm border border-white/60 flex flex-col justify-center items-center\">\n                <UserGroupIcon class=\"w-8 h-8 text-amber-500/50 mb-2\" />\n                <div class=\"text-xs text-gray-600\">用户信赖</div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 装饰方块组 3 -->\n          <div class=\"absolute right-16 top-56 w-44 h-32 rotate-12 transform hover:-translate-y-1 transition-transform\">\n            <div class=\"w-full h-full rounded-xl overflow-hidden shadow-lg bg-gradient-to-br from-yellow-400/10 to-orange-400/10 p-4\">\n              <div class=\"w-full h-full rounded-lg bg-white/40 backdrop-blur-sm border border-white/60 flex flex-col justify-center items-center\">\n                <HeartIcon class=\"w-8 h-8 text-yellow-500/50 mb-2\" />\n                <div class=\"text-xs text-gray-600\">深受欢迎</div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 装饰线条 -->\n          <div class=\"absolute inset-0\">\n            <div class=\"absolute top-1/4 right-1/3 w-12 h-px bg-gradient-to-r from-transparent via-gray-400/20 to-transparent\"></div>\n            <div class=\"absolute top-1/2 right-1/4 w-16 h-px bg-gradient-to-r from-transparent via-gray-400/20 to-transparent\"></div>\n            <div class=\"absolute bottom-1/3 right-1/2 w-8 h-px bg-gradient-to-r from-transparent via-gray-400/20 to-transparent\"></div>\n          </div>\n\n          <!-- 装饰圆点 -->\n          <div class=\"absolute top-1/4 right-1/3 w-1.5 h-1.5 rounded-full bg-orange-400/20\"></div>\n          <div class=\"absolute top-1/2 right-1/4 w-1.5 h-1.5 rounded-full bg-amber-400/20\"></div>\n          <div class=\"absolute bottom-1/3 right-1/2 w-1.5 h-1.5 rounded-full bg-yellow-400/20\"></div>\n        </div>\n\n        <!-- 背景装饰 -->\n        <div class=\"absolute right-0 top-0 -mt-16 -mr-16\">\n          <div class=\"w-64 h-64 rounded-full bg-gradient-to-r from-orange-200/40 to-amber-200/40 blur-3xl\"></div>\n        </div>\n        <!-- 左侧装饰 -->\n        <div class=\"absolute left-0 bottom-0 -mb-16 -ml-16\">\n          <div class=\"w-48 h-48 rounded-full bg-gradient-to-r from-amber-200/40 to-yellow-200/40 blur-3xl\"></div>\n        </div>\n      </div>\n    </div>\n\n    <!-- PC端导航 -->\n    <div class=\"bg-white rounded-xl border border-gray-100 p-4 hidden md:block\">\n      <div class=\"flex flex-col gap-4\">\n        <!-- 左侧主导航和右侧按钮 -->\n        <div class=\"flex flex-row sm:items-center justify-between\">\n          <!-- 主导航链接 -->\n          <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n            <router-link\n              v-for=\"nav in mainNavs\"\n              :key=\"nav.path\"\n              :to=\"nav.path\"\n              class=\"px-4 sm:px-8 py-3 mx-1 text-sm font-medium transition-colors relative group whitespace-nowrap\"\n              :class=\"[\n                $route.path === nav.path\n                  ? 'text-gray-900'\n                  : 'text-gray-500 hover:text-gray-700'\n              ]\"\n            >\n              {{ nav.name }}\n              <!-- 活跃状态指示器 -->\n              <div \n                class=\"absolute left-1/2 -translate-x-1/2 bottom-4.5 w-1.5 h-1.5 rounded-full transition-colors\"\n                :class=\"$route.path === nav.path ? 'bg-gray-900' : 'bg-transparent group-hover:bg-gray-200'\"\n              ></div>\n            </router-link>\n          </div>\n\n          <!-- 右侧操作按钮 -->\n          <div class=\"hidden sm:flex items-center gap-2 sm:gap-3\">\n            <button class=\"h-9 px-4 sm:px-5 bg-gradient-to-r from-rose-600 to-pink-600 text-white rounded-full hover:shadow-lg hover:shadow-rose-500/20 transition-all duration-300 text-sm font-medium flex items-center group\">\n              <PlusIcon class=\"w-4 h-4 mr-1.5 sm:mr-2 group-hover:scale-110 transition-transform\" />\n              <span>创建简历</span>\n            </button>\n          </div>\n        </div>\n\n        <!-- 分隔线 -->\n        <div class=\"h-px bg-gray-200\"></div>\n        \n        <!-- 二级分类标签 -->\n        <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n          <button\n            v-for=\"category in subCategories[$route.path]\"\n            :key=\"category.id\"\n            @click=\"currentCategory = category.id\"\n            class=\"px-4 py-2 mx-1 rounded-full text-sm font-medium transition-colors whitespace-nowrap\"\n            :class=\"[\n              currentCategory === category.id\n                ? 'bg-gray-900 text-white'\n                : 'bg-gray-100 text-gray-600 hover:bg-gray-200'\n            ]\"\n          >\n            {{ category.name }}\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <!-- 移动端分类导航 -->\n    <div class=\"md:hidden\">\n      <div class=\"bg-white rounded-xl border border-gray-100 overflow-hidden\">\n        <!-- 展开/收起按钮 -->\n        <button \n          @click=\"isMenuExpanded = !isMenuExpanded\"\n          class=\"w-full p-4 flex items-center justify-between\"\n        >\n          <span class=\"text-sm font-medium text-gray-900\">\n            {{ getCurrentNavName }}\n          </span>\n          <ChevronDownIcon \n            class=\"w-5 h-5 text-gray-500 transition-transform\"\n            :class=\"isMenuExpanded ? 'rotate-180' : ''\"\n          />\n        </button>\n\n        <!-- 展开的菜单内容 -->\n        <div \n          v-show=\"isMenuExpanded\"\n          class=\"border-t border-gray-100\"\n        >\n          <!-- 主分类列表 -->\n          <div class=\"divide-y divide-gray-100\">\n            <div\n              v-for=\"nav in mainNavs\"\n              :key=\"nav.path\"\n              class=\"group\"\n            >\n              <!-- 主分类项 -->\n              <button\n                @click=\"handleMainNavClick(nav)\"\n                class=\"w-full p-4 flex items-center justify-between\"\n                :class=\"[\n                  nav.path === '/templates' ? 'bg-gray-50 border-b border-gray-100' : '',\n                  $route.path === nav.path ? 'bg-gray-50' : ''\n                ]\"\n              >\n                <span \n                  class=\"text-sm font-medium\"\n                  :class=\"[\n                    nav.path === '/templates' ? 'text-gray-900' : '',\n                    $route.path === nav.path ? 'text-gray-900' : 'text-gray-500'\n                  ]\"\n                >\n                  {{ nav.name }}\n                </span>\n              </button>\n\n              <!-- 二级分类网格 -->\n              <div \n                v-if=\"$route.path === nav.path\"\n                class=\"grid grid-cols-3 gap-2 p-4 bg-gray-50\"\n              >\n                <button\n                  v-for=\"category in subCategories[nav.path]\"\n                  :key=\"category.id\"\n                  @click=\"currentCategory = category.id\"\n                  class=\"py-2 px-1 rounded-lg text-center transition-colors\"\n                  :class=\"[\n                    currentCategory === category.id\n                      ? 'bg-gray-900 text-white'\n                      : 'bg-white text-gray-600'\n                  ]\"\n                >\n                  <span class=\"text-xs\">{{ category.name }}</span>\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { \n  ChevronDownIcon, \n  PlusIcon,\n  DocumentIcon,\n  UserGroupIcon,\n  HeartIcon \n} from '@heroicons/vue/24/outline'\n\nconst router = useRouter()\nconst isMenuExpanded = ref(false)\nconst currentCategory = ref('all')\n\n// 主导航数据\nconst mainNavs = [\n  { name: '全部模板', path: '/templates' },\n  { name: '校园简历', path: '/templates/campus' },\n  { name: '社会简历', path: '/templates/professional' },\n  { name: '英文简历', path: '/templates/english' }\n]\n\n// 定义各页面的二级分类\nconst subCategories = {\n  '/templates': [\n    { id: 'all', name: '全部' },\n    { id: 'popular', name: '最热门' },\n    { id: 'newest', name: '最新' },\n    { id: 'recommended', name: '推荐' }\n  ],\n  '/templates/campus': [\n    { id: 'all', name: '全部' },\n    { id: 'internship', name: '实习生' },\n    { id: 'graduate', name: '应届生' },\n    { id: 'research', name: '研究生' },\n    { id: 'academic', name: '学术简历' },\n    { id: 'competition', name: '竞赛简历' }\n  ],\n  '/templates/professional': [\n    { id: 'all', name: '全部' },\n    { id: 'tech', name: '技术类' },\n    { id: 'management', name: '管理类' },\n    { id: 'marketing', name: '市场类' },\n    { id: 'finance', name: '金融类' },\n    { id: 'design', name: '设计类' }\n  ],\n  '/templates/english': [\n    { id: 'all', name: '全部' },\n    { id: 'basic', name: '基础型' },\n    { id: 'professional', name: '专业型' },\n    { id: 'executive', name: '高管型' }\n  ]\n}\n\n// 获取当前导航名称\nconst getCurrentNavName = computed(() => {\n  const currentNav = mainNavs.find(nav => nav.path === router.currentRoute.value.path)\n  return currentNav ? currentNav.name : '选择分类'\n})\n\n// 处理主导航点击\nconst handleMainNavClick = (nav) => {\n  if (router.currentRoute.value.path !== nav.path) {\n    router.push(nav.path)\n    currentCategory.value = 'all'\n  }\n}\n\n// 获取横幅描述\nconst getBannerDescription = computed(() => {\n  const descriptions = {\n    '/templates': '精选优质简历模板，助你打造完美简历',\n    '/templates/campus': '为在校生和应届生量身定制的简历模板',\n    '/templates/professional': '专业的职场简历模板，助力职业发展',\n    '/templates/english': '专业的英文简历模板，助力海外求职'\n  }\n  return descriptions[router.currentRoute.value.path] || ''\n})\n\n// 暴露给父组件的数据\ndefineExpose({\n  currentCategory\n})\n</script> "],"mappings":"AAwOA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAI;AAClC,SAASC,SAAS,QAAQ,YAAW;AACrC,SACEC,eAAe,EACfC,QAAQ,EACRC,YAAY,EACZC,aAAa,EACbC,SAAS,QACJ,2BAA0B;;;;;;IAEjC,MAAMC,MAAM,GAAGN,SAAS,CAAC;IACzB,MAAMO,cAAc,GAAGT,GAAG,CAAC,KAAK;IAChC,MAAMU,eAAe,GAAGV,GAAG,CAAC,KAAK;;IAEjC;IACA,MAAMW,QAAQ,GAAG,CACf;MAAEC,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAa,CAAC,EACpC;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAoB,CAAC,EAC3C;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAA0B,CAAC,EACjD;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAqB,EAC7C;;IAEA;IACA,MAAMC,aAAa,GAAG;MACpB,YAAY,EAAE,CACZ;QAAEC,EAAE,EAAE,KAAK;QAAEH,IAAI,EAAE;MAAK,CAAC,EACzB;QAAEG,EAAE,EAAE,SAAS;QAAEH,IAAI,EAAE;MAAM,CAAC,EAC9B;QAAEG,EAAE,EAAE,QAAQ;QAAEH,IAAI,EAAE;MAAK,CAAC,EAC5B;QAAEG,EAAE,EAAE,aAAa;QAAEH,IAAI,EAAE;MAAK,EACjC;MACD,mBAAmB,EAAE,CACnB;QAAEG,EAAE,EAAE,KAAK;QAAEH,IAAI,EAAE;MAAK,CAAC,EACzB;QAAEG,EAAE,EAAE,YAAY;QAAEH,IAAI,EAAE;MAAM,CAAC,EACjC;QAAEG,EAAE,EAAE,UAAU;QAAEH,IAAI,EAAE;MAAM,CAAC,EAC/B;QAAEG,EAAE,EAAE,UAAU;QAAEH,IAAI,EAAE;MAAM,CAAC,EAC/B;QAAEG,EAAE,EAAE,UAAU;QAAEH,IAAI,EAAE;MAAO,CAAC,EAChC;QA