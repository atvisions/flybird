{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ChevronDownIcon, PlusIcon, DocumentIcon, UserGroupIcon, HeartIcon } from '@heroicons/vue/24/outline';\nexport default {\n  __name: 'TemplateNavigation',\n  setup(__props, {\n    expose: __expose\n  }) {\n    const router = useRouter();\n    const isMenuExpanded = ref(false);\n    const currentCategory = ref('all');\n\n    // 主导航数据\n    const mainNavs = [{\n      name: '全部模板',\n      path: '/templates'\n    }, {\n      name: '校园简历',\n      path: '/templates/campus'\n    }, {\n      name: '社会简历',\n      path: '/templates/professional'\n    }, {\n      name: '英文简历',\n      path: '/templates/english'\n    }];\n\n    // 定义各页面的二级分类\n    const subCategories = {\n      '/templates': [{\n        id: 'all',\n        name: '全部'\n      }, {\n        id: 'popular',\n        name: '最热门'\n      }, {\n        id: 'newest',\n        name: '最新'\n      }, {\n        id: 'recommended',\n        name: '推荐'\n      }],\n      '/templates/campus': [{\n        id: 'all',\n        name: '全部'\n      }, {\n        id: 'internship',\n        name: '实习生'\n      }, {\n        id: 'graduate',\n        name: '应届生'\n      }, {\n        id: 'research',\n        name: '研究生'\n      }, {\n        id: 'academic',\n        name: '学术简历'\n      }, {\n        id: 'competition',\n        name: '竞赛简历'\n      }],\n      '/templates/professional': [{\n        id: 'all',\n        name: '全部'\n      }, {\n        id: 'tech',\n        name: '技术类'\n      }, {\n        id: 'management',\n        name: '管理类'\n      }, {\n        id: 'marketing',\n        name: '市场类'\n      }, {\n        id: 'finance',\n        name: '金融类'\n      }, {\n        id: 'design',\n        name: '设计类'\n      }],\n      '/templates/english': [{\n        id: 'all',\n        name: '全部'\n      }, {\n        id: 'basic',\n        name: '基础型'\n      }, {\n        id: 'professional',\n        name: '专业型'\n      }, {\n        id: 'executive',\n        name: '高管型'\n      }]\n    };\n\n    // 获取当前导航名称\n    const getCurrentNavName = computed(() => {\n      const currentNav = mainNavs.find(nav => nav.path === router.currentRoute.value.path);\n      return currentNav ? currentNav.name : '选择分类';\n    });\n\n    // 处理主导航点击\n    const handleMainNavClick = nav => {\n      if (router.currentRoute.value.path !== nav.path) {\n        router.push(nav.path);\n        currentCategory.value = 'all';\n      }\n    };\n\n    // 获取横幅描述\n    const getBannerDescription = computed(() => {\n      const descriptions = {\n        '/templates': '精选优质简历模板，助你打造完美简历',\n        '/templates/campus': '为在校生和应届生量身定制的简历模板',\n        '/templates/professional': '专业的职场简历模板，助力职业发展',\n        '/templates/english': '专业的英文简历模板，助力海外求职'\n      };\n      return descriptions[router.currentRoute.value.path] || '';\n    });\n\n    // 暴露给父组件的数据\n    __expose({\n      currentCategory\n    });\n    const __returned__ = {\n      router,\n      isMenuExpanded,\n      currentCategory,\n      mainNavs,\n      subCategories,\n      getCurrentNavName,\n      handleMainNavClick,\n      getBannerDescription,\n      ref,\n      computed,\n      get useRouter() {\n        return useRouter;\n      },\n      get ChevronDownIcon() {\n        return ChevronDownIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      },\n      get DocumentIcon() {\n        return DocumentIcon;\n      },\n      get UserGroupIcon() {\n        return UserGroupIcon;\n      },\n      get HeartIcon() {\n        return HeartIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRouter","ChevronDownIcon","PlusIcon","DocumentIcon","UserGroupIcon","HeartIcon","router","isMenuExpanded","currentCategory","mainNavs","name","path","subCategories","id","getCurrentNavName","currentNav","find","nav","currentRoute","value",