{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nimport { DocumentIcon, SparklesIcon, ShareIcon, UserIcon, PlusIcon } from '@heroicons/vue/24/outline';\nexport default {\n  __name: 'ResumeView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const route = useRoute();\n    const store = useStore();\n\n    // 使用 Vuex store 的 isAuthenticated 状态\n    const isAuthenticated = computed(() => store.state.isAuthenticated);\n\n    // 主导航数据\n    const mainNavs = [{\n      name: '全部模板',\n      path: '/resume'\n    }, {\n      name: '校园简历',\n      path: '/resume/campus'\n    }, {\n      name: '社会简历',\n      path: '/resume/professional'\n    }, {\n      name: '英文简历',\n      path: '/resume/english'\n    }];\n\n    // 当前选中的分类\n    const currentCategory = ref('all');\n\n    // 简历分类\n    const resumeCategories = [{\n      id: 'all',\n      name: '全部'\n    }, {\n      id: 'popular',\n      name: '热门推荐'\n    }, {\n      id: 'fresh',\n      name: '应届生'\n    }, {\n      id: 'experienced',\n      name: '经验型'\n    }, {\n      id: 'manager',\n      name: '管理层'\n    }, {\n      id: 'creative',\n      name: '创意型'\n    }, {\n      id: 'minimal',\n      name: '简约风'\n    }, {\n      id: 'professional',\n      name: '专业型'\n    }];\n    const __returned__ = {\n      router,\n      route,\n      store,\n      isAuthenticated,\n      mainNavs,\n      currentCategory,\n      resumeCategories,\n      ref,\n      computed,\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get useStore() {\n        return useStore;\n      },\n      get DocumentIcon() {\n        return DocumentIcon;\n      },\n      get SparklesIcon() {\n        return SparklesIcon;\n      },\n      get ShareIcon() {\n        return ShareIcon;\n      },\n      get UserIcon() {\n        return UserIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRouter","useRoute","useStore","DocumentIcon","SparklesIcon","ShareIcon","UserIcon","PlusIcon","router","route","store","isAuthenticated","state","mainNavs","name","path","currentCategory","resumeCategories","id"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/resume/ResumeView.vue"],"sourcesContent":["<template>\n  <div class=\"py-16 mt-8\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅区域 -->\n      <div class=\"relative overflow-hidden rounded-2xl bg-gradient-to-r from-rose-50/90 via-pink-50/80 to-orange-50/70 p-8 sm:p-12 min-h-[320px]\">\n        <div class=\"relative z-10\">\n          <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">简历模板</h1>\n          <p class=\"text-gray-600 text-lg max-w-2xl\">精选优质简历模板，打造专业求职形象</p>\n          \n          <!-- 数据统计 -->\n          <div class=\"flex flex-wrap gap-8 mt-8\">\n            <div class=\"flex items-center space-x-3\">\n              <div class=\"w-12 h-12 rounded-full bg-rose-50 flex items-center justify-center\">\n                <DocumentIcon class=\"w-6 h-6 text-rose-600\" />\n              </div>\n              <div>\n                <div class=\"text-lg font-medium text-gray-900\">专业模板</div>\n                <div class=\"text-gray-500 text-sm\">多种风格任你选择</div>\n              </div>\n            </div>\n            <div class=\"flex items-center space-x-3\">\n              <div class=\"w-12 h-12 rounded-full bg-pink-50 flex items-center justify-center\">\n                <SparklesIcon class=\"w-6 h-6 text-pink-600\" />\n              </div>\n              <div>\n                <div class=\"text-lg font-medium text-gray-900\">在线编辑</div>\n                <div class=\"text-gray-500 text-sm\">便捷的编辑与导出</div>\n              </div>\n            </div>\n            <div class=\"flex items-center space-x-3\">\n              <div class=\"w-12 h-12 rounded-full bg-orange-50 flex items-center justify-center\">\n                <ShareIcon class=\"w-6 h-6 text-orange-600\" />\n              </div>\n              <div>\n                <div class=\"text-lg font-medium text-gray-900\">简历指导</div>\n                <div class=\"text-gray-500 text-sm\">专业的优化建议</div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 右侧装饰 -->\n        <div class=\"absolute right-0 top-0 bottom-0 w-1/3 hidden lg:block pointer-events-none\">\n          <!-- 装饰方块组 1 -->\n          <div class=\"absolute right-24 top-12 w-48 h-36 rotate-6 transform hover:-translate-y-1 transition-transform\">\n            <div class=\"w-full h-full rounded-xl overflow-hidden shadow-lg bg-gradient-to-br from-rose-400/10 to-pink-400/10 p-4\">\n              <div class=\"w-full h-full rounded-lg bg-white/40 backdrop-blur-sm border border-white/60\"></div>\n            </div>\n          </div>\n\n          <!-- 装饰方块组 2 -->\n          <div class=\"absolute right-48 top-32 w-40 h-32 -rotate-3 transform hover:-translate-y-1 transition-transform\">\n            <div class=\"w-full h-full rounded-xl overflow-hidden shadow-lg bg-gradient-to-br from-pink-400/10 to-orange-400/10 p-4\">\n              <div class=\"w-full h-full rounded-lg bg-white/40 backdrop-blur-sm border border-white/60\"></div>\n            </div>\n          </div>\n\n          <!-- 装饰方块组 3 -->\n          <div class=\"absolute right-16 top-56 w-44 h-32 rotate-12 transform hover:-translate-y-1 transition-transform\">\n            <div class=\"w-full h-full rounded-xl overflow-hidden shadow-lg bg-gradient-to-br from-orange-400/10 to-rose-400/10 p-4\">\n              <div class=\"w-full h-full rounded-lg bg-white/40 backdrop-blur-sm border border-white/60\">\n                <!-- 装饰圆点 -->\n                <div class=\"absolute top-2 left-2 w-1 h-1 rounded-full bg-rose-400/20\"></div>\n                <div class=\"absolute bottom-2 right-2 w-1 h-1 rounded-full bg-pink-400/20\"></div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 装饰线条 -->\n          <div class=\"absolute inset-0\">\n            <div class=\"absolute top-1/4 right-1/3 w-12 h-px bg-gradient-to-r from-transparent via-gray-400/20 to-transparent\"></div>\n            <div class=\"absolute top-1/2 right-1/4 w-16 h-px bg-gradient-to-r from-transparent via-gray-400/20 to-transparent\"></div>\n            <div class=\"absolute bottom-1/3 right-1/2 w-8 h-px bg-gradient-to-r from-transparent via-gray-400/20 to-transparent\"></div>\n          </div>\n\n          <!-- 装饰圆点 -->\n          <div class=\"absolute top-1/4 right-1/3 w-1.5 h-1.5 rounded-full bg-rose-400/20\"></div>\n          <div class=\"absolute top-1/2 right-1/4 w-1.5 h-1.5 rounded-full bg-pink-400/20\"></div>\n          <div class=\"absolute bottom-1/3 right-1/2 w-1.5 h-1.5 rounded-full bg-orange-400/20\"></div>\n        </div>\n\n        <!-- 背景装饰 -->\n        <div class=\"absolute right-0 top-0 -mt-16 -mr-16 pointer-events-none\">\n          <div class=\"w-64 h-64 rounded-full bg-gradient-to-r from-rose-200/40 to-pink-200/40 blur-3xl\"></div>\n        </div>\n        <div class=\"absolute left-0 bottom-0 -mb-16 -ml-16 pointer-events-none\">\n          <div class=\"w-48 h-48 rounded-full bg-gradient-to-r from-pink-200/40 to-orange-200/40 blur-3xl\"></div>\n        </div>\n      </div>\n\n      <!-- 主导航区域 -->\n      <div class=\"mt-8 bg-white rounded-xl border border-gray-100 p-4 hidden md:block\">\n        <div class=\"flex flex-col gap-4\">\n          <!-- 左侧主导航和右侧按钮 -->\n          <div class=\"flex flex-row sm:items-center justify-between\">\n            <!-- 主导航链接 -->\n            <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n              <router-link\n                v-for=\"nav in mainNavs\"\n                :key=\"nav.path\"\n                :to=\"nav.path\"\n                class=\"px-4 sm:px-8 py-3 mx-1 text-sm font-medium transition-colors relative group whitespace-nowrap\"\n                :class=\"[\n                  $route.path === nav.path\n                    ? 'text-gray-900'\n                    : 'text-gray-500 hover:text-gray-700'\n                ]\"\n              >\n                {{ nav.name }}\n                <!-- 活跃状态指示器 -->\n                <div \n                  class=\"absolute left-1/2 -translate-x-1/2 bottom-4.5 w-1.5 h-1.5 rounded-full transition-colors\"\n                  :class=\"$route.path === nav.path ? 'bg-gray-900' : 'bg-transparent group-hover:bg-gray-200'\"\n                ></div>\n              </router-link>\n            </div>\n\n            <!-- 右侧操作按钮 -->\n            <div class=\"flex items-center gap-2 sm:gap-3\">\n              <template v-if=\"isAuthenticated\">\n                <!-- 创建简历按钮 -->\n                <button class=\"h-9 px-4 sm:px-5 bg-gradient-to-r from-rose-600 to-pink-600 text-white rounded-full hover:shadow-lg hover:shadow-rose-500/20 transition-all duration-300 text-sm font-medium flex items-center group\">\n                  <PlusIcon class=\"w-4 h-4 mr-1.5 sm:mr-2 group-hover:scale-110 transition-transform\" />\n                  <span class=\"hidden sm:inline\">创建简历</span>\n                  <span class=\"sm:hidden\">创建</span>\n                </button>\n              </template>\n              <template v-else>\n                <!-- 登录按钮 -->\n                <router-link \n                  :to=\"`/login?redirect=${encodeURIComponent($route.fullPath)}`\"\n                  class=\"h-9 px-4 sm:px-5 bg-gradient-to-r from-rose-600 to-pink-600 text-white rounded-full hover:shadow-lg hover:shadow-rose-500/20 transition-all duration-300 text-sm font-medium flex items-center group\"\n                >\n                  <UserIcon class=\"w-4 h-4 mr-1.5 sm:mr-2 group-hover:scale-110 transition-transform\" />\n                  <span>登录</span>\n                </router-link>\n              </template>\n            </div>\n          </div>\n\n          <!-- 分隔线 -->\n          <div class=\"h-px bg-gray-200\"></div>\n          \n          <!-- 二级分类标签 -->\n          <div class=\"flex items-center -mx-1 overflow-x-auto no-scrollbar\">\n            <button\n              v-for=\"category in resumeCategories\"\n              :key=\"category.id\"\n              @click=\"currentCategory = category.id\"\n              class=\"px-4 py-2 mx-1 rounded-full text-sm font-medium transition-colors whitespace-nowrap\"\n              :class=\"[\n                currentCategory === category.id\n                  ? 'bg-gray-900 text-white'\n                  : 'bg-gray-100 text-gray-600 hover:bg-gray-200'\n              ]\"\n            >\n              {{ category.name }}\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <!-- 简历模板列表 -->\n      <div class=\"mt-6\">\n        <!-- 这里添加简历模板列表的具体实现 -->\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport { useRouter, useRoute } from 'vue-router'\nimport { useStore } from 'vuex'\nimport {\n  DocumentIcon,\n  SparklesIcon,\n  ShareIcon,\n  UserIcon,\n  PlusIcon,\n} from '@heroicons/vue/24/outline'\n\nconst router = useRouter()\nconst route = useRoute()\nconst store = useStore()\n\n// 使用 Vuex store 的 isAuthenticated 状态\nconst isAuthenticated = computed(() => store.state.isAuthenticated)\n\n// 主导航数据\nconst mainNavs = [\n  { name: '全部模板', path: '/resume' },\n  { name: '校园简历', path: '/resume/campus' },\n  { name: '社会简历', path: '/resume/professional' },\n  { name: '英文简历', path: '/resume/english' }\n]\n\n// 当前选中的分类\nconst currentCategory = ref('all')\n\n// 简历分类\nconst resumeCategories = [\n  { id: 'all', name: '全部' },\n  { id: 'popular', name: '热门推荐' },\n  { id: 'fresh', name: '应届生' },\n  { id: 'experienced', name: '经验型' },\n  { id: 'manager', name: '管理层' },\n  { id: 'creative', name: '创意型' },\n  { id: 'minimal', name: '简约风' },\n  { id: 'professional', name: '专业型' }\n]\n</script> "],"mappings":"AA2KA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAI;AAClC,SAASC,SAAS,EAAEC,QAAQ,QAAQ,YAAW;AAC/C,SAASC,QAAQ,QAAQ,MAAK;AAC9B,SACEC,YAAY,EACZC,YAAY,EACZC,SAAS,EACTC,QAAQ,EACRC,QAAQ,QACH,2BAA0B;;;;;;;IAEjC,MAAMC,MAAM,GAAGR,SAAS,CAAC;IACzB,MAAMS,KAAK,GAAGR,QAAQ,CAAC;IACvB,MAAMS,KAAK,GAAGR,QAAQ,CAAC;;IAEvB;IACA,MAAMS,eAAe,GAAGZ,QAAQ,CAAC,MAAMW,KAAK,CAACE,KAAK,CAACD,eAAe;;IAElE;IACA,MAAME,QAAQ,GAAG,CACf;MAAEC,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAU,CAAC,EACjC;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAiB,CAAC,EACxC;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAuB,CAAC,EAC9C;MAAED,IAAI,EAAE,MAAM;MAAEC,IAAI,EAAE;IAAkB,EAC1C;;IAEA;IACA,MAAMC,eAAe,GAAGlB,GAAG,CAAC,KAAK;;IAEjC;IACA,MAAMmB,gBAAgB,GAAG,CACvB;MAAEC,EAAE,EAAE,KAAK;MAAEJ,IAAI,EAAE;IAAK,CAAC,EACzB;MAAEI,EAAE,EAAE,SAAS;MAAEJ,IAAI,EAAE;IAAO,CAAC,EAC/B;MAAEI,EAAE,EAAE,OAAO;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAC5B;MAAEI,EAAE,EAAE,aAAa;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAClC;MAAEI,EAAE,EAAE,SAAS;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAC9B;MAAEI,EAAE,EAAE,UAAU;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAC/B;MAAEI,EAAE,EAAE,SAAS;MAAEJ,IAAI,EAAE;IAAM,CAAC,EAC9B;MAAEI,EAAE,EAAE,cAAc;MAAEJ,IAAI,EAAE;IAAM,EACpC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}