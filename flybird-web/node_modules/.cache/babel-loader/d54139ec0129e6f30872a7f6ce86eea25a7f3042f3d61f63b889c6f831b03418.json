{"ast":null,"code":"import { ref, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { SparklesIcon, DocumentTextIcon, CloudArrowDownIcon, ShieldCheckIcon, UserGroupIcon } from '@heroicons/vue/24/outline';\nexport default {\n  __name: 'ProDialog',\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n    const router = useRouter();\n    const dialogVisible = ref(props.modelValue);\n\n    // 监听 modelValue 变化\n    watch(() => props.modelValue, val => {\n      dialogVisible.value = val;\n    });\n\n    // 监听 dialogVisible 变化\n    watch(() => dialogVisible.value, val => {\n      emit('update:modelValue', val);\n    });\n\n    // 会员权益列表\n    const benefits = [{\n      name: '简历导入',\n      desc: '一键导入解析',\n      icon: DocumentTextIcon\n    }, {\n      name: '批量下载',\n      desc: '无限次下载',\n      icon: CloudArrowDownIcon\n    }, {\n      name: '隐私保护',\n      desc: '数据安全加密',\n      icon: ShieldCheckIcon\n    }, {\n      name: '专属客服',\n      desc: '7×24小时服务',\n      icon: UserGroupIcon\n    }];\n    const handleClose = () => {\n      dialogVisible.value = false;\n    };\n    const handleUpgrade = () => {\n      dialogVisible.value = false;\n      router.push('/pro');\n    };\n    const __returned__ = {\n      props,\n      emit,\n      router,\n      dialogVisible,\n      benefits,\n      handleClose,\n      handleUpgrade,\n      ref,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get SparklesIcon() {\n        return SparklesIcon;\n      },\n      get DocumentTextIcon() {\n        return DocumentTextIcon;\n      },\n      get CloudArrowDownIcon() {\n        return CloudArrowDownIcon;\n      },\n      get ShieldCheckIcon() {\n        return ShieldCheckIcon;\n      },\n      get UserGroupIcon() {\n        return UserGroupIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","watch","useRouter","SparklesIcon","DocumentTextIcon","CloudArrowDownIcon","ShieldCheckIcon","UserGroupIcon","props","__props","emit","__emit","router","dialogVisible","modelValue","val","value","benefits","name","desc","icon","handleClose","handleUpgrade","push"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/components/ProDialog.vue"],"sourcesContent":["<template>\n  <el-dialog\n    v-model=\"dialogVisible\"\n    :show-close=\"true\"\n    width=\"340px\"\n    class=\"pro-dialog\"\n    @close=\"handleClose\"\n  >\n    <div class=\"relative p-6 text-center bg-[#1A1A1A]\">\n      <!-- 顶部装饰 -->\n      <div class=\"absolute -top-3 left-1/2 -translate-x-1/2 w-24 h-6 bg-[#1A1A1A] rounded-b-xl\"></div>\n      <div class=\"absolute -top-5 left-1/2 -translate-x-1/2\">\n        <div class=\"w-10 h-10 bg-[#1A1A1A] rounded-full border border-yellow-500/30 flex items-center justify-center\">\n          <div class=\"w-8 h-8 rounded-full bg-gradient-to-r from-yellow-500 to-amber-500 flex items-center justify-center\">\n            <SparklesIcon class=\"w-5 h-5 text-white\" />\n          </div>\n        </div>\n      </div>\n\n      <!-- 标题 -->\n      <div class=\"mt-4 mb-6\">\n        <h3 class=\"text-lg font-medium text-white\">专属权益升级</h3>\n        <div class=\"mt-1 text-xs text-yellow-500/80\">解锁全部高级功能</div>\n      </div>\n\n      <!-- 会员权益列表 -->\n      <div class=\"grid grid-cols-2 gap-3 mb-6\">\n        <div v-for=\"(benefit, index) in benefits\" :key=\"index\" \n          class=\"flex items-center p-3 rounded-lg bg-white/5 hover:bg-white/10 transition-colors group cursor-default\"\n        >\n          <div class=\"w-8 h-8 rounded-lg bg-gradient-to-br from-yellow-500/20 to-amber-500/20 flex items-center justify-center mr-3\">\n            <component :is=\"benefit.icon\" class=\"w-4 h-4 text-yellow-500\" />\n          </div>\n          <div class=\"text-left\">\n            <div class=\"text-xs text-white/90\">{{ benefit.name }}</div>\n            <div class=\"text-[10px] text-yellow-500/60\">{{ benefit.desc }}</div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 价格信息 -->\n      <div class=\"inline-flex items-center px-4 py-2 rounded-full bg-white/5 mb-6\">\n        <span class=\"text-xs text-white/60 mr-2\">仅需</span>\n        <span class=\"text-sm text-yellow-500\">¥</span>\n        <span class=\"text-2xl font-medium text-yellow-500 mx-0.5 -mt-0.5\">0.25</span>\n        <span class=\"text-xs text-white/60\">/天</span>\n      </div>\n\n      <!-- 按钮 -->\n      <button\n        class=\"w-full h-11 bg-gradient-to-r from-yellow-500 to-amber-500 text-white font-medium rounded-lg hover:from-yellow-400 hover:to-amber-400 transition-all duration-300 transform hover:shadow-[0_0_20px_rgba(245,158,11,0.3)]\"\n        @click=\"handleUpgrade\"\n      >\n        立即升级\n      </button>\n    </div>\n  </el-dialog>\n</template>\n\n<script setup>\nimport { ref, defineProps, defineEmits, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport {\n  SparklesIcon,\n  DocumentTextIcon,\n  CloudArrowDownIcon,\n  ShieldCheckIcon,\n  UserGroupIcon\n} from '@heroicons/vue/24/outline'\n\nconst props = defineProps({\n  modelValue: {\n    type: Boolean,\n    default: false\n  }\n})\n\nconst emit = defineEmits(['update:modelValue'])\nconst router = useRouter()\n\nconst dialogVisible = ref(props.modelValue)\n\n// 监听 modelValue 变化\nwatch(() => props.modelValue, (val) => {\n  dialogVisible.value = val\n})\n\n// 监听 dialogVisible 变化\nwatch(() => dialogVisible.value, (val) => {\n  emit('update:modelValue', val)\n})\n\n// 会员权益列表\nconst benefits = [\n  {\n    name: '简历导入',\n    desc: '一键导入解析',\n    icon: DocumentTextIcon\n  },\n  {\n    name: '批量下载',\n    desc: '无限次下载',\n    icon: CloudArrowDownIcon\n  },\n  {\n    name: '隐私保护',\n    desc: '数据安全加密',\n    icon: ShieldCheckIcon\n  },\n  {\n    name: '专属客服',\n    desc: '7×24小时服务',\n    icon: UserGroupIcon\n  }\n]\n\nconst handleClose = () => {\n  dialogVisible.value = false\n}\n\nconst handleUpgrade = () => {\n  dialogVisible.value = false\n  router.push('/pro')\n}\n</script>\n\n<style scoped>\n.pro-dialog :deep(.el-dialog) {\n  border: 1px solid rgba(245, 158, 11, 0.1);\n  border-radius: 16px;\n  overflow: visible;\n  background: #1A1A1A;\n  box-shadow: \n    0 25px 50px -12px rgba(0, 0, 0, 0.5),\n    0 0 30px rgba(245, 158, 11, 0.1);\n}\n\n.pro-dialog :deep(.el-dialog__header) {\n  display: none;\n}\n\n.pro-dialog :deep(.el-dialog__body) {\n  padding: 0;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn) {\n  top: -12px;\n  right: -12px;\n  z-index: 10;\n  background: #1A1A1A;\n  border: 1px solid rgba(245, 158, 11, 0.2);\n  border-radius: 50%;\n  width: 24px;\n  height: 24px;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn .el-dialog__close) {\n  color: rgba(245, 158, 11, 0.6);\n  font-size: 16px;\n  transition: color 0.3s;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn:hover .el-dialog__close) {\n  color: rgba(245, 158, 11, 0.9);\n}\n\n/* 添加动画效果 */\n@keyframes scaleIn {\n  from {\n    opacity: 0;\n    transform: scale(0.95);\n  }\n  to {\n    opacity: 1;\n    transform: scale(1);\n  }\n}\n\n.pro-dialog :deep(.el-dialog) {\n  animation: scaleIn 0.2s ease-out;\n}\n</style> "],"mappings":"AA4DA,SAASA,GAA6B,EAAEC,KAAK,QAAQ,KAAI;AACzD,SAASC,SAAS,QAAQ,YAAW;AACrC,SACEC,YAAY,EACZC,gBAAgB,EAChBC,kBAAkB,EAClBC,eAAe,EACfC,aAAY,QACP,2BAA0B;;;;;;;;;;;;;;;IAEjC,MAAMC,KAAK,GAAGC,OAAA;IAOd,MAAMC,IAAI,GAAGC,MAAA;IACb,MAAMC,MAAM,GAAGV,SAAS,CAAC;IAEzB,MAAMW,aAAa,GAAGb,GAAG,CAACQ,KAAK,CAACM,UAAU;;IAE1C;IACAb,KAAK,CAAC,MAAMO,KAAK,CAACM,UAAU,EAAGC,GAAG,IAAK;MACrCF,aAAa,CAACG,KAAK,GAAGD,GAAE;IAC1B,CAAC;;IAED;IACAd,KAAK,CAAC,MAAMY,aAAa,CAACG,KAAK,EAAGD,GAAG,IAAK;MACxCL,IAAI,CAAC,mBAAmB,EAAEK,GAAG;IAC/B,CAAC;;IAED;IACA,MAAME,QAAQ,GAAG,CACf;MACEC,IAAI,EAAE,MAAM;MAC