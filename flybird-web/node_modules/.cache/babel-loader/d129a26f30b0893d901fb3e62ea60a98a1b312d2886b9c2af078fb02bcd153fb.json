{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { useStore } from 'vuex';\nimport { MEDIA_URL } from '@/config';\nimport defaultAvatarImage from '@/assets/images/default-avatar.png';\nimport defaultBackground from '@/assets/images/default-background.jpg';\nimport { showToast } from '@/components/ToastMessage';\nimport { UserIcon, UserCircleIcon, DocumentTextIcon, BriefcaseIcon, StarIcon, CameraIcon, PhotoIcon, FolderIcon, Cog6ToothIcon, IdentificationIcon, HomeIcon, MapPinIcon, QuoteIcon, PencilSquareIcon } from '@heroicons/vue/24/outline';\nimport profile from '@/api/profile';\nimport { eventBus } from '@/utils/eventBus';\nexport default {\n  __name: 'UserHome',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const store = useStore();\n\n    // 用户信息状态\n    const userInfo = ref({\n      nickname: '',\n      avatar: '',\n      coverImage: '',\n      gender: '',\n      isVip: false,\n      bio: '',\n      location: '',\n      jobTitle: ''\n    });\n\n    // 获取用户基本信息\n    const fetchUserInfo = async () => {\n      try {\n        const response = await profile.getData();\n        if (response.data?.code === 200 && response.data?.data) {\n          const data = response.data.data;\n          userInfo.value = {\n            nickname: data.user?.username || data.basic_info?.name || '',\n            avatar: formatImageUrl(data.basic_info?.avatar),\n            coverImage: formatImageUrl(data.basic_info?.background),\n            gender: data.basic_info?.gender,\n            isVip: data.is_vip,\n            bio: data.basic_info?.personal_summary,\n            location: data.basic_info?.location,\n            jobTitle: data.job_intention?.job_title\n          };\n        }\n      } catch (error) {\n        console.error('获取用户信息失败:', error);\n        showToast('获取用户信息失败', 'error');\n      }\n    };\n\n    // 处理头像上传\n    const handleAvatarUpload = async event => {\n      const file = event.target.files[0];\n      if (!file) return;\n      if (!validateFile(file)) {\n        return;\n      }\n      try {\n        const formData = new FormData();\n        formData.append('avatar', file);\n        const response = await profile.uploadAvatar(formData);\n        if (response.data?.code === 200) {\n          showToast('头像上传成功', 'success');\n          const avatarUrl = response.data.data.avatar;\n          userInfo.value.avatar = formatImageUrl(avatarUrl);\n          eventBus.emit('avatar-updated', avatarUrl);\n        } else {\n          throw new Error(response.data?.message || '上传失败');\n        }\n      } catch (error) {\n        console.error('头像上传失败:', error);\n        showToast(error.message || '头像上传失败，请稍后重试', 'error');\n      }\n    };\n\n    // 处理背景图上传\n    const handleCoverUpload = async event => {\n      const file = event.target.files[0];\n      if (!file) return;\n      if (!validateFile(file)) {\n        return;\n      }\n      const formData = new FormData();\n      formData.append('background', file);\n      try {\n        const response = await profile.uploadBackground(formData);\n        if (response.data?.code === 200) {\n          showToast('背景图更新成功', 'success');\n          const backgroundUrl = response.data.data.background;\n          userInfo.value.coverImage = formatImageUrl(backgroundUrl);\n          store.commit('UPDATE_BACKGROUND', backgroundUrl);\n        }\n      } catch (error) {\n        console.error('背景图更新失败:', error);\n        showToast('背景图更新失败', 'error');\n      } finally {\n        event.target.value = '';\n      }\n    };\n\n    // 工具函数\n    const formatImageUrl = url => {\n      if (!url) return '';\n      return url.startsWith('http') ? url : `${MEDIA_URL}${url}`;\n    };\n    const validateFile = file => {\n      // 验证文件类型\n      if (!file.type.startsWith('image/')) {\n        showToast('请选择图片文件', 'error');\n        return false;\n      }\n\n      // 验证文件大小（限制为 2MB）\n      const maxSize = 2 * 1024 * 1024;\n      if (file.size > maxSize) {\n        showToast('图片大小不能超过 2MB', 'error');\n        return false;\n      }\n      return true;\n    };\n\n    // 错误处理函数\n    const handleImageError = e => {\n      e.target.src = defaultAvatarImage;\n    };\n    const handleBackgroundError = e => {\n      e.target.src = defaultBackground;\n    };\n\n    // 在组件挂载时获取用户信息\n    onMounted(async () => {\n      await fetchUserInfo();\n    });\n\n    // 档案管理模块\n    const profileActions = [{\n      title: '我的档案',\n      icon: IdentificationIcon,\n      action: () => router.push('/user/center?tab=profile')\n    }, {\n      title: '我的简历',\n      icon: DocumentTextIcon,\n      action: () => router.push('/user/center?tab=resumes'),\n      count: '2份'\n    }];\n\n    // 内容管理模块\n    const contentActions = [{\n      title: '作品集',\n      icon: PhotoIcon,\n      action: () => router.push('/user/center?tab=portfolio'),\n      count: '8个'\n    }, {\n      title: '收藏夹',\n      icon: StarIcon,\n      action: () => router.push('/user/center?tab=favorites'),\n      count: '16个'\n    }, {\n      title: '我的创作',\n      icon: BriefcaseIcon,\n      action: () => router.push('/user/center?tab=creations'),\n      count: '5篇'\n    }, {\n      title: '我的主页',\n      icon: HomeIcon,\n      action: () => router.push('/user/center?tab=homepage')\n    }];\n\n    // 系统设置模块\n    const settingsActions = [{\n      title: '账户设置',\n      icon: Cog6ToothIcon,\n      action: () => router.push('/user/center?tab=account')\n    }];\n\n    // 统计数据\n    const statistics = [{\n      label: '简历数',\n      value: '2'\n    }, {\n      label: '作品',\n      value: '8'\n    }, {\n      label: '收藏',\n      value: '16'\n    }, {\n      label: '获赞',\n      value: '32'\n    }];\n    const __returned__ = {\n      router,\n      store,\n      userInfo,\n      fetchUserInfo,\n      handleAvatarUpload,\n      handleCoverUpload,\n      formatImageUrl,\n      validateFile,\n      handleImageError,\n      handleBackgroundError,\n      profileActions,\n      contentActions,\n      settingsActions,\n      statistics,\n      ref,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get useStore() {\n        return useStore;\n      },\n      get MEDIA_URL() {\n        return MEDIA_URL;\n      },\n      get defaultAvatarImage() {\n        return defaultAvatarImage;\n      },\n      get defaultBackground() {\n        return defaultBackground;\n      },\n      get showToast() {\n        return showToast;\n      },\n      get UserIcon() {\n        return UserIcon;\n      },\n      get UserCircleIcon() {\n        return UserCircleIcon;\n      },\n      get DocumentTextIcon() {\n        return DocumentTextIcon;\n      },\n      get BriefcaseIcon() {\n        return BriefcaseIcon;\n      },\n      get StarIcon() {\n        return StarIcon;\n      },\n      get CameraIcon() {\n        return CameraIcon;\n      },\n      get PhotoIcon() {\n        return PhotoIcon;\n      },\n      get FolderIcon() {\n        return FolderIcon;\n      },\n      get Cog6ToothIcon() {\n        return Cog6ToothIcon;\n      },\n      get IdentificationIcon() {\n        return IdentificationIcon;\n      },\n      get HomeIcon() {\n        return HomeIcon;\n      },\n      get MapPinIcon() {\n        return MapPinIcon;\n      },\n      get QuoteIcon() {\n        return QuoteIcon;\n      },\n      get PencilSquareIcon() {\n        return PencilSquareIcon;\n      },\n      get profile() {\n        return profile;\n      },\n      get eventBus() {\n        return eventBus;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","ElMessage","useStore","MEDIA_URL","defaultAvatarImage","defaultBackground","showToast","UserIcon","UserCircleIcon","DocumentTextIcon","BriefcaseIcon","StarIcon","CameraIcon","PhotoIcon","FolderIcon","Cog6ToothIcon","IdentificationIcon","HomeIcon","MapPinIcon","QuoteIcon","PencilSquareIcon","profile","eventBus","router","store","userInfo","nickname","avatar","coverImage","gender","isVip","bio","location","jobTitle","fetchUserInfo","response","getData","data","code","value","user","username","basic_info","name","formatImageUrl","background","is_vip","personal_summary","job_intention","job_title","error","console","handleAvatarUpload","event","file","target","files","validateFile","formData","FormData","append","uploadAvatar","avatarUrl","emit","Error","message","handleCoverUpload","uploadBackground","backgroundUrl","commit","url","startsWith","type","maxSize","size","handleImageError","e","src","handleBackgroundError","profileActions","title","icon","action","push","count","contentActions","settingsActions","statistics","label"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/user/MyProfile/components/UserHome.vue"],"sourcesContent":["<template>\n  <div class=\"user-home\">\n    <!-- 顶部背景和用户信息 -->\n    <div class=\"relative\">\n      <!-- 背景图 -->\n      <div class=\"h-48 bg-gradient-to-r relative group\">\n        <img\n          :src=\"userInfo.coverImage || defaultBackground\"\n          class=\"w-full h-full object-cover rounded-lg\"\n          alt=\"背景图\"\n          @error=\"handleBackgroundError\"\n        />\n        <!-- 背景图上传按钮 -->\n        <div class=\"absolute inset-0 flex items-center justify-center bg-black bg-opacity-30 opacity-0 group-hover:opacity-100 transition-opacity\">\n          <label class=\"cursor-pointer bg-white bg-opacity-90 rounded-full p-2 hover:bg-opacity-100\">\n            <CameraIcon class=\"w-6 h-6 text-gray-600\" />\n            <input \n              type=\"file\" \n              class=\"hidden\" \n              accept=\"image/*\"\n              @change=\"handleCoverUpload\"\n            />\n          </label>\n        </div>\n      </div>\n      \n      <!-- 用户信息卡片 -->\n      <div class=\"absolute -bottom-40 left-0 right-0 px-4\">\n        <div class=\"bg-white rounded-lg shadow-md p-4 pb-6\">\n          <div class=\"flex items-center\">\n            <!-- 头像上传区域 -->\n            <div class=\"relative group\">\n              <img\n                :src=\"userInfo.avatar || defaultAvatarImage\"\n                class=\"w-20 h-20 rounded-full border-4 border-white object-cover\"\n                alt=\"头像\"\n                @error=\"handleImageError\"\n              />\n              <div class=\"absolute inset-0 flex items-center justify-center bg-black bg-opacity-30 rounded-full opacity-0 group-hover:opacity-100 transition-opacity\">\n                <label class=\"cursor-pointer\">\n                  <CameraIcon class=\"w-6 h-6 text-white\" />\n                  <input \n                    type=\"file\" \n                    class=\"hidden\" \n                    accept=\"image/*\"\n                    @change=\"handleAvatarUpload\"\n                  />\n                </label>\n              </div>\n              <span \n                v-if=\"userInfo.gender\" \n                class=\"absolute bottom-0 right-0 w-6 h-6 bg-white rounded-full flex items-center justify-center\"\n              >\n                <!-- 男性图标 -->\n                <svg \n                  v-if=\"userInfo.gender === 'male'\" \n                  class=\"w-4 h-4 text-blue-500\" \n                  viewBox=\"0 0 24 24\" \n                  fill=\"none\" \n                  stroke=\"currentColor\"\n                >\n                  <circle cx=\"12\" cy=\"12\" r=\"5\" stroke-width=\"2\"/>\n                  <path d=\"M16 8L21 3\" stroke-width=\"2\" stroke-linecap=\"round\"/>\n                  <path d=\"M21 3H16M21 3V8\" stroke-width=\"2\" stroke-linecap=\"round\"/>\n                </svg>\n                <!-- 女性图标 -->\n                <svg \n                  v-else-if=\"userInfo.gender === 'female'\" \n                  class=\"w-4 h-4 text-pink-500\" \n                  viewBox=\"0 0 24 24\" \n                  fill=\"none\" \n                  stroke=\"currentColor\"\n                >\n                  <circle cx=\"12\" cy=\"10\" r=\"5\" stroke-width=\"2\"/>\n                  <path d=\"M12 15V21\" stroke-width=\"2\" stroke-linecap=\"round\"/>\n                  <path d=\"M9 18H15\" stroke-width=\"2\" stroke-linecap=\"round\"/>\n                </svg>\n              </span>\n            </div>\n            \n            <!-- 用户基本信息 -->\n            <div class=\"ml-4 flex-1\">\n              <div class=\"flex items-center space-x-4\">\n                <h2 class=\"text-xl font-bold text-gray-900\">{{ userInfo.nickname }}</h2>\n                <span v-if=\"userInfo.isVip\" class=\"ml-2 px-2 py-0.5 bg-yellow-100 text-yellow-800 text-xs rounded-full\">\n                  VIP会员\n                </span>\n                <div class=\"flex items-center text-sm text-gray-500\">\n                  <span class=\"flex items-center\">\n                    <MapPinIcon class=\"w-4 h-4 mr-1\" />\n                    {{ userInfo.location || '未设置地区' }}\n                  </span>\n                  <span class=\"mx-2\">·</span>\n                  <span>{{ userInfo.jobTitle || '未设置职位' }}</span>\n                </div>\n              </div>\n              <!-- 个人简介 -->\n              <div class=\"mt-3 bg-gray-50 rounded-lg p-3\">\n                <div class=\"flex items-start\">\n                  <QuoteIcon class=\"w-4 h-4 text-gray-400 mt-1 flex-shrink-0\" />\n                  <div class=\"ml-2 flex-1\">\n                    <p v-if=\"userInfo.bio\" class=\"text-gray-600 text-sm line-clamp-2\">\n                      {{ userInfo.bio }}\n                    </p>\n                    <div v-else class=\"flex flex-col items-center py-2 text-gray-400\">\n                      <PencilSquareIcon class=\"w-5 h-5 mb-1\" />\n                      <p class=\"text-sm\">添加个人简介让别人更了解你（100-300字）</p>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 数据统计 -->\n          <div class=\"grid grid-cols-4 gap-4 mt-6 pt-6 border-t border-gray-100\">\n            <div v-for=\"(stat, index) in statistics\" :key=\"index\" class=\"text-center\">\n              <div class=\"text-xl font-bold text-indigo-600\">{{ stat.value }}</div>\n              <div class=\"text-sm text-gray-500\">{{ stat.label }}</div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 功能入口区域 -->\n    <div class=\"mt-56 px-4 space-y-6\">\n      <!-- 档案管理 -->\n      <div class=\"bg-white rounded-lg shadow-sm p-6\">\n        <h3 class=\"text-lg font-medium text-gray-900 mb-4 flex items-center\">\n          <DocumentTextIcon class=\"w-5 h-5 mr-2 text-indigo-600\" />\n          档案管理\n        </h3>\n        <div class=\"grid grid-cols-4 gap-4\">\n          <div \n            v-for=\"item in profileActions\" \n            :key=\"item.title\"\n            class=\"flex flex-col items-center p-4 rounded-lg hover:bg-gray-50 transition-colors cursor-pointer\"\n            @click=\"item.action\"\n          >\n            <component\n              :is=\"item.icon\"\n              class=\"w-8 h-8 text-indigo-600\"\n            />\n            <span class=\"mt-2 text-sm text-gray-600\">{{ item.title }}</span>\n          </div>\n        </div>\n      </div>\n\n      <!-- 内容管理 -->\n      <div class=\"bg-white rounded-lg shadow-sm p-6\">\n        <h3 class=\"text-lg font-medium text-gray-900 mb-4 flex items-center\">\n          <FolderIcon class=\"w-5 h-5 mr-2 text-indigo-600\" />\n          内容管理\n        </h3>\n        <div class=\"grid grid-cols-4 gap-4\">\n          <div \n            v-for=\"item in contentActions\" \n            :key=\"item.title\"\n            class=\"flex flex-col items-center p-4 rounded-lg hover:bg-gray-50 transition-colors cursor-pointer\"\n            @click=\"item.action\"\n          >\n            <component\n              :is=\"item.icon\"\n              class=\"w-8 h-8 text-indigo-600\"\n            />\n            <span class=\"mt-2 text-sm text-gray-600\">{{ item.title }}</span>\n            <span v-if=\"item.count\" class=\"mt-1 text-xs text-gray-400\">{{ item.count }}</span>\n          </div>\n        </div>\n      </div>\n      \n      <!-- 系统设置 -->\n      <div class=\"bg-white rounded-lg shadow-sm p-6\">\n        <h3 class=\"text-lg font-medium text-gray-900 mb-4 flex items-center\">\n          <Cog6ToothIcon class=\"w-5 h-5 mr-2 text-indigo-600\" />\n          系统设置\n        </h3>\n        <div class=\"grid grid-cols-4 gap-4\">\n          <div \n            v-for=\"item in settingsActions\" \n            :key=\"item.title\"\n            class=\"flex flex-col items-center p-4 rounded-lg hover:bg-gray-50 transition-colors cursor-pointer\"\n            @click=\"item.action\"\n          >\n            <component\n              :is=\"item.icon\"\n              class=\"w-8 h-8 text-indigo-600\"\n            />\n            <span class=\"mt-2 text-sm text-gray-600\">{{ item.title }}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { ElMessage } from 'element-plus'\nimport { useStore } from 'vuex'\nimport { MEDIA_URL } from '@/config'\nimport defaultAvatarImage from '@/assets/images/default-avatar.png'\nimport defaultBackground from '@/assets/images/default-background.jpg'\nimport { showToast } from '@/components/ToastMessage'\nimport {\n  UserIcon,\n  UserCircleIcon,\n  DocumentTextIcon,\n  BriefcaseIcon,\n  StarIcon,\n  CameraIcon,\n  PhotoIcon,\n  FolderIcon,\n  Cog6ToothIcon,\n  IdentificationIcon,\n  HomeIcon,\n  MapPinIcon,\n  QuoteIcon,\n  PencilSquareIcon\n} from '@heroicons/vue/24/outline'\nimport profile from '@/api/profile'\nimport { eventBus } from '@/utils/eventBus'\n\nconst router = useRouter()\nconst store = useStore()\n\n// 用户信息状态\nconst userInfo = ref({\n  nickname: '',\n  avatar: '',\n  coverImage: '',\n  gender: '',\n  isVip: false,\n  bio: '',\n  location: '',\n  jobTitle: ''\n})\n\n// 获取用户基本信息\nconst fetchUserInfo = async () => {\n  try {\n    const response = await profile.getData()\n    if (response.data?.code === 200 && response.data?.data) {\n      const data = response.data.data\n      userInfo.value = {\n        nickname: data.user?.username || data.basic_info?.name || '',\n        avatar: formatImageUrl(data.basic_info?.avatar),\n        coverImage: formatImageUrl(data.basic_info?.background),\n        gender: data.basic_info?.gender,\n        isVip: data.is_vip,\n        bio: data.basic_info?.personal_summary,\n        location: data.basic_info?.location,\n        jobTitle: data.job_intention?.job_title\n      }\n    }\n  } catch (error) {\n    console.error('获取用户信息失败:', error)\n    showToast('获取用户信息失败', 'error')\n  }\n}\n\n// 处理头像上传\nconst handleAvatarUpload = async (event) => {\n  const file = event.target.files[0]\n  if (!file) return\n  \n  if (!validateFile(file)) {\n    return\n  }\n\n  try {\n    const formData = new FormData()\n    formData.append('avatar', file)\n    \n    const response = await profile.uploadAvatar(formData)\n    if (response.data?.code === 200) {\n      showToast('头像上传成功', 'success')\n      const avatarUrl = response.data.data.avatar\n      userInfo.value.avatar = formatImageUrl(avatarUrl)\n      eventBus.emit('avatar-updated', avatarUrl)\n    } else {\n      throw new Error(response.data?.message || '上传失败')\n    }\n  } catch (error) {\n    console.error('头像上传失败:', error)\n    showToast(error.message || '头像上传失败，请稍后重试', 'error')\n  }\n}\n\n// 处理背景图上传\nconst handleCoverUpload = async (event) => {\n  const file = event.target.files[0]\n  if (!file) return\n  \n  if (!validateFile(file)) {\n    return\n  }\n  \n  const formData = new FormData()\n  formData.append('background', file)\n  \n  try {\n    const response = await profile.uploadBackground(formData)\n    if (response.data?.code === 200) {\n      showToast('背景图更新成功', 'success')\n      const backgroundUrl = response.data.data.background\n      userInfo.value.coverImage = formatImageUrl(backgroundUrl)\n      store.commit('UPDATE_BACKGROUND', backgroundUrl)\n    }\n  } catch (error) {\n    console.error('背景图更新失败:', error)\n    showToast('背景图更新失败', 'error')\n  } finally {\n    event.target.value = ''\n  }\n}\n\n// 工具函数\nconst formatImageUrl = (url) => {\n  if (!url) return ''\n  return url.startsWith('http') ? url : `${MEDIA_URL}${url}`\n}\n\nconst validateFile = (file) => {\n  // 验证文件类型\n  if (!file.type.startsWith('image/')) {\n    showToast('请选择图片文件', 'error')\n    return false\n  }\n\n  // 验证文件大小（限制为 2MB）\n  const maxSize = 2 * 1024 * 1024\n  if (file.size > maxSize) {\n    showToast('图片大小不能超过 2MB', 'error')\n    return false\n  }\n\n  return true\n}\n\n// 错误处理函数\nconst handleImageError = (e) => {\n  e.target.src = defaultAvatarImage\n}\n\nconst handleBackgroundError = (e) => {\n  e.target.src = defaultBackground\n}\n\n// 在组件挂载时获取用户信息\nonMounted(async () => {\n  await fetchUserInfo()\n})\n\n// 档案管理模块\nconst profileActions = [\n  {\n    title: '我的档案',\n    icon: IdentificationIcon,\n    action: () => router.push('/user/center?tab=profile')\n  },\n  {\n    title: '我的简历',\n    icon: DocumentTextIcon,\n    action: () => router.push('/user/center?tab=resumes'),\n    count: '2份'\n  }\n]\n\n// 内容管理模块\nconst contentActions = [\n  {\n    title: '作品集',\n    icon: PhotoIcon,\n    action: () => router.push('/user/center?tab=portfolio'),\n    count: '8个'\n  },\n  {\n    title: '收藏夹',\n    icon: StarIcon,\n    action: () => router.push('/user/center?tab=favorites'),\n    count: '16个'\n  },\n  {\n    title: '我的创作',\n    icon: BriefcaseIcon,\n    action: () => router.push('/user/center?tab=creations'),\n    count: '5篇'\n  },\n  {\n    title: '我的主页',\n    icon: HomeIcon,\n    action: () => router.push('/user/center?tab=homepage')\n  }\n]\n\n// 系统设置模块\nconst settingsActions = [\n  {\n    title: '账户设置',\n    icon: Cog6ToothIcon,\n    action: () => router.push('/user/center?tab=account')\n  }\n]\n\n// 统计数据\nconst statistics = [\n  { label: '简历数', value: '2' },\n  { label: '作品', value: '8' },\n  { label: '收藏', value: '16' },\n  { label: '获赞', value: '32' }\n]\n</script>\n\n<style scoped>\n.user-home {\n  min-height: calc(100vh - 64px);\n  background-color: #f9fafb;\n}\n</style> "],"mappings":"AAsMA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAI;AACnC,SAASC,SAAS,QAAQ,YAAW;AACrC,SAASC,SAAS,QAAQ,cAAa;AACvC,SAASC,QAAQ,QAAQ,MAAK;AAC9B,SAASC,SAAS,QAAQ,UAAS;AACnC,OAAOC,kBAAkB,MAAM,oCAAmC;AAClE,OAAOC,iBAAiB,MAAM,wCAAuC;AACrE,SAASC,SAAS,QAAQ,2BAA0B;AACpD,SACEC,QAAQ,EACRC,cAAc,EACdC,gBAAgB,EAChBC,aAAa,EACbC,QAAQ,EACRC,UAAU,EACVC,SAAS,EACTC,UAAU,EACVC,aAAa,EACbC,kBAAkB,EAClBC,QAAQ,EACRC,UAAU,EACVC,SAAS,EACTC,gBAAe,QACV,2BAA0B;AACjC,OAAOC,OAAO,MAAM,eAAc;AAClC,SAASC,QAAQ,QAAQ,kBAAiB;;;;;;;IAE1C,MAAMC,MAAM,GAAGvB,SAAS,CAAC;IACzB,MAAMwB,KAAK,GAAGtB,QAAQ,CAAC;;IAEvB;IACA,MAAMuB,QAAQ,GAAG3B,GAAG,CAAC;MACnB4B,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,KAAK;MACZC,GAAG,EAAE,EAAE;MACPC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;;IAED;IACA,MAAMC,aAAa,GAAG,MAAAA,CAAA,KAAY;MAChC,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMd,OAAO,CAACe,OAAO,CAAC;QACvC,IAAID,QAAQ,CAACE,IAAI,EAAEC,IAAI,KAAK,GAAG,IAAIH,QAAQ,CAACE,IAAI,EAAEA,IAAI,EAAE;UACtD,MAAMA,IAAI,GAAGF,QAAQ,CAACE,IAAI,CAACA,IAAG;UAC9BZ,QAAQ,CAACc,KAAK,GAAG;YACfb,QAAQ,EAAEW,IAAI,CAACG,IAAI,EAAEC,QAAQ,IAAIJ,IAAI,CAACK,UAAU,EAAEC,IAAI,IAAI,EAAE;YAC5DhB,MAAM,EAAEiB,cAAc,CAACP,IAAI,CAACK,UAAU,EAAEf,MAAM,CAAC;YAC/CC,UAAU,EAAEgB,cAAc,CAACP,IAAI,CAACK,UAAU,EAAEG,UAAU,CAAC;YACvDhB,MAAM,EAAEQ,IAAI,CAACK,UAAU,EAAEb,MAAM;YAC/BC,KAAK,EAAEO,IAAI,CAACS,MAAM;YAClBf,GAAG,EAAEM,IAAI,CAACK,UAAU,EAAEK,gBAAgB;YACtCf,QAAQ,EAAEK,IAAI,CAACK,UAAU,EAAEV,QAAQ;YACnCC,QAAQ,EAAEI,IAAI,CAACW,aAAa,EAAEC;UAChC;QACF;MA