{"ast":null,"code":"import { ref } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { HeartIcon, ChatBubbleLeftIcon, EyeIcon, ChevronRightIcon, BookmarkIcon, ShareIcon, LinkIcon, QrCodeIcon } from '@heroicons/vue/24/outline';\nimport { showToast } from '@/components/ToastMessage';\nexport default {\n  __name: 'TopicDetail',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n\n    // 话题数据\n    const topic = ref({\n      id: 1,\n      title: '2024年前端开发趋势讨论',\n      category: '技术讨论',\n      isHot: true,\n      isTop: false,\n      content: `大家好，想和大家讨论一下 2024 年前端开发的趋势。\n\n我觉得以下几个方向值得关注：\n\n1. AI 辅助开发\n2. WebAssembly 应用\n3. 微前端架构\n4. 边缘计算\n\n大家觉得还有哪些值得关注的方向？欢迎讨论！`,\n      author: {\n        name: '张三',\n        avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=author',\n        title: '高级前端工程师',\n        bio: '热爱技术，专注前端开发',\n        topics: 23,\n        followers: 156\n      },\n      createTime: '2024-03-15',\n      views: 1234,\n      replies: 23,\n      likes: 45,\n      tags: ['前端开发', '技术趋势', 'AI']\n    });\n\n    // 回复相关\n    const replyContent = ref('');\n    const replies = ref([{\n      id: 1,\n      author: {\n        name: '李四',\n        avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=user1',\n        isOP: false\n      },\n      content: '我觉得 AI 辅助开发确实是一个很重要的方向，特别是在提高开发效率方面。',\n      time: '2小时前',\n      likes: 5,\n      isLiked: false\n    }, {\n      id: 2,\n      author: {\n        name: '张三',\n        avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=author',\n        isOP: true\n      },\n      content: '确实，现在已经有很多 AI 工具可以帮助我们提高开发效率。',\n      time: '1小时前',\n      likes: 3,\n      isLiked: false\n    }]);\n\n    // 点赞和收藏状态\n    const isLiked = ref(false);\n    const isCollected = ref(false);\n\n    // 分享选项\n    const shareOptions = [{\n      icon: LinkIcon,\n      action: () => {\n        navigator.clipboard.writeText(window.location.href);\n        showToast('链接已复制', 'success');\n      }\n    }, {\n      icon: ShareIcon,\n      action: () => {\n        showToast('分享功能开发中', 'info');\n      }\n    }, {\n      icon: QrCodeIcon,\n      action: () => {\n        showToast('二维码分享功能开发中', 'info');\n      }\n    }];\n\n    // 相关话题\n    const relatedTopics = ref([{\n      id: 1,\n      title: '如何看待 AI 对前端开发的影响？',\n      replies: 45,\n      createTime: '2天前'\n    }, {\n      id: 2,\n      title: 'WebAssembly 在前端的实践经验分享',\n      replies: 32,\n      createTime: '3天前'\n    }, {\n      id: 3,\n      title: '微前端架构的优势与挑战',\n      replies: 28,\n      createTime: '4天前'\n    }]);\n\n    // 热门标签\n    const hotTags = ref([{\n      name: '前端开发',\n      count: 234\n    }, {\n      name: 'AI',\n      count: 189\n    }, {\n      name: 'WebAssembly',\n      count: 156\n    }, {\n      name: '微前端',\n      count: 145\n    }, {\n      name: '技术趋势',\n      count: 134\n    }]);\n\n    // 点赞话题\n    const toggleLike = () => {\n      if (!isLiked.value) {\n        topic.value.likes = (topic.value.likes || 0) + 1;\n      } else {\n        topic.value.likes--;\n      }\n      isLiked.value = !isLiked.value;\n    };\n\n    // 收藏话题\n    const handleCollect = () => {\n      isCollected.value = !isCollected.value;\n      showToast(isCollected.value ? '收藏成功' : '已取消收藏', 'success');\n    };\n\n    // 点赞回复\n    const toggleReplyLike = reply => {\n      if (!reply.isLiked) {\n        reply.likes++;\n      } else {\n        reply.likes--;\n      }\n      reply.isLiked = !reply.isLiked;\n    };\n\n    // 回复用户\n    const replyToUser = reply => {\n      replyContent.value = `@${reply.author.name} `;\n    };\n\n    // 提交回复\n    const submitReply = () => {\n      if (!replyContent.value.trim()) {\n        showToast('请输入回复内容', 'warning');\n        return;\n      }\n      replies.value.push({\n        id: Date.now(),\n        author: {\n          name: '当前用户',\n          avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=current',\n          isOP: false\n        },\n        content: replyContent.value,\n        time: '刚刚',\n        likes: 0,\n        isLiked: false\n      });\n      topic.value.replies++;\n      replyContent.value = '';\n      showToast('回复发表成功', 'success');\n    };\n    const __returned__ = {\n      route,\n      topic,\n      replyContent,\n      replies,\n      isLiked,\n      isCollected,\n      shareOptions,\n      relatedTopics,\n      hotTags,\n      toggleLike,\n      handleCollect,\n      toggleReplyLike,\n      replyToUser,\n      submitReply,\n      ref,\n      get useRoute() {\n        return useRoute;\n      },\n      get HeartIcon() {\n        return HeartIcon;\n      },\n      get ChatBubbleLeftIcon() {\n        return ChatBubbleLeftIcon;\n      },\n      get EyeIcon() {\n        return EyeIcon;\n      },\n      get ChevronRightIcon() {\n        return ChevronRightIcon;\n      },\n      get BookmarkIcon() {\n        return BookmarkIcon;\n      },\n      get ShareIcon() {\n        return ShareIcon;\n      },\n      get LinkIcon() {\n        return LinkIcon;\n      },\n      get QrCodeIcon() {\n        return QrCodeIcon;\n      },\n      get showToast() {\n        return showToast;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","useRoute","HeartIcon","ChatBubbleLeftIcon","EyeIcon","ChevronRightIcon","BookmarkIcon","ShareIcon","LinkIcon","QrCodeIcon","showToast","route","topic","id","title","category","isHot","isTop","content","author","name","avatar","bio","topics","followers","createTime","views","replies","likes","tags","replyContent","isOP","time","isLiked","isCollected","shareOptions","icon","action","navigator","clipboard","writeText","window","location","href","relatedTopics","hotTags","count","toggleLike","value","handleCollect","toggleReplyLike","reply","replyToUser","submitReply","trim","push","Date","now"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/community/TopicDetail.vue"],"sourcesContent":["<template>\n  <div class=\"py-4 pb-24 lg:py-6 mt-[28px] md:mt-[72px]\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 面包屑导航 -->\n      <nav class=\"flex items-center space-x-2 text-sm mb-6\" aria-label=\"Breadcrumb\">\n        <router-link to=\"/community\" class=\"text-gray-500 hover:text-gray-700\">\n          社区\n        </router-link>\n        <ChevronRightIcon class=\"w-4 h-4 text-gray-400\" />\n        <router-link to=\"/community/topics\" class=\"text-gray-500 hover:text-gray-700\">\n          话题\n        </router-link>\n        <ChevronRightIcon class=\"w-4 h-4 text-gray-400\" />\n        <span class=\"text-gray-900 font-medium truncate max-w-[200px]\" :title=\"topic.title\">\n          {{ topic.title }}\n        </span>\n      </nav>\n\n      <!-- 内容区域两栏布局 -->\n      <div class=\"flex flex-col lg:flex-row gap-4 lg:gap-6\">\n        <!-- 左侧主要内容 -->\n        <div class=\"lg:flex-1 lg:max-w-[calc(100%-320px-24px)]\">\n          <!-- 话题内容卡片 -->\n          <div class=\"bg-white rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <!-- 话题头部 -->\n            <header class=\"mb-6\">\n              <div class=\"flex items-center gap-3 mb-4\">\n                <span class=\"px-2.5 py-1 bg-violet-100 text-violet-600 rounded-full text-xs font-medium\">\n                  {{ topic.category }}\n                </span>\n                <span v-if=\"topic.isHot\" class=\"px-2.5 py-1 bg-orange-100 text-orange-600 rounded-full text-xs font-medium\">\n                  热门\n                </span>\n                <span v-if=\"topic.isTop\" class=\"px-2.5 py-1 bg-blue-100 text-blue-600 rounded-full text-xs font-medium\">\n                  置顶\n                </span>\n              </div>\n              <h1 class=\"text-2xl lg:text-3xl font-bold text-gray-900 mb-4\">\n                {{ topic.title }}\n              </h1>\n              \n              <!-- 作者信息和话题数据 -->\n              <div class=\"flex flex-wrap items-center gap-4\">\n                <div class=\"flex items-center\">\n                  <img \n                    :src=\"topic.author.avatar\" \n                    :alt=\"topic.author.name\"\n                    class=\"w-10 h-10 rounded-full mr-3\"\n                  >\n                  <div>\n                    <div class=\"font-medium text-gray-900\">{{ topic.author.name }}</div>\n                    <div class=\"text-sm text-gray-500\">{{ topic.createTime }}</div>\n                  </div>\n                </div>\n\n                <div class=\"hidden sm:block w-px h-8 bg-gray-200\"></div>\n                \n                <div class=\"flex items-center gap-4 text-sm text-gray-500\">\n                  <span class=\"flex items-center\">\n                    <EyeIcon class=\"w-4 h-4 mr-1\" />\n                    {{ topic.views }} 浏览\n                  </span>\n                  <span class=\"flex items-center\">\n                    <ChatBubbleLeftIcon class=\"w-4 h-4 mr-1\" />\n                    {{ topic.replies }} 回复\n                  </span>\n                  <span class=\"flex items-center\">\n                    <HeartIcon class=\"w-4 h-4 mr-1\" />\n                    {{ topic.likes }} 点赞\n                  </span>\n                </div>\n              </div>\n            </header>\n\n            <!-- 话题内容 -->\n            <div class=\"vs-container mb-8\">\n              <div class=\"vs-header\">\n                <div class=\"vs-tabs\">\n                  <div class=\"vs-tab active\">\n                    <span class=\"vs-filename\">讨论内容</span>\n                  </div>\n                </div>\n                <div class=\"vs-actions\">\n                  <button class=\"vs-action-btn\">\n                    <svg class=\"w-3.5 h-3.5\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\">\n                      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M19 9l-7 7-7-7\" />\n                    </svg>\n                  </button>\n                  <button class=\"vs-action-btn\">\n                    <svg class=\"w-3.5 h-3.5\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\">\n                      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M6 18L18 6M6 6l12 12\" />\n                    </svg>\n                  </button>\n                </div>\n              </div>\n              <div class=\"vs-content\">\n                <div class=\"vs-line-numbers\">\n                  {/* 生成行号 */}\n                  <template v-for=\"n in topic.content.split('\\n').length\" :key=\"n\">\n                    <div class=\"vs-line-number\">{{ n }}</div>\n                  </template>\n                </div>\n                <div class=\"vs-text\">{{ topic.content }}</div>\n              </div>\n            </div>\n\n            <!-- 话题标签 -->\n            <div class=\"flex items-center gap-2 mb-6 flex-wrap\">\n              <span \n                v-for=\"tag in topic.tags\" \n                :key=\"tag\"\n                class=\"px-3 py-1 bg-gray-100 text-gray-600 rounded-full text-sm hover:bg-gray-200 cursor-pointer transition-colors\"\n              >\n                {{ tag }}\n              </span>\n            </div>\n\n            <!-- 操作按钮 -->\n            <div class=\"flex items-center justify-between py-4 border-t border-gray-100\">\n              <div class=\"flex items-center gap-2\">\n                <button \n                  class=\"flex items-center px-4 py-2 rounded-lg transition-colors\"\n                  :class=\"isLiked ? 'text-red-500 bg-red-50' : 'text-gray-600 hover:bg-gray-50'\"\n                  @click=\"toggleLike\"\n                >\n                  <HeartIcon class=\"w-5 h-5 mr-2\" :class=\"{ 'fill-current': isLiked }\" />\n                  {{ isLiked ? '已点赞' : '点赞' }}\n                </button>\n                <button \n                  class=\"flex items-center px-4 py-2 text-gray-600 hover:bg-gray-50 rounded-lg transition-colors\"\n                  @click=\"handleCollect\"\n                >\n                  <BookmarkIcon class=\"w-5 h-5 mr-2\" :class=\"{ 'fill-current': isCollected }\" />\n                  {{ isCollected ? '已收藏' : '收藏' }}\n                </button>\n              </div>\n              \n              <div class=\"flex items-center gap-2\">\n                <button \n                  v-for=\"(item, index) in shareOptions\" \n                  :key=\"index\"\n                  class=\"p-2 text-gray-400 hover:text-gray-600 rounded-lg hover:bg-gray-50\"\n                  @click=\"item.action\"\n                >\n                  <component :is=\"item.icon\" class=\"w-5 h-5\" />\n                </button>\n              </div>\n            </div>\n          </div>\n\n          <!-- 回复列表 -->\n          <div class=\"mt-6 bg-white rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <h2 class=\"text-lg font-bold text-gray-900 mb-4\">\n              回复 ({{ topic.replies }})\n            </h2>\n\n            <!-- 回复输入框 -->\n            <div class=\"mb-6\">\n              <textarea\n                v-model=\"replyContent\"\n                rows=\"3\"\n                class=\"block w-full rounded-lg border-gray-300 shadow-sm focus:border-violet-500 focus:ring-violet-500\"\n                placeholder=\"写下你的回复...\"\n              ></textarea>\n              <div class=\"mt-2 flex justify-end\">\n                <button \n                  class=\"px-4 py-2 bg-violet-600 text-white rounded-lg hover:bg-violet-700 transition-colors\"\n                  @click=\"submitReply\"\n                >\n                  发表回复\n                </button>\n              </div>\n            </div>\n\n            <!-- 回复列表 -->\n            <div class=\"space-y-6\">\n              <div \n                v-for=\"reply in replies\" \n                :key=\"reply.id\"\n                class=\"flex gap-4\"\n              >\n                <img \n                  :src=\"reply.author.avatar\"\n                  :alt=\"reply.author.name\"\n                  class=\"w-10 h-10 rounded-full flex-shrink-0\"\n                >\n                <div class=\"flex-1\">\n                  <div class=\"bg-gray-50 rounded-lg p-4\">\n                    <div class=\"flex items-center justify-between mb-2\">\n                      <div class=\"flex items-center gap-2\">\n                        <span class=\"font-medium text-gray-900\">{{ reply.author.name }}</span>\n                        <span v-if=\"reply.author.isOP\" class=\"px-1.5 py-0.5 bg-violet-100 text-violet-600 rounded text-xs\">\n                          楼主\n                        </span>\n                      </div>\n                      <span class=\"text-sm text-gray-500\">{{ reply.time }}</span>\n                    </div>\n                    <p class=\"text-gray-600\">{{ reply.content }}</p>\n                  </div>\n                  <div class=\"flex items-center gap-4 mt-2 text-sm\">\n                    <button \n                      class=\"text-gray-500 hover:text-gray-700\"\n                      @click=\"replyToUser(reply)\"\n                    >\n                      回复\n                    </button>\n                    <button \n                      class=\"flex items-center text-gray-500 hover:text-gray-700\"\n                      @click=\"toggleReplyLike(reply)\"\n                    >\n                      <HeartIcon \n                        class=\"w-4 h-4 mr-1\"\n                        :class=\"{ 'fill-current text-red-500': reply.isLiked }\"\n                      />\n                      {{ reply.likes }}\n                    </button>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 右侧边栏 -->\n        <div class=\"lg:w-[320px] space-y-6\">\n          <!-- 作者信息卡片 -->\n          <div class=\"bg-white rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <div class=\"flex items-center space-x-4 mb-4\">\n              <img \n                :src=\"topic.author.avatar\" \n                :alt=\"topic.author.name\"\n                class=\"w-12 h-12 rounded-full\"\n              >\n              <div>\n                <div class=\"font-medium text-gray-900\">{{ topic.author.name }}</div>\n                <div class=\"text-sm text-gray-500\">{{ topic.author.title || '社区成员' }}</div>\n              </div>\n            </div>\n            <p class=\"text-sm text-gray-600 mb-4\">{{ topic.author.bio || '这个作者很懒，还没有写简介' }}</p>\n            <div class=\"flex items-center justify-between text-sm text-gray-500\">\n              <div>话题 {{ topic.author.topics || 0 }}</div>\n              <div>关注者 {{ topic.author.followers || 0 }}</div>\n            </div>\n            <button \n              class=\"mt-4 w-full h-9 rounded-lg border border-violet-600 text-violet-600 hover:bg-violet-50 transition-colors text-sm font-medium\"\n            >\n              关注作者\n            </button>\n          </div>\n\n          <!-- 相关话题 -->\n          <div class=\"bg-white rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <h2 class=\"text-lg font-bold text-gray-900 mb-4\">相关话题</h2>\n            <div class=\"space-y-4\">\n              <div v-for=\"topic in relatedTopics\" :key=\"topic.id\"\n                class=\"group cursor-pointer\"\n              >\n                <h3 class=\"text-sm font-medium text-gray-900 group-hover:text-violet-600 transition-colors line-clamp-2 mb-2\">\n                  {{ topic.title }}\n                </h3>\n                <div class=\"flex items-center text-xs text-gray-500\">\n                  <span class=\"flex items-center\">\n                    <ChatBubbleLeftIcon class=\"w-3 h-3 mr-1\" />{{ topic.replies }}\n                  </span>\n                  <span class=\"mx-2\">·</span>\n                  <span>{{ topic.createTime }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 热门标签 -->\n          <div class=\"bg-white rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <h2 class=\"text-lg font-bold text-gray-900 mb-4\">热门标签</h2>\n            <div class=\"flex flex-wrap gap-2\">\n              <span \n                v-for=\"tag in hotTags\" \n                :key=\"tag.name\"\n                class=\"px-3 py-1 bg-gray-100 text-gray-600 rounded-full text-sm cursor-pointer hover:bg-gray-200 transition-colors\"\n              >\n                {{ tag.name }}\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from 'vue'\nimport { useRoute } from 'vue-router'\nimport { \n  HeartIcon,\n  ChatBubbleLeftIcon,\n  EyeIcon,\n  ChevronRightIcon,\n  BookmarkIcon,\n  ShareIcon,\n  LinkIcon,\n  QrCodeIcon\n} from '@heroicons/vue/24/outline'\nimport { showToast } from '@/components/ToastMessage'\n\nconst route = useRoute()\n\n// 话题数据\nconst topic = ref({\n  id: 1,\n  title: '2024年前端开发趋势讨论',\n  category: '技术讨论',\n  isHot: true,\n  isTop: false,\n  content: `大家好，想和大家讨论一下 2024 年前端开发的趋势。\n\n我觉得以下几个方向值得关注：\n\n1. AI 辅助开发\n2. WebAssembly 应用\n3. 微前端架构\n4. 边缘计算\n\n大家觉得还有哪些值得关注的方向？欢迎讨论！`,\n  author: {\n    name: '张三',\n    avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=author',\n    title: '高级前端工程师',\n    bio: '热爱技术，专注前端开发',\n    topics: 23,\n    followers: 156\n  },\n  createTime: '2024-03-15',\n  views: 1234,\n  replies: 23,\n  likes: 45,\n  tags: ['前端开发', '技术趋势', 'AI']\n})\n\n// 回复相关\nconst replyContent = ref('')\nconst replies = ref([\n  {\n    id: 1,\n    author: {\n      name: '李四',\n      avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=user1',\n      isOP: false\n    },\n    content: '我觉得 AI 辅助开发确实是一个很重要的方向，特别是在提高开发效率方面。',\n    time: '2小时前',\n    likes: 5,\n    isLiked: false\n  },\n  {\n    id: 2,\n    author: {\n      name: '张三',\n      avatar: 'https://api.dicebear.com/7.x/avataaars/svg?seed=author',\n      isOP: true\n    },\n    content: '确实，现在已经有很多 AI 工具可以帮助我们提高开发效率。',\n    time: '1小时前',\n    likes: 3,\n    isLiked: false\n  }\n])\n\n// 点赞和收藏状态\nconst isLiked = ref(false)\nconst isCollected = ref(false)\n\n// 分享选项\nconst shareOptions = [\n  {\n    icon: LinkIcon,\n    action: () => {\n      navigator.clipboard.writeText(window.location.href)\n      showToast('链接已复制', 'success')\n    }\n  },\n  {\n    icon: ShareIcon,\n    action: () => {\n      showToast('分享功能开发中', 'info')\n    }\n  },\n  {\n    icon: QrCodeIcon,\n    action: () => {\n      showToast('二维码分享功能开发中', 'info')\n    }\n  }\n]\n\n// 相关话题\nconst relatedTopics = ref([\n  {\n    id: 1,\n    title: '如何看待 AI 对前端开发的影响？',\n    replies: 45,\n    createTime: '2天前'\n  },\n  {\n    id: 2,\n    title: 'WebAssembly 在前端的实践经验分享',\n    replies: 32,\n    createTime: '3天前'\n  },\n  {\n    id: 3,\n    title: '微前端架构的优势与挑战',\n    replies: 28,\n    createTime: '4天前'\n  }\n])\n\n// 热门标签\nconst hotTags = ref([\n  { name: '前端开发', count: 234 },\n  { name: 'AI', count: 189 },\n  { name: 'WebAssembly', count: 156 },\n  { name: '微前端', count: 145 },\n  { name: '技术趋势', count: 134 }\n])\n\n// 点赞话题\nconst toggleLike = () => {\n  if (!isLiked.value) {\n    topic.value.likes = (topic.value.likes || 0) + 1\n  } else {\n    topic.value.likes--\n  }\n  isLiked.value = !isLiked.value\n}\n\n// 收藏话题\nconst handleCollect = () => {\n  isCollected.value = !isCollected.value\n  showToast(isCollected.value ? '收藏成功' : '已取消收藏', 'success')\n}\n\n// 点赞回复\nconst toggleReplyLike = (reply) => {\n  if (!reply.isLiked) {\n    reply.likes++\n  } else {\n    reply.likes--\n  }\n  reply.isLiked = !reply.isLiked\n}\n\n// 回复用户\nconst replyToUser = (reply) => {\n  replyContent.value = `@${reply.author.name} `\n}\n\n// 提交回复\nconst submitReply = () => {\n  if (!replyContent.value.trim()) {\n    showToast('请输入回复内容', 'warning')\n    return\n  }\n\n  replies.value.pus