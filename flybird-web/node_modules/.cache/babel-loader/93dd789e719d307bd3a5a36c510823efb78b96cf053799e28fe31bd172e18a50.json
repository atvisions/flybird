{"ast":null,"code":"import { ref, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { CrownIcon } from '@heroicons/vue/24/outline';\nimport { membershipBenefits, membershipPricing } from '@/config/membership';\nexport default {\n  __name: 'ProDialog',\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n    const router = useRouter();\n    const dialogVisible = ref(props.modelValue);\n    const dailyPrice = membershipPricing.daily;\n\n    // 监听 modelValue 变化\n    watch(() => props.modelValue, val => {\n      dialogVisible.value = val;\n    });\n\n    // 监听 dialogVisible 变化\n    watch(() => dialogVisible.value, val => {\n      emit('update:modelValue', val);\n    });\n    const handleClose = () => {\n      dialogVisible.value = false;\n    };\n    const handleUpgrade = () => {\n      dialogVisible.value = false;\n      router.push('/pro');\n    };\n    const __returned__ = {\n      props,\n      emit,\n      router,\n      dialogVisible,\n      dailyPrice,\n      handleClose,\n      handleUpgrade,\n      ref,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get CrownIcon() {\n        return CrownIcon;\n      },\n      get membershipBenefits() {\n        return membershipBenefits;\n      },\n      get membershipPricing() {\n        return membershipPricing;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","watch","useRouter","CrownIcon","membershipBenefits","membershipPricing","props","__props","emit","__emit","router","dialogVisible","modelValue","dailyPrice","daily","val","value","handleClose","handleUpgrade","push"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/components/ProDialog.vue"],"sourcesContent":["<template>\n  <el-dialog\n    v-model=\"dialogVisible\"\n    :show-close=\"true\"\n    width=\"380px\"\n    class=\"pro-dialog\"\n    @close=\"handleClose\"\n  >\n    <div class=\"relative p-8 text-center bg-white\">\n      <!-- 背景装饰 -->\n      <div class=\"absolute inset-0 bg-gradient-to-b from-violet-50/50 to-transparent\"></div>\n      <div class=\"absolute top-0 left-0 w-full h-32 bg-[url('@/assets/images/pro-pattern.png')] bg-repeat-x opacity-5\"></div>\n      \n      <!-- 顶部装饰 -->\n      <div class=\"absolute -top-12 left-1/2 -translate-x-1/2\">\n        <div class=\"relative\">\n          <div class=\"absolute inset-0 bg-gradient-to-r from-violet-400 to-indigo-400 blur-xl opacity-20\"></div>\n          <div class=\"relative w-24 h-24 bg-gradient-to-br from-white to-violet-50 rounded-full shadow-xl flex items-center justify-center\">\n            <div class=\"w-16 h-16 rounded-full bg-gradient-to-r from-violet-500 to-indigo-500 flex items-center justify-center transform transition-transform group-hover:scale-105\">\n              <CrownIcon class=\"w-8 h-8 text-white\" />\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 标题 -->\n      <div class=\"mt-14 mb-10 relative\">\n        <h3 class=\"text-2xl font-semibold bg-gradient-to-r from-violet-600 to-indigo-600 bg-clip-text text-transparent\">\n          解锁全部高级功能\n        </h3>\n        <div class=\"mt-2 text-sm text-gray-500\">让求职之路更加顺畅</div>\n        <div class=\"absolute -left-2 -right-2 h-px bg-gradient-to-r from-transparent via-violet-200 to-transparent mt-4\"></div>\n      </div>\n\n      <!-- 会员权益列表 -->\n      <div class=\"grid grid-cols-2 gap-4 mb-10\">\n        <div v-for=\"(benefit, index) in membershipBenefits\" :key=\"index\" \n          class=\"group relative p-4 rounded-2xl bg-gradient-to-br from-white to-violet-50/30 hover:to-violet-50 transition-all duration-300 cursor-default\"\n        >\n          <div class=\"absolute inset-0 rounded-2xl bg-gradient-to-br from-violet-500/5 to-transparent opacity-0 group-hover:opacity-100 transition-opacity duration-300\"></div>\n          <div class=\"relative\">\n            <div class=\"w-12 h-12 mx-auto mb-3 rounded-xl bg-gradient-to-br from-violet-100/50 to-indigo-100/50 flex items-center justify-center group-hover:shadow-lg group-hover:shadow-violet-100/50 transition-shadow duration-300\">\n              <component :is=\"benefit.icon\" class=\"w-6 h-6 text-violet-600\" />\n            </div>\n            <div class=\"text-center\">\n              <div class=\"text-sm font-medium text-gray-800\">{{ benefit.name }}</div>\n              <div class=\"text-xs text-gray-500 mt-1\">{{ benefit.desc }}</div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 价格信息 -->\n      <div class=\"relative inline-flex items-center px-6 py-3 rounded-2xl bg-gradient-to-r from-violet-50 to-indigo-50 mb-8 group\">\n        <div class=\"absolute inset-0 rounded-2xl bg-gradient-to-r from-violet-400/5 to-indigo-400/5 opacity-0 group-hover:opacity-100 transition-opacity duration-300\"></div>\n        <div class=\"relative flex items-center\">\n          <span class=\"text-sm text-gray-500 mr-2\">仅需</span>\n          <span class=\"text-base text-violet-600 font-medium\">¥</span>\n          <span class=\"text-4xl font-semibold bg-gradient-to-r from-violet-600 to-indigo-600 bg-clip-text text-transparent mx-1\">{{ dailyPrice }}</span>\n          <span class=\"text-sm text-gray-500\">/天</span>\n        </div>\n      </div>\n\n      <!-- 按钮 -->\n      <button\n        class=\"relative w-full h-14 group overflow-hidden\"\n        @click=\"handleUpgrade\"\n      >\n        <div class=\"absolute inset-0 bg-gradient-to-r from-violet-500 to-indigo-500 group-hover:from-violet-600 group-hover:to-indigo-600 transition-colors duration-300\"></div>\n        <div class=\"absolute inset-0 opacity-0 group-hover:opacity-100 transition-opacity duration-300\">\n          <div class=\"absolute inset-0 bg-[radial-gradient(circle_at_50%_0%,rgba(255,255,255,0.1),transparent_50%)]\"></div>\n        </div>\n        <span class=\"relative text-white text-base font-medium\">立即开通</span>\n      </button>\n    </div>\n  </el-dialog>\n</template>\n\n<script setup>\nimport { ref, defineProps, defineEmits, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { CrownIcon } from '@heroicons/vue/24/outline'\nimport { membershipBenefits, membershipPricing } from '@/config/membership'\n\nconst props = defineProps({\n  modelValue: {\n    type: Boolean,\n    default: false\n  }\n})\n\nconst emit = defineEmits(['update:modelValue'])\nconst router = useRouter()\n\nconst dialogVisible = ref(props.modelValue)\nconst dailyPrice = membershipPricing.daily\n\n// 监听 modelValue 变化\nwatch(() => props.modelValue, (val) => {\n  dialogVisible.value = val\n})\n\n// 监听 dialogVisible 变化\nwatch(() => dialogVisible.value, (val) => {\n  emit('update:modelValue', val)\n})\n\nconst handleClose = () => {\n  dialogVisible.value = false\n}\n\nconst handleUpgrade = () => {\n  dialogVisible.value = false\n  router.push('/pro')\n}\n</script>\n\n<style scoped>\n.pro-dialog :deep(.el-dialog) {\n  border: none;\n  border-radius: 24px;\n  overflow: hidden;\n  background: white;\n  box-shadow: \n    0 25px 50px -12px rgba(0, 0, 0, 0.15),\n    0 0 40px rgba(139, 92, 246, 0.15);\n}\n\n.pro-dialog :deep(.el-dialog__header) {\n  display: none;\n}\n\n.pro-dialog :deep(.el-dialog__body) {\n  padding: 0;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn) {\n  top: -14px;\n  right: -14px;\n  z-index: 10;\n  background: white;\n  border: 1px solid rgba(139, 92, 246, 0.1);\n  border-radius: 50%;\n  width: 32px;\n  height: 32px;\n  box-shadow: \n    0 4px 12px rgba(139, 92, 246, 0.1),\n    0 0 0 4px rgba(255, 255, 255, 0.7);\n  transition: all 0.3s;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn:hover) {\n  transform: scale(1.1);\n  box-shadow: \n    0 6px 16px rgba(139, 92, 246, 0.15),\n    0 0 0 6px rgba(255, 255, 255, 0.8);\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn .el-dialog__close) {\n  color: rgba(139, 92, 246, 0.6);\n  font-size: 18px;\n  transition: color 0.3s;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn:hover .el-dialog__close) {\n  color: rgba(139, 92, 246, 0.9);\n}\n\n@keyframes floatIn {\n  from {\n    opacity: 0;\n    transform: scale(0.95) translateY(20px);\n  }\n  to {\n    opacity: 1;\n    transform: scale(1) translateY(0);\n  }\n}\n\n.pro-dialog :deep(.el-dialog) {\n  animation: floatIn 0.4s cubic-bezier(0.2, 0.8, 0.2, 1);\n}\n</style> "],"mappings":"AA+EA,SAASA,GAA6B,EAAEC,KAAK,QAAQ,KAAI;AACzD,SAASC,SAAS,QAAQ,YAAW;AACrC,SAASC,SAAS,QAAQ,2BAA0B;AACpD,SAASC,kBAAkB,EAAEC,iBAAiB,QAAQ,qBAAoB;;;;;;;;;;;;;;;IAE1E,MAAMC,KAAK,GAAGC,OAAA;IAOd,MAAMC,IAAI,GAAGC,MAAA;IACb,MAAMC,MAAM,GAAGR,SAAS,CAAC;IAEzB,MAAMS,aAAa,GAAGX,GAAG,CAACM,KAAK,CAACM,UAAU;IAC1C,MAAMC,UAAU,GAAGR,iBAAiB,CAACS,KAAI;;IAEzC;IACAb,KAAK,CAAC,MAAMK,KAAK,CAACM,UAAU,EAAGG,GAAG,IAAK;MACrCJ,aAAa,CAACK,KAAK,GAAGD,GAAE;IAC1B,CAAC;;IAED;IACAd,KAAK,CAAC,MAAMU,aAAa,CAACK,KAAK,EAAGD,GAAG,IAAK;MACxCP,IAAI,CAAC,mBAAmB,EAAEO,GAAG;IAC/B,CAAC;IAED,MAAME,WAAW,GAAGA,CAAA,KAAM;MACxBN,aAAa,CAACK,KAAK,GAAG,KAAI;IAC5B;IAEA,MAAME,aAAa,GAAGA,CAAA,KAAM;MAC1BP,aAAa,CAACK,KAAK,GAAG,KAAI;MAC1BN,MAAM,CAACS,IAAI,CAAC,MAAM;IACpB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}