{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useRoute } from 'vue-router';\nimport HeadView from '@/components/HeadView.vue';\nimport FooterView from '@/components/FooterView.vue';\nimport { PaintBrushIcon, CommandLineIcon, DocumentTextIcon, HomeIcon, UserIcon, PlusIcon } from '@heroicons/vue/24/outline';\nexport default {\n  __name: 'PortfolioView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const mainTabs = [{\n      name: '发现',\n      path: '/portfolio',\n      icon: HomeIcon\n    }, {\n      name: '设计作品',\n      path: '/portfolio/design',\n      icon: PaintBrushIcon\n    }, {\n      name: '开发项目',\n      path: '/portfolio/development',\n      icon: CommandLineIcon\n    }, {\n      name: '文案策划',\n      path: '/portfolio/writing',\n      icon: DocumentTextIcon\n    }];\n    const myTab = {\n      name: '我的作品',\n      path: '/portfolio/my',\n      icon: UserIcon\n    };\n    const isCurrentRoute = path => {\n      return route.path === path;\n    };\n    const __returned__ = {\n      route,\n      mainTabs,\n      myTab,\n      isCurrentRoute,\n      ref,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      HeadView,\n      FooterView,\n      get PaintBrushIcon() {\n        return PaintBrushIcon;\n      },\n      get CommandLineIcon() {\n        return CommandLineIcon;\n      },\n      get DocumentTextIcon() {\n        return DocumentTextIcon;\n      },\n      get HomeIcon() {\n        return HomeIcon;\n      },\n      get UserIcon() {\n        return UserIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRoute","HeadView","FooterView","PaintBrushIcon","CommandLineIcon","DocumentTextIcon","HomeIcon","UserIcon","PlusIcon","route","mainTabs","name","path","icon","myTab","isCurrentRoute"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/portfolio/PortfolioView.vue"],"sourcesContent":["<template>\n  <div class=\"min-h-screen bg-gray-50\">\n    <HeadView />\n    \n    <!-- 页面头部 -->\n    <div class=\"bg-white border-b\">\n      <div class=\"max-w-7xl mx-auto\">\n        <!-- 导航区域 -->\n        <div class=\"flex items-center justify-between h-16 px-4 sm:px-6 lg:px-8\">\n          <!-- 左侧主要导航 -->\n          <div class=\"flex items-center space-x-8\">\n            <router-link\n              v-for=\"tab in mainTabs\"\n              :key=\"tab.path\"\n              :to=\"tab.path\"\n              class=\"flex items-center space-x-2 py-5 relative group\"\n              :class=\"[\n                isCurrentRoute(tab.path)\n                  ? 'text-gray-900'\n                  : 'text-gray-600 hover:text-gray-900'\n              ]\"\n            >\n              <component :is=\"tab.icon\" \n                class=\"w-5 h-5 transition-colors\"\n                :class=\"isCurrentRoute(tab.path) ? 'text-gray-900' : 'text-gray-400 group-hover:text-gray-700'\"\n              />\n              <span class=\"text-sm font-medium\">{{ tab.name }}</span>\n              \n              <!-- 活跃状态指示器 -->\n              <div \n                class=\"absolute bottom-0 left-0 right-0 h-0.5 transition-colors\"\n                :class=\"isCurrentRoute(tab.path) ? 'bg-gray-900' : 'bg-transparent group-hover:bg-gray-200'\"\n              ></div>\n            </router-link>\n          </div>\n\n          <!-- 右侧操作区 -->\n          <div class=\"flex items-center space-x-4\">\n            <button class=\"px-4 py-2 bg-gray-900 text-white rounded-lg hover:bg-gray-800 transition-colors text-sm font-medium flex items-center\">\n              <PlusIcon class=\"w-4 h-4 mr-1.5\" />\n              上传作品\n            </button>\n            <router-link\n              :to=\"myTab.path\"\n              class=\"flex items-center space-x-2 py-5 relative group\"\n              :class=\"[\n                isCurrentRoute(myTab.path)\n                  ? 'text-gray-900'\n                  : 'text-gray-600 hover:text-gray-900'\n              ]\"\n            >\n              <component :is=\"myTab.icon\" \n                class=\"w-5 h-5 transition-colors\"\n                :class=\"isCurrentRoute(myTab.path) ? 'text-gray-900' : 'text-gray-400 group-hover:text-gray-700'\"\n              />\n              <span class=\"text-sm font-medium\">{{ myTab.name }}</span>\n              \n              <!-- 活跃状态指示器 -->\n              <div \n                class=\"absolute bottom-0 left-0 right-0 h-0.5 transition-colors\"\n                :class=\"isCurrentRoute(myTab.path) ? 'bg-gray-900' : 'bg-transparent group-hover:bg-gray-200'\"\n              ></div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 内容区域 -->\n    <router-view></router-view>\n\n    <FooterView />\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport { useRoute } from 'vue-router'\nimport HeadView from '@/components/HeadView.vue'\nimport FooterView from '@/components/FooterView.vue'\nimport {\n  PaintBrushIcon,\n  CommandLineIcon,\n  DocumentTextIcon,\n  HomeIcon,\n  UserIcon,\n  PlusIcon\n} from '@heroicons/vue/24/outline'\n\nconst route = useRoute()\n\nconst mainTabs = [\n  { \n    name: '发现', \n    path: '/portfolio', \n    icon: HomeIcon \n  },\n  { \n    name: '设计作品', \n    path: '/portfolio/design', \n    icon: PaintBrushIcon \n  },\n  { \n    name: '开发项目', \n    path: '/portfolio/development', \n    icon: CommandLineIcon \n  },\n  { \n    name: '文案策划', \n    path: '/portfolio/writing', \n    icon: DocumentTextIcon \n  }\n]\n\nconst myTab = {\n  name: '我的作品',\n  path: '/portfolio/my',\n  icon: UserIcon\n}\n\nconst isCurrentRoute = (path) => {\n  return route.path === path\n}\n</script> "],"mappings":"AA4EA,SAASA,GAAG,EAAEC,QAAQ