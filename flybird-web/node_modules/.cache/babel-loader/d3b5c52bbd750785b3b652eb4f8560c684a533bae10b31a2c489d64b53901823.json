{"ast":null,"code":"import { ref, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { CrownIcon } from '@heroicons/vue/24/outline';\nimport { membershipBenefits, membershipPricing } from '@/config/membership';\nexport default {\n  __name: 'ProDialog',\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n    const router = useRouter();\n    const dialogVisible = ref(props.modelValue);\n    const dailyPrice = membershipPricing.daily;\n\n    // 监听 modelValue 变化\n    watch(() => props.modelValue, val => {\n      dialogVisible.value = val;\n    });\n\n    // 监听 dialogVisible 变化\n    watch(() => dialogVisible.value, val => {\n      emit('update:modelValue', val);\n    });\n    const handleClose = () => {\n      dialogVisible.value = false;\n    };\n    const handleUpgrade = () => {\n      dialogVisible.value = false;\n      router.push('/pro');\n    };\n    const __returned__ = {\n      props,\n      emit,\n      router,\n      dialogVisible,\n      dailyPrice,\n      handleClose,\n      handleUpgrade,\n      ref,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get CrownIcon() {\n        return CrownIcon;\n      },\n      get membershipBenefits() {\n        return membershipBenefits;\n      },\n      get membershipPricing() {\n        return membershipPricing;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","watch","useRouter","CrownIcon","membershipBenefits","membershipPricing","props","__props","emit","__emit","router","dialogVisible","modelValue","dailyPrice","daily","val","value","handleClose","handleUpgrade","push"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/components/ProDialog.vue"],"sourcesContent":["<template>\n  <el-dialog\n    v-model=\"dialogVisible\"\n    :show-close=\"true\"\n    width=\"360px\"\n    class=\"pro-dialog\"\n    @close=\"handleClose\"\n  >\n    <div class=\"relative p-8 text-center bg-gradient-to-br from-[#FCF8FF] via-white to-[#F5F7FF]\">\n      <!-- 顶部装饰 -->\n      <div class=\"absolute -top-3 left-1/2 -translate-x-1/2 w-32 h-6 bg-gradient-to-br from-[#FCF8FF] to-[#F5F7FF] rounded-b-xl\"></div>\n      <div class=\"absolute -top-6 left-1/2 -translate-x-1/2\">\n        <div class=\"w-12 h-12 bg-white rounded-full shadow-[0_0_30px_rgba(139,92,246,0.15)] flex items-center justify-center\">\n          <div class=\"w-9 h-9 rounded-full bg-gradient-to-r from-violet-500 to-indigo-500 flex items-center justify-center\">\n            <CrownIcon class=\"w-5 h-5 text-white\" />\n          </div>\n        </div>\n      </div>\n\n      <!-- 标题 -->\n      <div class=\"mt-5 mb-8\">\n        <h3 class=\"text-xl font-semibold bg-gradient-to-r from-violet-600 to-indigo-600 bg-clip-text text-transparent\">解锁全部高级功能</h3>\n        <div class=\"mt-1.5 text-sm text-violet-600/80\">让求职之路更加顺畅</div>\n      </div>\n\n      <!-- 会员权益列表 -->\n      <div class=\"grid grid-cols-2 gap-4 mb-8\">\n        <div v-for=\"(benefit, index) in membershipBenefits\" :key=\"index\" \n          class=\"flex items-center p-3.5 rounded-xl bg-white shadow-[0_4px_20px_rgba(139,92,246,0.08)] hover:shadow-[0_4px_20px_rgba(139,92,246,0.15)] transition-all duration-300 group cursor-default\"\n        >\n          <div class=\"w-10 h-10 rounded-lg bg-gradient-to-br from-violet-50 to-indigo-50 flex items-center justify-center mr-3\">\n            <component :is=\"benefit.icon\" class=\"w-5 h-5 text-violet-600\" />\n          </div>\n          <div class=\"text-left flex-1\">\n            <div class=\"text-sm text-gray-800 font-medium\">{{ benefit.name }}</div>\n            <div class=\"text-xs text-violet-500/70 mt-0.5\">{{ benefit.desc }}</div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 价格信息 -->\n      <div class=\"inline-flex items-center px-5 py-2.5 rounded-full bg-gradient-to-r from-violet-50 to-indigo-50 mb-8\">\n        <span class=\"text-sm text-gray-500 mr-2\">仅需</span>\n        <span class=\"text-base text-violet-600 font-medium\">¥</span>\n        <span class=\"text-3xl font-semibold bg-gradient-to-r from-violet-600 to-indigo-600 bg-clip-text text-transparent mx-1 -mt-0.5\">{{ dailyPrice }}</span>\n        <span class=\"text-sm text-gray-500\">/天</span>\n      </div>\n\n      <!-- 按钮 -->\n      <button\n        class=\"w-full h-12 bg-gradient-to-r from-violet-600 to-indigo-600 text-white text-base font-medium rounded-xl hover:from-violet-500 hover:to-indigo-500 transition-all duration-300 transform hover:shadow-[0_4px_20px_rgba(139,92,246,0.3)]\"\n        @click=\"handleUpgrade\"\n      >\n        立即开通\n      </button>\n    </div>\n  </el-dialog>\n</template>\n\n<script setup>\nimport { ref, defineProps, defineEmits, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { CrownIcon } from '@heroicons/vue/24/outline'\nimport { membershipBenefits, membershipPricing } from '@/config/membership'\n\nconst props = defineProps({\n  modelValue: {\n    type: Boolean,\n    default: false\n  }\n})\n\nconst emit = defineEmits(['update:modelValue'])\nconst router = useRouter()\n\nconst dialogVisible = ref(props.modelValue)\nconst dailyPrice = membershipPricing.daily\n\n// 监听 modelValue 变化\nwatch(() => props.modelValue, (val) => {\n  dialogVisible.value = val\n})\n\n// 监听 dialogVisible 变化\nwatch(() => dialogVisible.value, (val) => {\n  emit('update:modelValue', val)\n})\n\nconst handleClose = () => {\n  dialogVisible.value = false\n}\n\nconst handleUpgrade = () => {\n  dialogVisible.value = false\n  router.push('/pro')\n}\n</script>\n\n<style scoped>\n.pro-dialog :deep(.el-dialog) {\n  border: none;\n  border-radius: 20px;\n  overflow: visible;\n  background: linear-gradient(135deg, #FCF8FF 0%, #FFFFFF 50%, #F5F7FF 100%);\n  box-shadow: \n    0 25px 50px -12px rgba(0, 0, 0, 0.15),\n    0 0 30px rgba(139, 92, 246, 0.15);\n}\n\n.pro-dialog :deep(.el-dialog__header) {\n  display: none;\n}\n\n.pro-dialog :deep(.el-dialog__body) {\n  padding: 0;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn) {\n  top: -12px;\n  right: -12px;\n  z-index: 10;\n  background: white;\n  border: 1px solid rgba(139, 92, 246, 0.1);\n  border-radius: 50%;\n  width: 28px;\n  height: 28px;\n  box-shadow: 0 4px 12px rgba(139, 92, 246, 0.1);\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn .el-dialog__close) {\n  color: rgba(139, 92, 246, 0.6);\n  font-size: 18px;\n  transition: color 0.3s;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn:hover .el-dialog__close) {\n  color: rgba(139, 92, 246, 0.9);\n}\n\n/* 添加动画效果 */\n@keyframes scaleIn {\n  from {\n    opacity: 0;\n    transform: scale(0.95) translateY(10px);\n  }\n  to {\n    opacity: 1;\n    transform: scale(1) translateY(0);\n  }\n}\n\n.pro-dialog :deep(.el-dialog) {\n  animation: scaleIn 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n}\n</style> "],"mappings":"AA4DA,SAASA,GAA6B,EAAEC,KAAK,QAAQ,KAAI;AACzD,SAASC,SAAS,QAAQ,YAAW;AACrC,SAASC,SAAS,QAAQ,2BAA0B;AACpD,SAASC,kBAAkB,EAAEC,iBAAiB,QAAQ,qBAAoB;;;;;;;;;;;;;;;IAE1E,MAAMC,KAAK,GAAGC,OAAA;IAOd,MAAMC,IAAI,GAAGC,MAAA;IACb,MAAMC,MAAM,GAAGR,SAAS,CAAC;IAEzB,MAAMS,aAAa,GAAGX,GAAG,CAACM,KAAK,CAACM,UAAU;IAC1C,MAAMC,UAAU,GAAGR,iBAAiB,CAACS,KAAI;;IAEzC;IACAb,KAAK,CAAC,MAAMK,KAAK,CAACM,UAAU,EAAGG,GAAG,IAAK;MACrCJ,aAAa,CAACK,KAAK,GAAGD,GAAE;IAC1B,CAAC;;IAED;IACAd,KAAK,CAAC,MAAMU,aAAa,CAACK,KAAK,EAAGD,GAAG,IAAK;MACxCP,IAAI,CAAC,mBAAmB,EAAEO,GAAG;IAC/B,CAAC;IAED,MAAME,WAAW,GAAGA,CAAA,KAAM;MACxBN,aAAa,CAACK,KAAK,GAAG,KAAI;IAC5B;IAEA,MAAME,aAAa,GAAGA,CAAA,KAAM;MAC1BP,aAAa,CAACK,KAAK,GAAG,KAAI;MAC1BN,MAAM,CAACS,IAAI,CAAC,MAAM;IACpB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}