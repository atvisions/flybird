{"ast":null,"code":"import { ref, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { SparklesIcon, DocumentTextIcon, CloudArrowDownIcon, ShieldCheckIcon, UserGroupIcon } from '@heroicons/vue/24/outline';\nexport default {\n  __name: 'ProDialog',\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n    const router = useRouter();\n    const dialogVisible = ref(props.modelValue);\n\n    // 监听 modelValue 变化\n    watch(() => props.modelValue, val => {\n      dialogVisible.value = val;\n    });\n\n    // 监听 dialogVisible 变化\n    watch(() => dialogVisible.value, val => {\n      emit('update:modelValue', val);\n    });\n\n    // 会员权益列表\n    const benefits = [{\n      name: '简历导入',\n      icon: DocumentTextIcon\n    }, {\n      name: '批量下载',\n      icon: CloudArrowDownIcon\n    }, {\n      name: '隐私保护',\n      icon: ShieldCheckIcon\n    }, {\n      name: '专属客服',\n      icon: UserGroupIcon\n    }];\n    const handleClose = () => {\n      dialogVisible.value = false;\n    };\n    const handleUpgrade = () => {\n      dialogVisible.value = false;\n      router.push('/pro');\n    };\n    const __returned__ = {\n      props,\n      emit,\n      router,\n      dialogVisible,\n      benefits,\n      handleClose,\n      handleUpgrade,\n      ref,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get SparklesIcon() {\n        return SparklesIcon;\n      },\n      get DocumentTextIcon() {\n        return DocumentTextIcon;\n      },\n      get CloudArrowDownIcon() {\n        return CloudArrowDownIcon;\n      },\n      get ShieldCheckIcon() {\n        return ShieldCheckIcon;\n      },\n      get UserGroupIcon() {\n        return UserGroupIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","watch","useRouter","SparklesIcon","DocumentTextIcon","CloudArrowDownIcon","ShieldCheckIcon","UserGroupIcon","props","__props","emit","__emit","router","dialogVisible","modelValue","val","value","benefits","name","icon","handleClose","handleUpgrade","push"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/components/ProDialog.vue"],"sourcesContent":["<template>\n  <el-dialog\n    v-model=\"dialogVisible\"\n    :show-close=\"true\"\n    width=\"360px\"\n    class=\"pro-dialog\"\n    @close=\"handleClose\"\n  >\n    <div class=\"relative p-8 text-center bg-gradient-to-b from-gray-900 to-gray-800\">\n      <!-- 装饰元素 -->\n      <div class=\"absolute top-0 left-1/2 -translate-x-1/2 -translate-y-1/2\">\n        <div class=\"w-12 h-12 bg-gradient-to-r from-yellow-400 to-yellow-300 rounded-full border-4 border-white shadow-xl flex items-center justify-center\">\n          <SparklesIcon class=\"w-7 h-7 text-white\" />\n        </div>\n      </div>\n\n      <!-- 标题 -->\n      <div class=\"relative\">\n        <h3 class=\"text-xl font-medium text-white mb-2\">专属权益大升级</h3>\n        <div class=\"w-12 h-1 bg-gradient-to-r from-yellow-400 to-yellow-300 mx-auto\"></div>\n      </div>\n\n      <!-- 会员权益列表 -->\n      <div class=\"grid grid-cols-4 gap-4 my-8\">\n        <div v-for=\"(benefit, index) in benefits\" :key=\"index\" class=\"text-center group\">\n          <div class=\"w-12 h-12 mx-auto mb-2 rounded-xl bg-gray-700/50 backdrop-blur-sm flex items-center justify-center transform transition-all duration-300 group-hover:scale-110 group-hover:bg-yellow-400/20\">\n            <component :is=\"benefit.icon\" class=\"w-6 h-6 text-yellow-400 transition-colors group-hover:text-yellow-300\" />\n          </div>\n          <div class=\"text-xs text-gray-300 group-hover:text-yellow-400 transition-colors\">{{ benefit.name }}</div>\n        </div>\n      </div>\n\n      <!-- 价格信息 -->\n      <div class=\"text-sm text-gray-400 mb-8\">\n        <span class=\"text-xs\">¥</span>\n        <span class=\"text-3xl font-medium text-yellow-400\">0.25</span>\n        <span class=\"text-gray-300\">/天</span>\n      </div>\n\n      <!-- 按钮 -->\n      <button\n        class=\"w-full py-3 bg-gradient-to-r from-yellow-400 to-yellow-300 text-gray-900 font-medium rounded-xl hover:from-yellow-300 hover:to-yellow-200 transition-all duration-300 transform hover:scale-[1.02] shadow-lg hover:shadow-xl\"\n        @click=\"handleUpgrade\"\n      >\n        立即升级\n      </button>\n    </div>\n  </el-dialog>\n</template>\n\n<script setup>\nimport { ref, defineProps, defineEmits, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport {\n  SparklesIcon,\n  DocumentTextIcon,\n  CloudArrowDownIcon,\n  ShieldCheckIcon,\n  UserGroupIcon\n} from '@heroicons/vue/24/outline'\n\nconst props = defineProps({\n  modelValue: {\n    type: Boolean,\n    default: false\n  }\n})\n\nconst emit = defineEmits(['update:modelValue'])\nconst router = useRouter()\n\nconst dialogVisible = ref(props.modelValue)\n\n// 监听 modelValue 变化\nwatch(() => props.modelValue, (val) => {\n  dialogVisible.value = val\n})\n\n// 监听 dialogVisible 变化\nwatch(() => dialogVisible.value, (val) => {\n  emit('update:modelValue', val)\n})\n\n// 会员权益列表\nconst benefits = [\n  {\n    name: '简历导入',\n    icon: DocumentTextIcon\n  },\n  {\n    name: '批量下载',\n    icon: CloudArrowDownIcon\n  },\n  {\n    name: '隐私保护',\n    icon: ShieldCheckIcon\n  },\n  {\n    name: '专属客服',\n    icon: UserGroupIcon\n  }\n]\n\nconst handleClose = () => {\n  dialogVisible.value = false\n}\n\nconst handleUpgrade = () => {\n  dialogVisible.value = false\n  router.push('/pro')\n}\n</script>\n\n<style scoped>\n.pro-dialog :deep(.el-dialog) {\n  border: none;\n  border-radius: 1.5rem;\n  overflow: hidden;\n  background: transparent;\n  box-shadow: 0 25px 50px -12px rgba(0, 0, 0, 0.5);\n}\n\n.pro-dialog :deep(.el-dialog__header) {\n  display: none;\n}\n\n.pro-dialog :deep(.el-dialog__body) {\n  padding: 0;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn) {\n  top: 1rem;\n  right: 1rem;\n  z-index: 10;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn .el-dialog__close) {\n  color: rgba(255, 255, 255, 0.6);\n  transition: color 0.3s;\n}\n\n.pro-dialog :deep(.el-dialog__headerbtn:hover .el-dialog__close) {\n  color: rgba(255, 255, 255, 0.9);\n}\n\n/* 添加动画效果 */\n@keyframes slideIn {\n  from {\n    opacity: 0;\n    transform: translateY(-20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n.pro-dialog :deep(.el-dialog) {\n  animation: slideIn 0.3s ease-out;\n}\n</style> "],"mappings":"AAmDA,SAASA,GAA6B,EAAEC,KAAK,QAAQ,KAAI;AACzD,SAASC,SAAS,QAAQ,YAAW;AACrC,SACEC,YAAY,EACZC,gBAAgB,EAChBC,kBAAkB,EAClBC,eAAe,EACfC,aAAY,QACP,2BAA0B;;;;;;;;;;;;;;;IAEjC,MAAMC,KAAK,GAAGC,OAAA;IAOd,MAAMC,IAAI,GAAGC,MAAA;IACb,MAAMC,MAAM,GAAGV,SAAS,CAAC;IAEzB,MAAMW,aAAa,GAAGb,GAAG,CAACQ,KAAK,CAACM,UAAU;;IAE1C;IACAb,KAAK,CAAC,MAAMO,KAAK,CAACM,UAAU,EAAGC,GAAG,IAAK;MACrCF,aAAa,CAACG,KAAK,GAAGD,GAAE;IAC1B,CAAC;;IAED;IACAd,KAAK,CAAC,MAAMY,aAAa,CAACG,KAAK,EAAGD,GAAG,IAAK;MACxCL,IAAI,CAAC,mBAAmB,EAAEK,GAAG;IAC/B,CAAC;;IAED;IACA,MAAME,QAAQ,GAAG,CACf;MACEC,IAAI,EAAE,MAAM;MACZC,IAAI,EAAEf;IACR,CAAC,EACD;MACEc,IAAI,EAAE,MAAM;MACZC,IAAI,EAAEd;IACR,CAAC,EACD;MACEa,IAAI,EAAE,MAAM;MACZC,IAAI,EAAEb;IACR,CAAC,EACD;MACEY,IAAI,EAAE,MAAM;MACZC,IAAI,EAAEZ;IACR,EACF;IAEA,MAAMa,WAAW,GAAGA,CAAA,KAAM;MACxBP,aAAa,CAACG,KAAK,GAAG,KAAI;IAC5B;IAEA,MAAMK,aAAa,GAAGA,CAAA,KAAM;MAC1BR,aAAa,CAACG,KAAK,GAAG,KAAI;MAC1BJ,MAAM,CAACU,IAAI,CAAC,MAAM;IACpB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}