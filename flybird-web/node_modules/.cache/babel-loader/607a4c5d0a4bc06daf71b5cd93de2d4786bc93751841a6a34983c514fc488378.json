"grid grid-cols-2 sm:grid-cols-3 lg:grid-cols-4 gap-2 md:gap-6\">\n          <TemplateCard\n            v-for=\"template in filteredTemplates\"\n            :key=\"template.id\"\n            :template=\"template\"\n          />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport TemplateCard from '@/components/resume/ResumeCard.vue'\nimport {\n  DocumentIcon,\n  UserGroupIcon,\n  HeartIcon,\n  PlusIcon,\n  ChevronDownIcon\n} from '@heroicons/vue/24/outline'\nimport { useRouter } from 'vue-router'\n\nconst router = useRouter()\nconst isMenuExpanded = ref(false)\n\n// 主导航数据\nconst mainNavs = [\n  { name: '全部模板', path: '/templates' },\n  { name: '校园简历', path: '/templates/campus' },\n  { name: '社会简历', path: '/templates/professional' },\n  { name: '英文简历', path: '/templates/english' }\n]\n\n// 当前选中的主导航\nconst currentMainNav = ref('/templates/campus')\n\n// 当前选中的分类\nconst currentCategory = ref('all')\n\n// 模板分类\nconst templateCategories = [\n  { id: 'all', name: '全部' },\n  { id: 'internship', name: '实习生' },\n  { id: 'graduate', name: '应届生' },\n  { id: 'research', name: '研究生' },\n  { id: 'academic', name: '学术简历' },\n  { id: 'competition', name: '竞赛简历' }\n]\n\n// 模板数据\nconst templates = ref([\n  {\n    id: 1,\n    title: '应届生求职简历',\n    description: '突出学历背景和实习经验，帮助应届生展现职场竞争力。',\n    cover: 'https://picsum.photos/600/800?random=3',\n    author: {\n      name: '校园招聘官',\n      avatar: 'https://picsum.photos/32/32?random=3'\n    },\n    views: 2341,\n    likes: 178,\n    type: 'graduate'\n  },\n  // ... 更多模板数据\n])\n\n// 筛选逻辑\nconst filteredTemplates = computed(() => {\n  if (currentCategory.value === 'all') {\n    return templates.value\n  }\n  return templates.value.filter(template => template.type === currentCategory.value)\n})\n\n// 定义各页面的二级分类\nconst subCategories = {\n  '/templates': [\n    { id: 'all', name: '全部' },\n    { id: 'popular', name: '最热门' },\n    { id: 'newest', name: '最新' },\n    { id: 'recommended', name: '推荐' }\n  ],\n  '/templates/campus': [\n    { id: 'all', name: '全部' },\n    { id: 'internship', name: '实习生' },\n    { id: 'graduate', name: '应届生' },\n    { id: 'research', name: '研究生' },\n    { id: 'academic', name: '学术简历' },\n    { id: 'competition', name: '竞赛简历' }\n  ],\n  '/templates/professional': [\n    { id: 'all', name: '全部' },\n    { id: 'tech', name: '技术类' },\n    { id: 'management', name: '管理类' },\n    { id: 'marketing', name: '市场类' },\n    { id: 'finance', name: '金融类' },\n    { id: 'design', name: '设计类' }\n  ],\n  '/templates/english': [\n    { id: 'all', name: '全部' },\n    { id: 'basic', name: '基础型' },\n    { id: 'professional', name: '专业型' },\n    { id: 'executive', name: '高管型' }\n  ]\n}\n\n// 获取当前导航名称\nconst getCurrentNavName = computed(() => {\n  const currentNav = mainNavs.find(nav => nav.path === router.currentRoute.value.path)\n  return currentNav ? currentNav.name : '选择分类'\n})\n\n// 处理主导航点击\nconst handleMainNavClick = (nav) => {\n  if (router.currentRoute.value.path !== nav.path) {\n    router.push(nav.path)\n    currentCategory.value = 'all'\n  }\n}\n</script> "],"mappings":";;EACOA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAwC;;EAI5CA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAA+D;;;uBANhFC,mBAAA,CAeM,OAfNC,UAeM,GAdJC,mBAAA,CAaM,OAbNC,UAaM,GAZJC,YAAA,CAAsBC,6BAAA,GAEtBC,mBAAA,UAAa,EACbJ,mBAAA,CAQM,OARNK,UAQM,GAPJL,mBAAA,CAMM,OANNM,UAMM,I,kBALJR,mBAAA,CAIES,SAAA,QAZZC,WAAA,CAS+BC,MAAA,CAAAC,iBAAiB,EAA7BC,QAAQ;yBADjBC,YAAA,CAIEH,MAAA;MAFCI,GAAG,EAAEF,QAAQ,CAACG,EAAE;MAChBH,QAAQ,EAAEA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}