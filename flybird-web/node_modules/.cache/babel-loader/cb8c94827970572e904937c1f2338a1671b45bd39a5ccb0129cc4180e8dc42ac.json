{"ast":null,"code":"import { ref, onMounted, onUnmounted, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useAuthStore } from '@/stores/auth';\nimport { useAccountStore } from '@/stores/account';\nimport { showToast } from '@/components/ToastMessage';\nimport { STORAGE_KEYS } from '@/constants';\nimport HeadView from '@/components/HeadView.vue';\nexport default {\n  __name: 'LoginView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const authStore = useAuthStore();\n    const accountStore = useAccountStore();\n\n    // 添加 loading ref\n    const loading = ref(false);\n    const showPassword = ref(false);\n\n    // 表单状态\n    const form = ref({\n      account: '',\n      password: '',\n      rememberMe: false\n    });\n\n    // 表单验证状态\n    const state = ref({\n      accountError: '',\n      passwordError: ''\n    });\n\n    // 表单验证\n    const validateAccount = () => {\n      state.value.accountError = '';\n      if (!form.value.account) {\n        state.value.accountError = '请输入账号';\n        return false;\n      }\n      const account = form.value.account.trim();\n      // 验证账号格式：手机号/邮箱/UID\n      const phoneRegex = /^1[3-9]\\d{9}$/;\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      const uidRegex = /^\\d+$/;\n      console.log('Validating account:', {\n        account,\n        isPhone: phoneRegex.test(account),\n        isEmail: emailRegex.test(account),\n        isUID: uidRegex.test(account)\n      });\n\n      // 优先判断是否为手机号\n      if (phoneRegex.test(account)) {\n        return true;\n      }\n\n      // 其次判断是否为邮箱\n      if (emailRegex.test(account)) {\n        return true;\n      }\n\n      // 最后判断是否为 UID（且不是手机号格式）\n      if (uidRegex.test(account) && !phoneRegex.test(account)) {\n        return true;\n      }\n\n      // 如果都不符合，显示错误信息\n      state.value.accountError = '请输入正确的手机号/邮箱/UID';\n      return false;\n    };\n    const validatePassword = () => {\n      state.value.passwordError = '';\n      if (!form.value.password) {\n        state.value.passwordError = '请输入密码';\n        return false;\n      }\n      if (form.value.password.length < 6) {\n        state.value.passwordError = '密码长度不能少于6位';\n        return false;\n      }\n      return true;\n    };\n\n    // 输入事件处理\n    const handleAccountInput = () => {\n      if (state.value.accountError) {\n        validateAccount();\n      }\n    };\n    const handlePasswordInput = () => {\n      if (state.value.passwordError) {\n        validatePassword();\n      }\n    };\n\n    // 切换密码显示\n    const togglePassword = () => {\n      showPassword.value = !showPassword.value;\n    };\n\n    // 处理登录\n    const handleLogin = async () => {\n      if (!validateAccount() || !validatePassword()) {\n        return;\n      }\n      try {\n        loading.value = true;\n        const response = await authStore.login({\n          account: form.value.account.trim(),\n          password: form.value.password\n        }, form.value.rememberMe);\n\n        // 登录成功\n        router.push(redirect || '/');\n      } catch (error) {\n        console.log('Login Error:', {\n          response: error.response?.data,\n          status: error.response?.status\n        });\n        if (error.response?.data) {\n          const {\n            message,\n            error_code\n          } = error.response.data;\n\n          // 根据错误代码处理不同情况\n          switch (error_code) {\n            case 'phone_not_registered':\n              showToast('该手机号未注册，即将跳转到注册页面', 'info');\n              setTimeout(() => {\n                router.push({\n                  path: '/register',\n                  query: {\n                    phone: form.value.account\n                  }\n                });\n              }, 1500);\n              break;\n            case 'email_not_registered':\n              showToast('该邮箱未注册，请先注册', 'error');\n              break;\n            case 'uid_not_found':\n              showToast('该UID不存在', 'error');\n              break;\n            case 'invalid_account_format':\n              showToast('请输入正确的手机号/邮箱/UID', 'error');\n              break;\n            case 'invalid_password':\n              showToast('密码错误，请重新输入', 'error');\n              form.value.password = ''; // 清空密码\n              break;\n            case 'account_disabled':\n              showToast('该账号已被禁用，请联系客服', 'error');\n              break;\n            case 'account_not_found':\n              showToast('账号不存在', 'error');\n              break;\n            case 'validation_error':\n              showToast(message || '输入有误，请检查', 'error');\n              break;\n            case 'system_error':\n              showToast('系统错误，请稍后重试', 'error');\n              break;\n            default:\n              showToast(message || '登录失败，请稍后重试', 'error');\n          }\n        } else {\n          showToast('网络连接失败，请检查网络设置', 'error');\n        }\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // 在组件挂载时恢复记住我状态\n    onMounted(() => {\n      const savedRememberMe = localStorage.getItem(STORAGE_KEYS.REMEMBER_ME);\n      if (savedRememberMe !== null) {\n        form.value.rememberMe = savedRememberMe === 'true';\n      }\n\n      // 如果有记住的账号，自动填充\n      const rememberedAccount = localStorage.getItem(STORAGE_KEYS.REMEMBERED_ACCOUNT);\n      if (rememberedAccount && form.value.rememberMe) {\n        form.value.account = rememberedAccount;\n      }\n    });\n\n    // 监听 rememberMe 的变化\n    watch(() => form.value.rememberMe, newValue => {\n      if (!newValue) {\n        localStorage.removeItem(STORAGE_KEYS.REMEMBERED_ACCOUNT);\n        localStorage.removeItem(STORAGE_KEYS.REMEMBER_ME);\n      }\n      localStorage.setItem(STORAGE_KEYS.REMEMBER_ME, newValue.toString());\n    });\n\n    // 在组件卸载时，如果没有选择记住我，清除相关信息\n    onUnmounted(() => {\n      if (!form.value.rememberMe) {\n        localStorage.removeItem(STORAGE_KEYS.REMEMBERED_ACCOUNT);\n        localStorage.removeItem(STORAGE_KEYS.REMEMBER_ME);\n      }\n    });\n    const __returned__ = {\n      router,\n      authStore,\n      accountStore,\n      loading,\n      showPassword,\n      form,\n      state,\n      validateAccount,\n      validatePassword,\n      handleAccountInput,\n      handlePasswordInput,\n      togglePassword,\n      handleLogin,\n      ref,\n      onMounted,\n      onUnmounted,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get useAuthStore() {\n        return useAuthStore;\n      },\n      get useAccountStore() {\n        return useAccountStore;\n      },\n      get showToast() {\n        return showToast;\n      },\n      get STORAGE_KEYS() {\n        return STORAGE_KEYS;\n      },\n      HeadView\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","watch","useRouter","useAuthStore","useAccountStore","showToast","STORAGE_KEYS","HeadView","router","authStore","accountStore","loading","showPassword","form","account","password","rememberMe","state","accountError","passwordError","validateAccount","value","trim","phoneRegex","emailRegex","uidRegex","console","log","isPhone","test","isEmail","isUID","validatePassword","length","handleAccountInput","handlePasswordInput","togglePassword","handleLogin","response","login","push","redirect","error","data","status","message","error_code","setTimeout","path","query","phone","savedRememberMe","localStorage","getItem","REMEMBER_ME","rememberedAccount","REMEMBERED_ACCOUNT","newValue","removeItem","setItem","toString"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col min-h-screen\">\n    <HeadView />\n    <main class=\"flex-1 pt-14 relative z-[1] bg-gray-50\">\n      <!-- Logo 部分 -->\n      <div class=\"sm:mx-auto sm:w-full sm:max-w-md pt-8\">\n        <h2 class=\"mt-6 text-center text-2xl font-bold leading-9 tracking-tight text-gray-900\">登录账号</h2>\n      </div>\n\n      <div class=\"mt-10 sm:mx-auto sm:w-full sm:max-w-[480px]\">\n        <div class=\"bg-white px-6 py-12 shadow sm:rounded-lg sm:px-12\">\n          <form class=\"space-y-6\" @submit.prevent=\"handleLogin\">\n            <!-- 手机号输入 -->\n            <div>\n              <label for=\"account\" class=\"block text-sm font-medium leading-6 text-gray-900\">账号</label>\n              <div class=\"mt-2\">\n                <input \n                  type=\"text\" \n                  id=\"account\" \n                  v-model=\"form.account\"\n                  @input=\"handleAccountInput\"\n                  required\n                  class=\"block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-primary-600 sm:text-sm sm:leading-6 px-3\"\n                  :class=\"{'ring-red-300': state.accountError}\"\n                  placeholder=\"请输入手机号/邮箱/UID\"\n                >\n                <div v-if=\"state.accountError\" class=\"mt-1 text-sm text-red-600\">{{ state.accountError }}</div>\n              </div>\n            </div>\n\n            <!-- 密码输入 -->\n            <div>\n              <label for=\"password\" class=\"block text-sm font-medium text-gray-700\">密码</label>\n              <div class=\"mt-2 relative\">\n                <input \n                  :type=\"showPassword ? 'text' : 'password'\"\n                  id=\"password\"\n                  v-model=\"form.password\"\n                  @input=\"handlePasswordInput\"\n                  required\n                  class=\"block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-primary-600 sm:text-sm sm:leading-6 px-3 pr-10\"\n                  :class=\"{'ring-red-300': state.passwordError}\"\n                >\n                <button \n                  type=\"button\"\n                  @click=\"togglePassword\"\n                  class=\"absolute inset-y-0 right-0 flex items-center pr-3\"\n                >\n                  <!-- 密码显示/隐藏图标 -->\n                  <svg v-if=\"showPassword\" class=\"h-5 w-5 text-gray-400\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 12a3 3 0 11-6 0 3 3 0 016 0z\" />\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z\" />\n                  </svg>\n                  <svg v-else class=\"h-5 w-5 text-gray-400\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M13.875 18.825A10.05 10.05 0 0112 19c-4.478 0-8.268-2.943-9.543-7a9.97 9.97 0 011.563-3.029m5.858.908a3 3 0 114.243 4.243M9.878 9.878l4.242 4.242M9.88 9.88l-3.29-3.29m7.532 7.532l3.29 3.29M3 3l3.59 3.59m0 0A9.953 9.953 0 0112 5c4.478 0 8.268 2.943 9.543 7a10.025 10.025 0 01-4.132 5.411m0 0L21 21\" />\n                  </svg>\n                </button>\n              </div>\n              <div v-if=\"state.passwordError\" class=\"mt-1 text-sm text-red-600\">{{ state.passwordError }}</div>\n            </div>\n\n            <!-- 记住我和忘记密码 -->\n            <div class=\"flex items-center justify-between\">\n              <div class=\"flex items-center\">\n                <input\n                  id=\"remember-me\"\n                  v-model=\"form.rememberMe\"\n                  type=\"checkbox\"\n                  class=\"h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded\"\n                />\n                <label for=\"remember-me\" class=\"ml-2 block text-sm text-gray-900\">\n                  记住我\n                </label>\n              </div>\n\n              <div class=\"text-sm\">\n                <router-link to=\"/reset-password\" class=\"font-medium text-primary-600 hover:text-primary-500\">\n                  忘记密码？\n                </router-link>\n              </div>\n            </div>\n\n            <!-- 登录按钮 -->\n            <div>\n              <button \n                type=\"submit\"\n                :disabled=\"loading\"\n                class=\"flex w-full justify-center rounded-md bg-indigo-600 px-3 py-1.5 text-sm font-semibold leading-6 text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600 disabled:opacity-50\"\n              >\n                <svg v-if=\"loading\" class=\"animate-spin -ml-1 mr-3 h-5 w-5 text-white\" fill=\"none\" viewBox=\"0 0 24 24\">\n                  <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"></circle>\n                  <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z\"></path>\n                </svg>\n                {{ loading ? '登录中...' : '登录' }}\n              </button>\n            </div>\n          </form>\n\n          <!-- 其他登录方式 -->\n          <div class=\"mt-6\">\n            <div class=\"relative\">\n              <div class=\"absolute inset-0 flex items-center\">\n                <div class=\"w-full border-t border-gray-300\"></div>\n              </div>\n              <div class=\"relative flex justify-center text-sm\">\n                <span class=\"bg-white px-2 text-gray-500\">其他登录方式</span>\n              </div>\n            </div>\n\n            <div class=\"mt-6 grid grid-cols-3 gap-3\">\n              <div>\n                <button\n                  @click=\"handleWeChatClick\" \n                  class=\"w-full inline-flex justify-center py-2 px-4 border border-gray-300 rounded-md shadow-sm bg-white text-sm font-medium text-gray-500 hover:bg-gray-50\"\n                >\n                  <span class=\"sr-only\">微信登录</span>\n                  <svg class=\"w-5 h-5 text-green-600\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\n                    <path d=\"M8.667 15.273c-.328 0-.593-.265-.593-.593v-1.778a.592.592 0 0 1 1.186 0v1.778c0 .328-.265.593-.593.593zm6.666 0c-.328 0-.593-.265-.593-.593v-1.778a.592.592 0 0 1 1.186 0v1.778c0 .328-.265.593-.593.593z\"/>\n                    <path d=\"M12 2C6.477 2 2 6.477 2 12s4.477 10 10 10 10-4.477 10-10S17.523 2 12 2zm5.918 12.08c-.533 2.923-3.982 5.012-7.918 4.37-2.718-.444-4.963-2.165-5.735-4.37-.844-2.413.437-4.693 2.735-5.912.842-.446 1.777-.724 2.735-.832.437-.049.877-.073 1.318-.073 1.683 0 3.23.446 4.503 1.208 1.41.841 2.333 2.083 2.432 3.49.05.706-.095 1.406-.432 2.032-.535.99-1.563 1.756-2.9 2.165-1.096.336-2.245.361-3.368.073-.89-.227-1.683-.631-2.333-1.208-.437-.384-.706-.841-.816-1.333-.073-.336.024-.681.267-.927.242-.242.583-.339.915-.266.339.073.63.291.78.582.145.29.436.532.824.678.63.242 1.37.266 2.023.073.824-.242 1.37-.752 1.37-1.284 0-.532-.533-1.018-1.37-1.26-.63-.193-1.37-.169-2.023.073-.388.146-.679.388-.824.679-.145.29-.436.508-.78.581-.332.073-.673-.024-.915-.266-.243-.242-.34-.587-.267-.923.11-.492.379-.95.816-1.333.65-.577 1.443-.981 2.333-1.208 1.123-.288 2.272-.263 3.368.073 1.337.409 2.365 1.175 2.9 2.165.337.626.482 1.326.432 2.032-.099 1.407-1.022 2.649-2.432 3.49-1.273.762-2.82 1.208-4.503 1.208-.441 0-.881-.024-1.318-.073-.958-.108-1.893-.386-2.735-.832-2.298-1.219-3.579-3.499-2.735-5.912.772-2.205 3.017-3.926 5.735-4.37 3.936-.642 7.385 1.447 7.918 4.37z\"/>\n                  </svg>\n                </button>\n              </div>\n\n              <div>\n                <a href=\"#\" class=\"w-full inline-flex justify-center py-2 px-4 border border-gray-300 rounded-md shadow-sm bg-white text-sm font-medium text-gray-500 hover:bg-gray-50\">\n                  <span class=\"sr-only\">QQ登录</span>\n                  <svg class=\"w-5 h-5\" fill=\"currentColor\" viewBox=\"0 0 24 24\">\n                    <path d=\"M12.003 2c-2.265 0-6.29 1.364-6.29 7.325v1.195S3.55 14.96 3.55 17.474c0 .665.17 1.025.281 1.025.114 0 .902-.484 1.748-2.072 0 0-.18 2.197 1.904 3.967 0 0-1.77.495-1.77 1.182 0 .686 4.078.43 6.29.43 2.213 0 6.29.256 6.29-.43 0-.687-1.77-1.182-1.77-1.182s2.085-1.77 1.905-3.967c.846 1.588 1.634 2.072 1.746 2.072.111 0 .283-.36.283-1.025 0-2.514-2.166-6.954-2.166-6.954V9.325C18.29 3.364 14.268 2 12.003 2z\"/>\n                  </svg>\n                </a>\n              </div>\n\n              <div>\n                <a href=\"#\" class=\"w-full inline-flex justify-center py-2 px-4 border border-gray-300 rounded-md shadow-sm bg-white text-sm font-medium text-gray-500 hover:bg-gray-50\">\n                  <span class=\"sr-only\">微博登录</span>\n                  <svg class=\"w-5 h-5\" fill=\"currentColor\" viewBox=\"0 0 24 24\">\n                    <path d=\"M10.098 20.323c-3.977.391-7.414-1.406-7.672-4.02-.259-2.609 2.759-5.047 6.74-5.441 3.979-.394 7.413 1.404 7.672 4.018.259 2.6-2.759 5.049-6.74 5.443z\"/>\n                  </svg>\n                </a>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useAuthStore } from '@/stores/auth'\nimport { useAccountStore } from '@/stores/account'\nimport { showToast } from '@/components/ToastMessage'\nimport { STORAGE_KEYS } from '@/constants'\nimport HeadView from '@/components/HeadView.vue'\n\nconst router = useRouter()\nconst authStore = useAuthStore()\nconst accountStore = useAccountStore()\n\n// 添加 loading ref\nconst loading = ref(false)\nconst showPassword = ref(false)\n\n// 表单状态\nconst form = ref({\n  account: '',\n  password: '',\n  rememberMe: false\n})\n\n// 表单验证状态\nconst state = ref({\n  accountError: '',\n  passwordError: ''\n})\n\n// 表单验证\nconst validateAccount = () => {\n  state.value.accountError = ''\n  if (!form.value.account) {\n    state.value.accountError = '请输入账号'\n    return false\n  }\n  \n  const account = form.value.account.trim()\n  // 验证账号格式：手机号/邮箱/UID\n  const phoneRegex = /^1[3-9]\\d{9}$/\n  const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/\n  const uidRegex = /^\\d+$/\n  \n  console.log('Validating account:', {\n    account,\n    isPhone: phoneRegex.test(account),\n    isEmail: emailRegex.test(account),\n    isUID: uidRegex.test(account)\n  })\n  \n  // 优先判断是否为手机号\n  if (phoneRegex.test(account)) {\n    return true\n  }\n  \n  // 其次判断是否为邮箱\n  if (emailRegex.test(account)) {\n    return true\n  }\n  \n  // 最后判断是否为 UID（且不是手机号格式）\n  if (uidRegex.test(account) && !phoneRegex.test(account)) {\n    return true\n  }\n  \n  // 如果都不符合，显示错误信息\n  state.value.accountError = '请输入正确的手机号/邮箱/UID'\n  return false\n}\n\nconst validatePassword = () => {\n  state.value.passwordError = ''\n  if (!form.value.password) {\n    state.value.passwordError = '请输入密码'\n    return false\n  }\n  if (form.value.password.length < 6) {\n    state.value.passwordError = '密码长度不能少于6位'\n    return false\n  }\n  return true\n}\n\n// 输入事件处理\nconst handleAccountInput = () => {\n  if (state.value.accountError) {\n    validateAccount()\n  }\n}\n\nconst handlePasswordInput = () => {\n  if (state.value.passwordError) {\n    validatePassword()\n  }\n}\n\n// 切换密码显示\nconst togglePassword = () => {\n  showPassword.value = !showPassword.value\n}\n\n// 处理登录\nconst handleLogin = async () => {\n  if (!validateAccount() || !validatePassword()) {\n    return\n  }\n\n  try {\n    loading.value = true\n    const response = await authStore.login({\n      account: form.value.account.trim(),\n      password: form.value.password\n    }, form.value.rememberMe)\n\n    // 登录成功\n    router.push(redirect || '/')\n\n  } catch (error) {\n    console.log('Login Error:', {\n      response: error.response?.data,\n      status: error.response?.status\n    })\n\n    if (error.response?.data) {\n      const { message, error_code } = error.response.data\n\n      // 根据错误代码处理不同情况\n      switch (error_code) {\n        case 'phone_not_registered':\n          showToast('该手机号未注册，即将跳转到注册页面', 'info')\n          setTimeout(() => {\n            router.push({\n              path: '/register',\n              query: { phone: form.value.account }\n            })\n          }, 1500)\n          break\n\n        case 'email_not_registered':\n          showToast('该邮箱未注册，请先注册', 'error')\n          break\n\n        case 'uid_not_found':\n          showToast('该UID不存在', 'error')\n          break\n\n        case 'invalid_account_format':\n          showToast('请输入正确的手机号/邮箱/UID', 'error')\n          break\n\n        case 'invalid_password':\n          showToast('密码错误，请重新输入', 'error')\n          form.value.password = ''  // 清空密码\n          break\n\n        case 'account_disabled':\n          showToast('该账号已被禁用，请联系客服', 'error')\n          break\n\n        case 'account_not_found':\n          showToast('账号不存在', 'error')\n          break\n\n        case 'validation_error':\n          showToast(message || '输入有误，请检查', 'error')\n          break\n\n        case 'system_error':\n          showToast('系统错误，请稍后重试', 'error')\n          break\n\n        default:\n          showToast(message || '登录失败，请稍后重试', 'error')\n      }\n    } else {\n      showToast('网络连接失败，请检查网络设置', 'error')\n    }\n  } finally {\n    loading.value = false\n  }\n}\n\n// 在组件挂载时恢复记住我状态\nonMounted(() => {\n  const savedRememberMe = localStorage.getItem(STORAGE_KEYS.REMEMBER_ME)\n  if (savedRememberMe !== null) {\n    form.value.rememberMe = savedRememberMe === 'true'\n  }\n  \n  // 如果有记住的账号，自动填充\n  const rememberedAccount = localStorage.getItem(STORAGE_KEYS.REMEMBERED_ACCOUNT)\n  if (rememberedAccount && form.value.rememberMe) {\n    form.value.account = rememberedAccount\n  }\n})\n\n// 监听 rememberMe 的变化\nwatch(() => form.value.rememberMe, (newValue) => {\n  if (!newValue) {\n    localStorage.removeItem(STORAGE_KEYS.REMEMBERED_ACCOUNT)\n    localStorage.removeItem(STORAGE_KEYS.REMEMBER_ME)\n  }\n  localStorage.setItem(STORAGE_KEYS.REMEMBER_ME, newValue.toString())\n})\n\n// 在组件卸载时，如果没有选择记住我，清除相关信息\nonUnmounted(() => {\n  if (!form.value.rememberMe) {\n    localStorage.removeItem(STORAGE_KEYS.REMEMBERED_ACCOUNT)\n    localStorage.removeItem(STORAGE_KEYS.REMEMBER_ME)\n  }\n})\n</script>"],"mappings":"AAqJA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAW,EAAEC,KAAK,QAAQ,KAAI;AACvD,SAASC,SAAS,QAAQ,YAAW;AACrC,SAASC,YAAY,QAAQ,eAAc;AAC3C,SAASC,eAAe,QAAQ,kBAAiB;AACjD,SAASC,SAAS,QAAQ,2BAA0B;AACpD,SAASC,YAAY,QAAQ,aAAY;AACzC,OAAOC,QAAQ,MAAM,2BAA0B;;;;;;;IAE/C,MAAMC,MAAM,GAAGN,SAAS,CAAC;IACzB,MAAMO,SAAS,GAAGN,YAAY,CAAC;IAC/B,MAAMO,YAAY,GAAGN,eAAe,CAAC;;IAErC;IACA,MAAMO,OAAO,GAAGb,GAAG,CAAC,KAAK;IACzB,MAAMc,YAAY,GAAGd,GAAG,CAAC,KAAK;;IAE9B;IACA,MAAMe,IAAI,GAAGf,GAAG,CAAC;MACfgB,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd,CAAC;;IAED;IACA,MAAMC,KAAK,GAAGnB,GAAG,CAAC;MAChBoB,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE;IACjB,CAAC;;IAED;IACA,MAAMC,eAAe,GAAGA,CAAA,KAAM;MAC5BH,KAAK,CAACI,KAAK,CAACH,YAAY,GAAG,EAAC;MAC5B,IAAI,CAACL,IAAI,CAACQ,KAAK,CAACP,OAAO,EAAE;QACvBG,KAAK,CAACI,KAAK,CAACH,YAAY,GAAG,OAAM;QACjC,OAAO,KAAI;MACb;MAEA,MAAMJ,OAAO,GAAGD,IAAI,CAACQ,KAAK,CAACP,OAAO,CAACQ,IAAI,CAAC;MACxC;MACA,MAAMC,UAAU,GAAG,eAAc;MACjC,MAAMC,UAAU,GAAG,4BAA2B;MAC9C,MAAMC,QAAQ,GAAG,OAAM;MAEvBC,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAE;QACjCb,OAAO;QACPc,OAAO,EAAEL,UAAU,CAACM,IAAI,CAACf,OAAO,CAAC;QACjCgB,OAAO,EAAEN,UAAU,CAACK,IAAI,CAACf,OAAO,CAAC;QACjCiB,KAAK,EAAEN,QAAQ,CAACI,IAAI,CAACf,OAAO;MAC9B,CAAC;;MAED;MACA,IAAIS,UAAU,CAACM,IAAI,CAACf,OAAO,CAAC,EAAE;QAC5B,OAAO,IAAG;MACZ;;MAEA;MACA,IAAIU,UAAU,CAACK,IAAI,CAACf,OAAO,CAAC,EAAE;QAC5B,OAAO,IAAG;MACZ;;MAEA;MACA,IAAIW,QAAQ,CAACI,IAAI,CAACf,OAAO,CAAC,IAAI,CAACS,UAAU,