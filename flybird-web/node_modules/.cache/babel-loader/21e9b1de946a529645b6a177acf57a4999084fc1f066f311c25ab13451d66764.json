um.photos/600/300?random=3',\n      author: {\n        name: '王大力',\n        avatar: 'https://picsum.photos/32/32?random=3'\n      },\n      views: 1987,\n      comments: 89,\n      createTime: '1天前'\n    }, {\n      id: 4,\n      title: 'Flutter vs React Native：2024移动开发框架对比',\n      description: '详细对比两大主流跨平台开发框架的优劣，从性能、生态、开发效率等多个维度进行分析。',\n      cover: 'https://picsum.photos/600/300?random=4',\n      author: {\n        name: '赵明',\n        avatar: 'https://picsum.photos/32/32?random=4'\n      },\n      views: 1654,\n      comments: 67,\n      createTime: '2天前'\n    }, {\n      id: 5,\n      title: 'MongoDB 性能优化实战指南',\n      description: '深入探讨 MongoDB 在大规模应用场景下的性能优化策略，包括索引设计、查询优化等。',\n      cover: 'https://picsum.photos/600/300?random=5',\n      author: {\n        name: '李云',\n        avatar: 'https://picsum.photos/32/32?random=5'\n      },\n      views: 1432,\n      comments: 34,\n      createTime: '2天前'\n    }]);\n\n    // 热门话题数据\n    const hotTopics = ref([{\n      id: 1,\n      title: '开发移动应用选择哪个方案？',\n      optionA: 'Flutter',\n      optionB: 'React Native',\n      votesA: 234,\n      votesB: 187,\n      author: {\n        name: '王大力',\n        avatar: 'https://picsum.photos/32/32?random=1'\n      },\n      comments: 45\n    }, {\n      id: 2,\n      title: '前端状态管理选型？',\n      optionA: 'Pinia',\n      optionB: 'Redux',\n      votesA: 445,\n      votesB: 300,\n      author: {\n        name: '李小明',\n        avatar: 'https://picsum.photos/32/32?random=2'\n      },\n      comments: 67\n    }, {\n      id: 3,\n      title: '选择哪个UI组件库？',\n      optionA: 'Element Plus',\n      optionB: 'Ant Design Vue',\n      votesA: 567,\n      votesB: 432,\n      author: {\n        name: '张前端',\n        avatar: 'https://picsum.photos/32/32?random=3'\n      },\n      comments: 89\n    }]);\n\n    // 热门问答数据\n    const hotQuestions = ref([{\n      id: 1,\n      title: '如何优雅地处理Vue3项目中的全局状态管理？',\n      answers: 12,\n      createTime: '2小时前'\n    }, {\n      id: 2,\n      title: 'Vue3性能优化的最佳实践有哪些？',\n      answers: 8,\n      createTime: '4小时前'\n    }, {\n      id: 3,\n      title: '如何实现高性能的无限滚动列表？',\n      answers: 15,\n      createTime: '6小时前'\n    }]);\n\n    // 当前选中的分类\n    const currentCategory = ref('all');\n    const __returned__ = {\n      router,\n      route,\n      store,\n      isAuthenticated,\n      mainNavs,\n      hotArticles,\n      hotTopics,\n      hotQuestions,\n      currentCategory,\n      ref,\n      computed,\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get useStore() {\n        return useStore;\n      },\n      get DocumentTextIcon() {\n        return DocumentTextIcon;\n      },\n      get QuestionMarkCircleIcon() {\n        return QuestionMarkCircleIcon;\n      },\n      get ScaleIcon() {\n        return ScaleIcon;\n      },\n      get UsersIcon() {\n        return UsersIcon;\n      },\n      get UserIcon() {\n        return UserIcon;\n      },\n      get ChevronRightIcon() {\n        return ChevronRightIcon;\n      },\n      get EyeIcon() {\n        return EyeIcon;\n      },\n      get ChatBubbleLeftIcon() {\n        return ChatBubbleLeftIcon;\n      },\n      get HandThumbUpIcon() {\n        return HandThumbUpIcon;\n      },\n      get PlusIcon() {\n        return PlusIcon;\n      },\n      PageBanner,\n      CommunityNavigation\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRouter","useRoute","useStore","DocumentTextIcon","QuestionMarkCircleIcon","ScaleIcon","UsersIcon","UserIcon","ChevronRightIcon","EyeIcon","ChatBubbleLeftIcon","HandThumbUpIcon","PlusIcon","PageBanner","CommunityNavigation","router","route","store","isAuthenticated","state","mainNavs","name","path","hotArticles","id","title","description","cover","author","avatar","views","comments","createTime","hotTopics","optionA","optionB","votesA","votesB","hotQuestions","answers","currentCategory"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/community/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"py-16 lg:py-16 mt-8 lg:mt-8\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅 -->\n      <PageBanner theme=\"violet\">\n        <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">社区</h1>\n        <p class=\"text-gray-600 text-lg max-w-2xl\">发现优秀作品，交流学习经验</p>\n      </PageBanner>\n\n      <!-- 使用导航组件 -->\n      <CommunityNavigation v-model:currentCategory=\"currentCategory\" />\n\n      <!-- 首页内容概览 -->\n      <div class=\"flex flex-col lg:flex-row gap-3 lg:gap-6\">\n        <!-- 左侧内容：话题 + 文章 -->\n        <div class=\"lg:w-3/4 space-y-3 lg:space-y-6\">\n          <!-- 热门话题 -->\n          <div class=\"bg-white rounded-lg lg:rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <div class=\"flex items-center justify-between mb-3 lg:mb-4\">\n              <h2 class=\"text-lg font-bold text-gray-900\">热门话题</h2>\n              <router-link \n                to=\"/community/topics\"\n                class=\"text-sm font-medium text-gray-500 hover:text-gray-900\"\n              >\n                更多\n              </router-link>\n            </div>\n            <!-- 显示一个话题 -->\n            <div v-if=\"hotTopics[0]\" class=\"group cursor-pointer\">\n              <!-- 话题标题 -->\n              <h3 class=\"text-xl font-bold text-gray-900 mb-6\">{{ hotTopics[0].title }}</h3>\n              \n              <!-- 作者信息 -->\n              <div class=\"flex items-center space-x-3 mb-6\">\n                <img :src=\"hotTopics[0].author.avatar\" class=\"w-8 h-8 rounded-full\">\n                <div class=\"flex items-center gap-3\">\n                  <span class=\"text-sm font-medium text-gray-900\">{{ hotTopics[0].author.name }}</span>\n                  <span class=\"text-xs text-gray-500\">{{ hotTopics[0].createTime }}</span>\n                </div>\n              </div>\n              \n              <!-- VS选项对比区域 -->\n              <div class=\"relative mb-8\">\n                <!-- 进度条背景 -->\n                <div class=\"absolute inset-0 flex rounded-xl overflow-hidden\">\n                  <!-- 左侧进度 -->\n                  <div \n                    class=\"h-full bg-gradient-to-r from-blue-50 to-blue-100\"\n                    :style=\"{ width: `${(hotTopics[0].votesA / (hotTopics[0].votesA + hotTopics[0].votesB)) * 100}%` }\"\n                  ></div>\n                  <!-- 右侧进度 -->\n                  <div \n                    class=\"h-full bg-gradient-to-l from-purple-50 to-purple-100\"\n                    :style=\"{ width: `${(hotTopics[0].votesB / (hotTopics[0].votesA + hotTopics[0].votesB)) * 100}%` }\"\n                  ></div>\n                </div>\n\n                <!-- 选项内容 -->\n                <div class=\"relative flex items-center min-h-[100px]\">\n                  <!-- 选项A -->\n                  <div class=\"flex-1 p-6\">\n                    <div class=\"text-center\">\n                      <!-- 百分比指示器 -->\n                      <div class=\"absolute -top-3 left-[25%] -translate-x-1/2 px-3 py-1 bg-blue-600 rounded-full text-white text-xs font-medium\">\n                        {{ Math.round((hotTopics[0].votesA / (hotTopics[0].votesA + hotTopics[0].votesB)) * 100) }}%\n                      </div>\n                      <div class=\"text-lg font-medium text-blue-600 mb-2\">{{ hotTopics[0].optionA }}</div>\n                      <div class=\"text-sm text-gray-500 mb-4\">{{ hotTopics[0].votesA }} 票</div>\n                      <!-- 投票按钮 -->\n                      <button \n                        class=\"px-6 h-10 rounded-full bg-blue-50 hover:bg-blue-100 transition-colors mx-auto flex items-center justify-center cursor-pointer\"\n                      >\n                        <span class=\"text-sm font-medium text-blue-600\">投票</span>\n                      </button>\n                    </div>\n                  </div>\n\n                  <!-- VS标志 -->\n                  <div class=\"w-10 h-10 rounded-full bg-white shadow-sm flex items-center justify-center z-10\">\n                    <span class=\"text-xs font-bold text-gray-400\">VS</span>\n                  </div>\n\n                  <!-- 选项B -->\n                  <div class=\"flex-1 p-6\">\n                    <div class=\"text-center\">\n                      <!-- 百分比指示器 -->\n                      <div class=\"absolute -top-3 right-[25%] translate-x-1/2 px-3 py-1 bg-purple-600 rounded-full text-white text-xs font-medium\">\n                        {{ Math.round((hotTopics[0].votesB / (hotTopics[0].votesA + hotTopics[0].votesB)) * 100) }}%\n                      </div>\n                      <div class=\"text-lg font-medium text-purple-600 mb-2\">{{ hotTopics[0].optionB }}</div>\n                      <div class=\"text-sm text-gray-500 mb-4\">{{ hotTopics[0].votesB }} 票</div>\n                      <!-- 投票按钮 -->\n                      <button \n                        class=\"px-6 h-10 rounded-full bg-purple-50 hover:bg-purple-100 transition-colors mx-auto flex items-center justify-center cursor-pointer\"\n                      >\n                        <span class=\"text-sm font-medium text-purple-600\">投票</span>\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <!-- 底部信息 -->\n              <div class=\"flex items-center justify-between\">\n                <!-- 参与者头像组 -->\n                <div class=\"flex items-center gap-2\">\n                  <div class=\"flex -space-x-2\">\n                    <img v-for=\"i in Math.min(3, Math.floor((hotTopics[0].votesA + hotTopics[0].votesB) / 10))\" :key=\"i\"\n                      :src=\"`https://picsum.photos/32/32?random=${hotTopics[0].id * 10 + i}`\"\n                      class=\"w-6 h-6 rounded-full ring-2 ring-white\"\n                    >\n                  </div>\n                  <span class=\"text-xs text-gray-500\">{{ hotTopics[0].votesA + hotTopics[0].votesB }} 人参与</span>\n                </div>\n                <div class=\"flex items-center space-x-3\">\n                  <span class=\"flex items-center text-xs text-gray-500\">\n                    <ChatBubbleLeftIcon class=\"w-3 h-3 mr-1\" />{{ hotTopics[0].comments }}\n                  </span>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 热门文章 -->\n          <div class=\"bg-white rounded-lg lg:rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <div class=\"flex items-center justify-between mb-3 lg:mb-6\">\n              <h2 class=\"text-lg font-bold text-gray-900\">热门文章</h2>\n              <router-link \n                to=\"/community/articles\"\n                class=\"text-sm font-medium text-gray-500 hover:text-gray-900 flex items-center group\"\n              >\n                更多\n                <ChevronRightIcon class=\"w-4 h-4 ml-1 group-hover:translate-x-1 transition-transform\" />\n              </router-link>\n            </div>\n            <div class=\"space-y-3 lg:space-y-5\">\n              <div v-for=\"(article, index) in hotArticles\" :key=\"article.id\"\n                class=\"group cursor-pointer rounded-lg lg:rounded-xl hover:bg-gray-50 transition-colors\"\n              >\n                <!-- 使用flex-col在移动端垂直布局，lg:flex-row在大屏幕水平布局 -->\n                <div class=\"flex flex-col lg:flex-row gap-3 lg:gap-4 p-3 lg:p-4\">\n                  <!-- 序号和内容容器 -->\n                  <div class=\"flex flex-1 gap-4\">\n                    <!-- 序号 -->\n                    <div class=\"flex-shrink-0 w-6 h-6 rounded-full flex items-center justify-center\"\n                      :class=\"{\n                        'bg-orange-100 text-orange-600': index === 0,\n                        'bg-blue-100 text-blue-600': index === 1,\n                        'bg-purple-100 text-purple-600': index === 2,\n                        'bg-gray-100 text-gray-600': index > 2\n                      }\"\n                    >\n                      <span class=\"text-sm font-medium\">{{ index + 1 }}</span>\n                    </div>\n\n                    <!-- 文章信息 -->\n                    <div class=\"flex-1 min-w-0\">\n                      <h3 class=\"text-base font-medium text-gray-900 group-hover:text-blue-600 transition-colors line-clamp-1 mb-2\">\n                        {{ article.title }}\n                      </h3>\n                      <!-- 在移动端隐藏描述 -->\n                      <p class=\"text-sm text-gray-500 line-clamp-1 mb-3 hidden lg:block\">{{ article.description }}</p>\n                      <div class=\"flex flex-wrap items-center gap-4\">\n                        <div class=\"flex items-center space-x-2\">\n                          <img :src=\"article.author.avatar\" class=\"w-5 h-5 rounded-full\">\n                          <span class=\"text-sm text-gray-600\">{{ article.author.name }}</span>\n                        </div>\n                        <div class=\"flex items-center text-xs text-gray-500 space-x-3\">\n                          <span class=\"flex items-center\">\n                            <EyeIcon class=\"w-3 h-3 mr-1\" />{{ article.views }}\n                          </span>\n                          <span class=\"flex items-center\">\n                            <ChatBubbleLeftIcon class=\"w-3 h-3 mr-1\" />{{ article.comments }}\n                          </span>\n                          <span class=\"hidden sm:inline\">{{ article.createTime }}</span>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n\n                  <!-- 文章封面图 -->\n                  <div class=\"flex-shrink-0 w-full lg:w-24 h-32 lg:h-24 rounded-lg overflow-hidden order-first lg:order-last\">\n                    <img \n                      :src=\"article.cover\" \n                      class=\"w-full h-full object-cover transform group-hover:scale-105 transition-transform duration-500\"\n                      alt=\"\"\n                    >\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 右侧内容：问答 -->\n        <div class=\"lg:w-1/4\">\n          <div class=\"bg-white rounded-lg lg:rounded-xl border border-gray-100 p-4 lg:p-6\">\n            <div class=\"flex items-center justify-between mb-3 lg:mb-6\">\n              <h2 class=\"text-lg font-bold text-gray-900\">热门问答</h2>\n              <router-link \n                to=\"/community/questions\"\n                class=\"text-sm font-medium text-gray-500 hover:text-gray-900 flex items-center group\"\n              >\n                更多\n                <ChevronRightIcon class=\"w-4 h-4 ml-1 group-hover:translate-x-1 transition-transform\" />\n              </router-link>\n            </div>\n            <div class=\"space-y-3 lg:space-y-5\">\n              <div v-for=\"(question, index) in hotQuestions\" :key=\"question.id\"\n                class=\"group cursor-pointer p-3 lg:p-4 rounded-lg lg:rounded-xl hover:bg-gray-50 transition-colors\"\n              >\n                <!-- 问题标题和回答数 -->\n                <div class=\"flex items-start space-x-3\">\n                  <div class=\"flex-shrink-0 w-8 h-8 rounded-lg bg-blue-50 flex items-center justify-center\">\n                    <QuestionMarkCircleIcon class=\"w-5 h-5 text-blue-500\" />\n                  </div>\n                  <div class=\"flex-1 min-w-0\">\n                    <h3 class=\"text-base font-medium text-gray-900 group-hover:text-blue-600 transition-colors line-clamp-2 mb-2\">\n                      {{ question.title }}\n                    </h3>\n                    <div class=\"flex items-center space-x-4\">\n                      <div class=\"flex items-center text-xs text-gr