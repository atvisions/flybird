{"ast":null,"code":"import { ref, computed, watch } from 'vue';\nimport { useRoute } from 'vue-router';\nimport MobileTabBar from '@/components/MobileTabBar.vue';\nimport PageBanner from '@/components/common/PageBanner.vue';\nimport PortfolioNavigation from '@/components/portfolio/PortfolioNavigation.vue';\nimport CategoryMenu from '@/components/portfolio/CategoryMenu.vue';\nimport { useWorks } from '@/composables/useWorks';\nexport default {\n  __name: 'PortfolioList',\n  props: {\n    title: {\n      type: String,\n      required: true\n    },\n    description: {\n      type: String,\n      required: true\n    },\n    theme: {\n      type: String,\n      default: 'blue'\n    },\n    type: {\n      type: String,\n      required: true\n    },\n    categories: {\n      type: Array,\n      required: true\n    },\n    menuGroups: {\n      type: Array,\n      required: true\n    }\n  },\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const props = __props;\n    const route = useRoute();\n\n    // 使用组合式函数获取作品数据\n    const {\n      works,\n      loading,\n      error\n    } = useWorks(props.type);\n\n    // 分类相关状态\n    const currentCategory = ref('all');\n    const showCategoryMenu = ref(false);\n    const currentLevel = ref('main');\n    const currentMainCategory = ref('all');\n    const currentSubCategory = ref('');\n    const currentThirdCategory = ref('');\n\n    // 排序相关状态\n    const showSortMenu = ref(false);\n    const currentSort = ref('popular');\n\n    // ... 其他计算属性和方法保持不变 ...\n\n    // 监听路由参数变化\n    watch(() => route.query.category, newCategory => {\n      if (newCategory) {\n        currentCategory.value = newCategory;\n      }\n    }, {\n      immediate: true\n    });\n    const __returned__ = {\n      props,\n      route,\n      works,\n      loading,\n      error,\n      currentCategory,\n      showCategoryMenu,\n      currentLevel,\n      currentMainCategory,\n      currentSubCategory,\n      currentThirdCategory,\n      showSortMenu,\n      currentSort,\n      ref,\n      computed,\n      watch,\n      get useRoute() {\n        return useRoute;\n      },\n      MobileTabBar,\n      PageBanner,\n      PortfolioNavigation,\n      CategoryMenu,\n      get useWorks() {\n        return useWorks;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","watch","useRoute","MobileTabBar","PageBanner","PortfolioNavigation","CategoryMenu","useWorks","props","__props","route","works","loading","error","type","currentCategory","showCategoryMenu","currentLevel","currentMainCategory","currentSubCategory","currentThirdCategory","showSortMenu","currentSort","query","category","newCategory","value","immediate"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/components/portfolio/PortfolioList.vue"],"sourcesContent":["<template>\n  <div class=\"py-4 lg:py-6 mt-[72px]\">\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n      <!-- 顶部横幅 -->\n      <PageBanner :theme=\"theme\">\n        <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-4\">{{ title }}</h1>\n        <p class=\"text-gray-600 text-lg max-w-2xl\">{{ description }}</p>\n      </PageBanner>\n\n      <!-- 移动端当前分类显示 -->\n      <div class=\"flex items-center justify-between py-3 sm:hidden\">\n        <div class=\"flex items-center space-x-2\">\n          <span class=\"text-sm text-gray-500\">当前：</span>\n          <span class=\"text-sm font-medium text-gray-900\">\n            {{ currentCategoryName }}\n          </span>\n        </div>\n        <button \n          class=\"text-sm text-gray-500 hover:text-gray-900 flex items-center\"\n          @click=\"showCategoryMenu = true\"\n        >\n          切换分类\n          <ChevronRightIcon class=\"w-4 h-4 ml-1\" />\n        </button>\n      </div>\n\n      <!-- 使用导航组件 -->\n      <PortfolioNavigation \n        v-model:currentCategory=\"currentCategory\" \n        class=\"hidden sm:block\"\n        :categories=\"categories\"\n      />\n\n      <!-- 作品列表 -->\n      <div class=\"max-w-7xl mx-auto mt-6\">\n        <div class=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 lg:gap-6\">\n          <div v-for=\"work in filteredWorks\" :key=\"work.id\"\n            class=\"bg-white rounded-lg lg:rounded-xl border border-gray-100 overflow-hidden hover:shadow-lg transition-all duration-300\"\n          >\n            <!-- 作品封面 -->\n            <div class=\"aspect-w-16 aspect-h-9 sm:aspect-w-1 sm:aspect-h-1 bg-gray-100 relative group overflow-hidden\">\n              <img \n                :src=\"work.cover\" \n                class=\"w-full h-full object-cover transform group-hover:scale-110 transition-all duration-500\"\n                alt=\"\"\n              >\n              <!-- 视频时长标记 -->\n              <div v-if=\"work.isVideo\" \n                class=\"absolute bottom-2 right-2 px-2 py-1 bg-black/70 rounded-md text-white text-xs flex items-center\"\n              >\n                <PlayIcon class=\"w-3 h-3 mr-1\" />\n                {{ work.duration }}\n              </div>\n              <!-- 悬浮遮罩 -->\n              <div class=\"absolute inset-0 bg-black/60 invisible group-hover:visible opacity-0 group-hover:opacity-100 transition-all duration-300 flex flex-col items-center justify-center transform group-hover:scale-110\">\n                <span class=\"text-white font-medium mb-2\">{{ categories.find(c => c.id === work.type)?.name }}</span>\n                <div class=\"flex items-center gap-3\">\n                  <span class=\"flex items-center text-gray-300 text-sm\">\n                    <EyeIcon class=\"w-4 h-4 mr-1\" />{{ work.views }}\n                  </span>\n                  <span class=\"flex items-center text-gray-300 text-sm\">\n                    <HeartIcon class=\"w-4 h-4 mr-1\" />{{ work.likes }}\n                  </span>\n                </div>\n              </div>\n            </div>\n\n            <!-- 作品信息 -->\n            <div class=\"p-4 lg:p-4\">\n              <h3 class=\"text-sm font-medium text-gray-900 hover:text-blue-600 transition-colors line-clamp-1 mb-2\">\n                {{ work.title }}\n              </h3>\n              <div class=\"flex items-center justify-between\">\n                <div class=\"flex items-center space-x-2\">\n                  <img :src=\"work.author.avatar\" class=\"w-5 h-5 rounded-full\">\n                  <span class=\"text-sm text-gray-600\">{{ work.author.name }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 分类切换菜单 -->\n    <CategoryMenu\n      v-model:show=\"showCategoryMenu\"\n      v-model:currentLevel=\"currentLevel\"\n      v-model:currentMainCategory=\"currentMainCategory\"\n      v-model:currentSubCategory=\"currentSubCategory\"\n      v-model:currentThirdCategory=\"currentThirdCategory\"\n      :categories=\"categories\"\n      @category-change=\"handleCategoryChange\"\n    />\n\n    <!-- 使用移动端底部导航栏 -->\n    <MobileTabBar :menu-groups=\"menuGroups\" />\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, watch } from 'vue'\nimport { useRoute } from 'vue-router'\nimport MobileTabBar from '@/components/MobileTabBar.vue'\nimport PageBanner from '@/components/common/PageBanner.vue'\nimport PortfolioNavigation from '@/components/portfolio/PortfolioNavigation.vue'\nimport CategoryMenu from '@/components/portfolio/CategoryMenu.vue'\nimport { useWorks } from '@/composables/useWorks'\n\nconst props = defineProps({\n  title: {\n    type: String,\n    required: true\n  },\n  description: {\n    type: String,\n    required: true\n  },\n  theme: {\n    type: String,\n    default: 'blue'\n  },\n  type: {\n    type: String,\n    required: true\n  },\n  categories: {\n    type: Array,\n    required: true\n  },\n  menuGroups: {\n    type: Array,\n    required: true\n  }\n})\n\nconst route = useRoute()\n\n// 使用组合式函数获取作品数据\nconst { works, loading, error } = useWorks(props.type)\n\n// 分类相关状态\nconst currentCategory = ref('all')\nconst showCategoryMenu = ref(false)\nconst currentLevel = ref('main')\nconst currentMainCategory = ref('all')\nconst currentSubCategory = ref('')\nconst currentThirdCategory = ref('')\n\n// 排序相关状态\nconst showSortMenu = ref(false)\nconst currentSort = ref('popular')\n\n// ... 其他计算属性和方法保持不变 ...\n\n// 监听路由参数变化\nwatch(() => route.query.category, (newCategory) => {\n  if (newCategory) {\n    currentCategory.value = newCategory\n  }\n}, { immediate: true })\n</script> "],"mappings":"AAqGA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,KAAK,QAAQ,KAAI;AACzC,SAASC,QAAQ,QAAQ,YAAW;AACpC,OAAOC,YAAY,MAAM,+BAA8B;AACvD,OAAOC,UAAU,MAAM,oCAAmC;AAC1D,OAAOC,mBAAmB,MAAM,gDAA+C;AAC/E,OAAOC,YAAY,MAAM,yCAAwC;AACjE,SAASC,QAAQ,QAAQ,wBAAuB;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;IAEhD,MAAMC,KAAK,GAAGC,OAAA;IA2Bd,MAAMC,KAAK,GAAGR,QAAQ,CAAC;;IAEvB;IACA,MAAM;MAAES,KAAK;MAAEC,OAAO;MAAEC;IAAM,CAAC,GAAGN,QAAQ,CAACC,KAAK,CAACM,IAAI;;IAErD;IACA,MAAMC,eAAe,GAAGhB,GAAG,CAAC,KAAK;IACjC,MAAMiB,gBAAgB,GAAGjB,GAAG,CAAC,KAAK;IAClC,MAAMkB,YAAY,GAAGlB,GAAG,CAAC,MAAM;IAC/B,MAAMmB,mBAAmB,GAAGnB,GAAG,CAAC,KAAK;IACrC,MAAMoB,kBAAkB,GAAGpB,GAAG,CAAC,EAAE;IACjC,MAAMqB,oBAAoB,GAAGrB,GAAG,CAAC,EAAE;;IAEnC;IACA,MAAMsB,YAAY,GAAGtB,GAAG,CAAC,KAAK;IAC9B,MAAMuB,WAAW,GAAGvB,GAAG,CAAC,SAAS;;IAEjC;;IAEA;IACAE,KAAK,CAAC,MAAMS,KAAK,CAACa,KAAK,CAACC,QAAQ,EAAGC,WAAW,IAAK;MACjD,IAAIA,WAAW,EAAE;QACfV,eAAe,CAACW,KAAK,GAAGD,WAAU;MACpC;IACF,CAAC,EAAE;MAAEE,SAAS,EAAE;IAAK,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}