{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { DocumentDuplicateIcon, ArrowTopRightOnSquareIcon } from '@heroicons/vue/24/outline';\nimport { showToast } from '@/components/ToastMessage';\n\n// 模拟用户数据\n\nexport default {\n  __name: 'MyHomepage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userInfo = ref({\n      name: '张三',\n      title: '高级前端工程师',\n      avatar: 'https://picsum.photos/200/200',\n      bio: '热爱技术，专注于前端开发和用户体验设计。5年+开发经验，擅长Vue.js和React技术栈。'\n    });\n\n    // 生成页面URL\n    const pageUrl = computed(() => {\n      return `${window.location.origin}/u/${userInfo.value.name}`;\n    });\n\n    // 生成二维码URL（这里需要集成二维码生成库）\n    const qrCodeUrl = computed(() => {\n      // 临时使用示例图片\n      return 'https://picsum.photos/200/200';\n    });\n\n    // 复制链接\n    const copyLink = async () => {\n      try {\n        await navigator.clipboard.writeText(pageUrl.value);\n        showToast('链接已复制到剪贴板', 'success');\n      } catch (error) {\n        showToast('复制失败，请手动复制', 'error');\n      }\n    };\n    const __returned__ = {\n      userInfo,\n      pageUrl,\n      qrCodeUrl,\n      copyLink,\n      ref,\n      computed,\n      get DocumentDuplicateIcon() {\n        return DocumentDuplicateIcon;\n      },\n      get ArrowTopRightOnSquareIcon() {\n        return ArrowTopRightOnSquareIcon;\n      },\n      get showToast() {\n        return showToast;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","DocumentDuplicateIcon","ArrowTopRightOnSquareIcon","showToast","userInfo","name","title","avatar","bio","pageUrl","window","location","origin","value","qrCodeUrl","copyLink","navigator","clipboard","writeText","error"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/user/MyProfile/components/MyHomepage.vue"],"sourcesContent":["<template>\n  <div class=\"bg-white rounded-lg shadow\">\n    <div class=\"px-6 py-4 border-b border-gray-200\">\n      <h3 class=\"text-lg font-medium text-gray-900\">我的主页</h3>\n    </div>\n    <div class=\"p-6\">\n      <!-- 主页预览卡片 -->\n      <div class=\"mb-6 p-4 bg-gray-50 rounded-lg overflow-hidden\">\n        <div class=\"flex items-center mb-4\">\n          <img \n            :src=\"userInfo.avatar || 'https://picsum.photos/200/200'\" \n            class=\"w-16 h-16 rounded-full\"\n            alt=\"头像\"\n          >\n          <div class=\"ml-4\">\n            <h4 class=\"text-lg font-medium text-gray-900\">{{ userInfo.name }}</h4>\n            <p class=\"text-sm text-gray-500\">{{ userInfo.title }}</p>\n          </div>\n        </div>\n        <p class=\"text-gray-600\">{{ userInfo.bio }}</p>\n      </div>\n\n      <!-- 主页链接 -->\n      <div class=\"space-y-4\">\n        <div class=\"flex flex-col sm:flex-row sm:items-center gap-4 p-4 bg-gray-50 rounded-lg\">\n          <div class=\"flex-1 mr-4\">\n            <p class=\"text-sm text-gray-500 mb-1\">我的主页链接</p>\n            <div class=\"flex items-center\">\n              <span class=\"text-gray-900 truncate text-sm\">{{ pageUrl }}</span>\n            </div>\n          </div>\n          <div class=\"flex items-center space-x-2 w-full sm:w-auto justify-end\">\n            <button \n              @click=\"copyLink\"\n              class=\"flex-1 sm:flex-none inline-flex items-center justify-center px-3 py-1.5 text-sm font-medium text-gray-700 bg-white border border-gray-300 rounded-md hover:bg-gray-50\"\n            >\n              <DocumentDuplicateIcon class=\"w-4 h-4 mr-1\" />\n              复制链接\n            </button>\n            <a \n              :href=\"pageUrl\"\n              target=\"_blank\"\n              class=\"flex-1 sm:flex-none inline-flex items-center justify-center px-3 py-1.5 text-sm font-medium text-white bg-indigo-600 rounded-md hover:bg-indigo-700\"\n            >\n              <ArrowTopRightOnSquareIcon class=\"w-4 h-4 mr-1\" />\n              访问主页\n            </a>\n          </div>\n        </div>\n\n        <!-- 二维码分享 -->\n        <div class=\"flex flex-col sm:flex-row sm:items-center justify-between p-4 bg-gray-50 rounded-lg\">\n          <div>\n            <p class=\"text-sm text-gray-500 mb-1\">主页二维码</p>\n            <p class=\"text-xs text-gray-400\">扫描二维码，在手机上查看</p>\n          </div>\n          <div class=\"w-24 h-24 bg-white p-2 rounded-lg mt-4 sm:mt-0 mx-auto sm:mx-0\">\n            <!-- 这里需要集成二维码生成库 -->\n            <img \n              :src=\"qrCodeUrl\" \n              class=\"w-full h-full\"\n              alt=\"二维码\"\n            >\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport { \n  DocumentDuplicateIcon, \n  ArrowTopRightOnSquareIcon \n} from '@heroicons/vue/24/outline'\nimport { showToast } from '@/components/ToastMessage'\n\n// 模拟用户数据\nconst userInfo = ref({\n  name: '张三',\n  title: '高级前端工程师',\n  avatar: 'https://picsum.photos/200/200',\n  bio: '热爱技术，专注于前端开发和用户体验设计。5年+开发经验，擅长Vue.js和React技术栈。'\n})\n\n// 生成页面URL\nconst pageUrl = computed(() => {\n  return `${window.location.origin}/u/${userInfo.value.name}`\n})\n\n// 生成二维码URL（这里需要集成二维码生成库）\nconst qrCodeUrl = computed(() => {\n  // 临时使用示例图片\n  return 'https://picsum.photos/200/200'\n})\n\n// 复制链接\nconst copyLink = async () => {\n  try {\n    await navigator.clipboard.writeText(pageUrl.value)\n    showToast('链接已复制到剪贴板', 'success')\n  } catch (error) {\n    showToast('复制失败，请手动复制', 'error')\n  }\n}\n</script> "],"mappings":"AAuEA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAI;AAClC,SACEC,qBAAqB,EACrBC,yBAAyB,QACpB,2BAA0B;AACjC,SAASC,SAAS,QAAQ,2BAA0B;;AAEpD;;;;;;;;IACA,MAAMC,QAAQ,GAAGL,GAAG,CAAC;MACnBM,IAAI,EAAE,IAAI;MACVC,KAAK,EAAE,SAAS;MAChBC,MAAM,EAAE,+BAA+B;MACvCC,GAAG,EAAE;IACP,CAAC;;IAED;IACA,MAAMC,OAAO,GAAGT,QAAQ,CAAC,MAAM;MAC7B,OAAO,GAAGU,MAAM,CAACC,QAAQ,CAACC,MAAM,MAAMR,QAAQ,CAACS,KAAK,CAACR,IAAI,EAAC;IA