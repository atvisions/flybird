{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport { ref, onMounted, onUnmounted, watch } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport ToolbarPanel from './components/ToolbarPanel.vue';\nimport Canvas from './components/Canvas.vue';\nimport { VExpansionPanels, VExpansionPanel, VExpansionPanelTitle, VExpansionPanelText } from 'vuetify/components';\nimport PropertyPanel from './components/PropertyPanel.vue';\nimport { useProfileStore } from '@/stores/profile';\nimport { useMessage } from 'naive-ui';\nimport SaveTemplateDialog from './components/SaveTemplateDialog.vue';\nimport templateApi from '@/api/template';\nexport default {\n  __name: 'index',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const scale = ref(1);\n    let isDragging = false;\n    let startX = 0;\n    let startScale = 0;\n    const activeTab = ref('layout');\n\n    // 工具栏 tabs 数据\n    const toolbarTabs = ref([{\n      value: 'layout',\n      text: '布局',\n      icon: 'mdi-view-grid'\n    }, {\n      value: 'components',\n      text: '组件',\n      icon: 'mdi-puzzle'\n    }, {\n      value: 'files',\n      text: '档案',\n      icon: 'mdi-file'\n    }]);\n\n    // 获取主分类\n    const fetchMainCategories = async () => {\n      try {\n        const response = await axios.get('/api/v1/resume/component-categories/main_categories/');\n        const mainCategories = response.data.map(category => ({\n          value: category.code,\n          text: category.name,\n          icon: category.icon || 'mdi-view-grid'\n        }));\n        if (mainCategories.length > 0) {\n          toolbarTabs.value = mainCategories;\n          // 如果当前选中的 tab 不在新的分类列表中，则选择第一个分类\n          if (!mainCategories.find(tab => tab.value === activeTab.value)) {\n            activeTab.value = mainCategories[0].value;\n          }\n        }\n      } catch (error) {\n        console.error('获取主分类失败:', error);\n      }\n    };\n\n    // 获取档案数据\n    const profileStore = useProfileStore();\n\n    // 处理模板变更\n    const handleTemplateChange = async template => {\n      console.log('选择的模板:', template);\n\n      // 创建新简历\n      await createResumeFromTemplate(template);\n    };\n    onMounted(() => {\n      fetchMainCategories();\n    });\n\n    // 历史记录状态\n    const canUndo = ref(true);\n    const canRedo = ref(true);\n\n    // 历史记录相关方法\n    const handleUndo = () => {\n      console.log('undo');\n    };\n    const handleRedo = () => {\n      console.log('redo');\n    };\n\n    // 缩放相关方法\n    const startDrag = e => {\n      isDragging = true;\n      startX = e.clientX;\n      startScale = scale.value;\n      document.addEventListener('mousemove', onDrag);\n      document.addEventListener('mouseup', stopDrag);\n    };\n    const onDrag = e => {\n      if (!isDragging) return;\n      const track = document.querySelector('.zoom-track');\n      const trackRect = track.getBoundingClientRect();\n      const trackWidth = trackRect.width;\n      const deltaX = e.clientX - startX;\n      const deltaScale = deltaX / trackWidth * 1.5;\n      let newScale = startScale + deltaScale;\n      newScale = Math.min(2, Math.max(0.5, newScale));\n      newScale = Math.round(newScale * 10) / 10;\n      scale.value = newScale;\n    };\n    const stopDrag = () => {\n      isDragging = false;\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    };\n    onUnmounted(() => {\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    });\n    const handleExit = () => {\n      router.push('/resumes');\n    };\n\n    // 当前模板数据\n    const currentTemplate = ref(null);\n\n    // 处理属性面板的更新\n    const handlePropertyUpdate = updatedTemplate => {\n      console.log('属性更新:', updatedTemplate);\n      if (canvasRef.value) {\n        canvasRef.value.loadTemplate(updatedTemplate);\n      }\n    };\n\n    // 监听模板属性变化\n    watch(currentTemplate, newTemplate => {\n      if (newTemplate && canvasRef.value) {\n        canvasRef.value.loadTemplate(newTemplate);\n      }\n    }, {\n      deep: true\n    });\n\n    // 获取 Canvas 组件的引用\n    const canvasRef = ref(null);\n\n    // 更新 Canvas 组件的引用\n    const updateCanvasRef = el => {\n      canvasRef.value = el;\n    };\n    const message = useMessage();\n\n    // 模板表单数据\n    const templateForm = ref({\n      name: '',\n      description: ''\n    });\n\n    // 保存模板对话框\n    const saveTemplateDialog = ref(false);\n\n    // 显示保存模板对话框\n    const showSaveTemplateDialog = ref(false);\n\n    // 处理保存模板\n    const handleSaveTemplate = async formData => {\n      if (!canvasRef.value) return;\n      try {\n        // 获取当前画布的布局数据\n        const layoutData = canvasRef.value.exportLayoutData();\n        const layout = layoutData[0];\n\n        // 构建模板数据\n        const templateData = {\n          name: formData.name,\n          description: formData.description,\n          // 布局配置\n          layout: {\n            type: layout.config.type || 'grid',\n            areas: layout.config.areas || [],\n            columns: layout.config.columns || 12,\n            gap: layout.config.gap || '20px'\n          },\n          // 组件配置\n          components: layout.config.components.map(comp => ({\n            id: comp.id,\n            type: comp.type,\n            name: comp.name,\n            area: comp.area || 'main',\n            config: {\n              fields: {},\n              // 清空字段数据，只保留配置\n              styles: comp.config.styles || {},\n              layout: comp.config.layout || {}\n            }\n          })),\n          // 主题配置\n          theme: {\n            colors: currentTemplate.value?.theme?.colors || {\n              primary: '#1867C0',\n              secondary: '#5CBBF6',\n              accent: '#4CAF50',\n              background: '#FFFFFF',\n              text: '#2C3E50',\n              border: '#E0E0E0'\n            },\n            typography: currentTemplate.value?.theme?.typography || {\n              body: {\n                font: 'Arial',\n                size: '14px',\n                weight: 'normal'\n              },\n              heading: {\n                font: 'Arial',\n                size: '16px',\n                weight: 'bold'\n              }\n            },\n            spacing: currentTemplate.value?.theme?.spacing || {\n              padding: '16px',\n              margin: '16px',\n              gap: '16px'\n            }\n          },\n          // TODO: 生成预览图\n          thumbnail: ''\n        };\n        console.log('保存的模板数据:', templateData);\n\n        // 调用保存模板接口\n        const response = await templateApi.createTemplate(templateData);\n        message.success('模板保存成功');\n        showSaveTemplateDialog.value = false;\n        return response.data;\n      } catch (error) {\n        message.error('保存模板失败：' + error.message);\n        console.error('保存模板失败:', error);\n      }\n    };\n\n    // 保存简历\n    const saveResume = async () => {\n      if (!canvasRef.value) return;\n      try {\n        // 获取当前画布的布局数据\n        const layoutData = canvasRef.value.exportLayoutData();\n\n        // 构建简历数据\n        const resumeData = {\n          name: '我的简历',\n          // TODO: 可以让用户输入\n          content: {\n            layout: currentTemplate.value.layout,\n            components: layoutData[0].config.components,\n            theme: currentTemplate.value.theme\n          }\n        };\n\n        // 调用保存简历接口\n        const response = await axios.post('/api/v1/resume/resumes/', resumeData);\n        message.success('简历保存成功');\n        return response.data;\n      } catch (error) {\n        message.error('保存简历失败：' + error.message);\n        console.error('保存简历失败:', error);\n      }\n    };\n\n    // 使用模板创建简历\n    const createResumeFromTemplate = async template => {\n      try {\n        // 1. 创建新简历\n        const resumeData = {\n          name: '我的简历',\n          // 可以弹窗让用户输入\n          template_id: template.id,\n          content: {\n            layout: template.layout,\n            components: template.components,\n            theme: template.theme\n          }\n        };\n        const response = await axios.post('/api/v1/resume/resumes/', resumeData);\n        message.success('简历创建成功');\n\n        // 2. 跳转到编辑器\n        router.push({\n          name: 'resume-editor',\n          params: {\n            id: response.data.id\n          }\n        });\n      } catch (error) {\n        message.error('创建简历失败：' + error.message);\n        console.error('创建简历失败:', error);\n      }\n    };\n    const __returned__ = {\n      router,\n      scale,\n      get isDragging() {\n        return isDragging;\n      },\n      set isDragging(v) {\n        isDragging = v;\n      },\n      get startX() {\n        return startX;\n      },\n      set startX(v) {\n        startX = v;\n      },\n      get startScale() {\n        return startScale;\n      },\n      set startScale(v) {\n        startScale = v;\n      },\n      activeTab,\n      toolbarTabs,\n      fetchMainCategories,\n      profileStore,\n      handleTemplateChange,\n      canUndo,\n      canRedo,\n      handleUndo,\n      handleRedo,\n      startDrag,\n      onDrag,\n      stopDrag,\n      handleExit,\n      currentTemplate,\n      handlePropertyUpdate,\n      canvasRef,\n      updateCanvasRef,\n      message,\n      templateForm,\n      saveTemplateDialog,\n      showSaveTemplateDialog,\n      handleSaveTemplate,\n      saveResume,\n      createResumeFromTemplate,\n      ref,\n      onMounted,\n      onUnmounted,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get axios() {\n        return axios;\n      },\n      ToolbarPanel,\n      Canvas,\n      get VExpansionPanels() {\n        return VExpansionPanels;\n      },\n      get VExpansionPanel() {\n        return VExpansionPanel;\n      },\n      get VExpansionPanelTitle() {\n        return VExpansionPanelTitle;\n      },\n      get VExpansionPanelText() {\n        return VExpansionPanelText;\n      },\n      PropertyPanel,\n      get useProfileStore() {\n        return useProfileStore;\n      },\n      get useMessage() {\n        return useMessage;\n      },\n      SaveTemplateDialog,\n      get templateApi() {\n        return templateApi;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","watch","useRouter","axios","ToolbarPanel","Canvas","VExpansionPanels","VExpansionPanel","VExpansionPanelTitle","VExpansionPanelText","PropertyPanel","useProfileStore","useMessage","SaveTemplateDialog","templateApi","router","scale","isDragging","startX","startScale","activeTab","toolbarTabs","value","text","icon","fetchMainCategories","response","get","mainCategories","data","map","category","code","name","length","find","tab","error","console","profileStore","handleTemplateChange","template","log","createResumeFromTemplate","canUndo","canRedo","handleUndo","handleRedo","startDrag","e","clientX","document","addEventListener","onDrag","stopDrag","track","querySelector","trackRect","getBoundingClientRect","trackWidth","width","deltaX","deltaScale","newScale","Math","min","max","round","removeEventListener","handleExit","push","currentTemplate","handlePropertyUpdate","updatedTemplate","canvasRef","loadTemplate","newTemplate","deep","updateCanvasRef","el","message","templateForm","description","saveTemplateDialog","showSaveTemplateDialog","handleSaveTemplate","formData","layoutData","exportLayoutData","layout","templateData","type","config","areas","columns","gap","components","comp","id","area","fields","styles","theme","colors","primary","secondary","accent","background","border","typography","body","font","size","weight","heading","spacing","padding","margin","thumbnail","createTemplate","success","saveResume","resumeData","content","post","template_id","params"],"sources":["/Users/liuzhao/Documents/Projects/flybird/flybird-web/src/views/resume-editor/index.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"resume-editor\">\n      <!-- 顶部栏 -->\n      <v-app-bar class=\"editor-header\" elevation=\"0\" height=\"60\">\n        <!-- 左侧：退出箭头和简历名称 -->\n        <div class=\"d-flex align-center justify-space-between w-100\">\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"返回\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon @click=\"handleExit\" v-bind=\"props\">\n                  <v-icon>mdi-arrow-left</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-divider vertical class=\"mx-3\"></v-divider>\n            <span class=\"text-h6\">我的简历</span>\n          </div>\n\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"撤销\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" :disabled=\"!canUndo\" v-bind=\"props\" @click=\"handleUndo\">\n                  <v-icon>mdi-undo</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"重做\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon :disabled=\"!canRedo\" v-bind=\"props\" @click=\"handleRedo\">\n                  <v-icon>mdi-redo</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n          </div>\n\n          <div class=\"d-flex align-center\">\n            <v-tooltip text=\"保存为模板\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\" @click=\"showSaveTemplateDialog = true\">\n                  <v-icon>mdi-content-save-outline</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"草稿箱\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\">\n                  <v-icon>mdi-file-document-outline</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"打印预览\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon class=\"mr-2\" v-bind=\"props\">\n                  <v-icon>mdi-printer</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n            <v-tooltip text=\"保存\" location=\"bottom\">\n              <template v-slot:activator=\"{ props }\">\n                <v-btn icon v-bind=\"props\" @click=\"saveResume\">\n                  <v-icon>mdi-content-save</v-icon>\n                </v-btn>\n              </template>\n            </v-tooltip>\n          </div>\n        </div>\n      </v-app-bar>\n\n      <!-- 主要内容区域 -->\n      <div class=\"editor-main\">\n        <!-- 左侧工具栏 -->\n        <div class=\"editor-toolbar\">\n          <div class=\"d-flex\">\n            <ToolbarPanel \n              v-model=\"activeTab\" \n              @template-change=\"handleTemplateChange\"\n            />\n\n            <!-- 竖向标签页 -->\n            <div class=\"vertical-tabs\">\n              <div class=\"tab-items\">\n                <div class=\"tab-item\" \n                  v-for=\"tab in toolbarTabs\"\n                  :key=\"tab.value\"\n                  :class=\"{ active: activeTab === tab.value }\"\n                  @click=\"activeTab = tab.value\">\n                  <span class=\"tab-text\">{{ tab.text }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 中间画布区域 -->\n        <div class=\"editor-canvas\">\n          <Canvas ref=\"canvasRef\" :scale=\"scale\" />\n        </div>\n\n        <!-- 右侧操作栏 -->\n        <div class=\"editor-properties\">\n          <PropertyPanel \n            v-model=\"currentTemplate\"\n            @update:model-value=\"handlePropertyUpdate\"\n          />\n        </div>\n      </div>\n\n      <!-- 底部通栏 -->\n      <div class=\"editor-footer\">\n        <div class=\"d-flex align-center justify-center h-100\">\n          <div class=\"zoom-control\">\n            <span class=\"zoom-label zoom-label-left\">50%</span>\n            <div class=\"zoom-track\">\n              <div class=\"zoom-slider\" \n                :style=\"{ left: ((scale - 0.5) / 1.5 * 100) + '%' }\"\n                @mousedown=\"startDrag\">\n                <div class=\"zoom-value\">{{ Math.round(scale * 100) }}%</div>\n              </div>\n              <div class=\"zoom-line\"></div>\n            </div>\n            <span class=\"zoom-label zoom-label-right\">200%</span>\n          </div>\n        </div>\n      </div>\n\n      <!-- 添加保存模板对话框 -->\n      <v-dialog v-model=\"saveTemplateDialog\" max-width=\"500\">\n        <v-card>\n          <v-card-title>保存为模板</v-card-title>\n          <v-card-text>\n            <v-form ref=\"templateForm\">\n              <v-text-field\n                v-model=\"templateForm.name\"\n                label=\"模板名称\"\n                required\n                :rules=\"[v => !!v || '请输入模板名称']\"\n              ></v-text-field>\n              <v-textarea\n                v-model=\"templateForm.description\"\n                label=\"模板描述\"\n                rows=\"3\"\n              ></v-textarea>\n            </v-form>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"grey-darken-1\" variant=\"text\" @click=\"saveTemplateDialog = false\">\n              取消\n            </v-btn>\n            <v-btn color=\"primary\" variant=\"text\" @click=\"handleSaveTemplate\">\n              保存\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n\n      <SaveTemplateDialog\n        v-model=\"showSaveTemplateDialog\"\n        @save=\"handleSaveTemplate\"\n      />\n    </div>\n  </v-app>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, watch } from 'vue'\nimport { useRouter } from 'vue-router'\nimport axios from 'axios'\nimport ToolbarPanel from './components/ToolbarPanel.vue'\nimport Canvas from './components/Canvas.vue'\nimport { VExpansionPanels, VExpansionPanel, VExpansionPanelTitle, VExpansionPanelText } from 'vuetify/components'\nimport PropertyPanel from './components/PropertyPanel.vue'\nimport { useProfileStore } from '@/stores/profile'\nimport { useMessage } from 'naive-ui'\nimport SaveTemplateDialog from './components/SaveTemplateDialog.vue'\nimport templateApi from '@/api/template'\n\nconst router = useRouter()\nconst scale = ref(1)\nlet isDragging = false\nlet startX = 0\nlet startScale = 0\nconst activeTab = ref('layout')\n\n// 工具栏 tabs 数据\nconst toolbarTabs = ref([\n  { value: 'layout', text: '布局', icon: 'mdi-view-grid' },\n  { value: 'components', text: '组件', icon: 'mdi-puzzle' },\n  { value: 'files', text: '档案', icon: 'mdi-file' }\n])\n\n// 获取主分类\nconst fetchMainCategories = async () => {\n  try {\n    const response = await axios.get('/api/v1/resume/component-categories/main_categories/')\n    const mainCategories = response.data.map(category => ({\n      value: category.code,\n      text: category.name,\n      icon: category.icon || 'mdi-view-grid'\n    }))\n    \n    if (mainCategories.length > 0) {\n      toolbarTabs.value = mainCategories\n      // 如果当前选中的 tab 不在新的分类列表中，则选择第一个分类\n      if (!mainCategories.find(tab => tab.value === activeTab.value)) {\n        activeTab.value = mainCategories[0].value\n      }\n    }\n  } catch (error) {\n    console.error('获取主分类失败:', error)\n  }\n}\n\n// 获取档案数据\nconst profileStore = useProfileStore()\n\n// 处理模板变更\nconst handleTemplateChange = async (template) => {\n  console.log('选择的模板:', template)\n  \n  // 创建新简历\n  await createResumeFromTemplate(template)\n}\n\nonMounted(() => {\n  fetchMainCategories()\n})\n\n// 历史记录状态\nconst canUndo = ref(true)\nconst canRedo = ref(true)\n\n// 历史记录相关方法\nconst handleUndo = () => {\n  console.log('undo')\n}\n\nconst handleRedo = () => {\n  console.log('redo')\n}\n\n// 缩放相关方法\nconst startDrag = (e) => {\n  isDragging = true\n  startX = e.clientX\n  startScale = scale.value\n  document.addEventListener('mousemove', onDrag)\n  document.addEventListener('mouseup', stopDrag)\n}\n\nconst onDrag = (e) => {\n  if (!isDragging) return\n  \n  const track = document.querySelector('.zoom-track')\n  const trackRect = track.getBoundingClientRect()\n  const trackWidth = trackRect.width\n  const deltaX = e.clientX - startX\n  const deltaScale = (deltaX / trackWidth) * 1.5\n\n  let newScale = startScale + deltaScale\n  newScale = Math.min(2, Math.max(0.5, newScale))\n  newScale = Math.round(newScale * 10) / 10\n  \n  scale.value = newScale\n}\n\nconst stopDrag = () => {\n  isDragging = false\n  document.removeEventListener('mousemove', onDrag)\n  document.removeEventListener('mouseup', stopDrag)\n}\n\nonUnmounted(() => {\n  document.removeEventListener('mousemove', onDrag)\n  document.removeEventListener('mouseup', stopDrag)\n})\n\nconst handleExit = () => {\n  router.push('/resumes')\n}\n\n// 当前模板数据\nconst currentTemplate = ref(null)\n\n// 处理属性面板的更新\nconst handlePropertyUpdate = (updatedTemplate) => {\n  console.log('属性更新:', updatedTemplate)\n  if (canvasRef.value) {\n    canvasRef.value.loadTemplate(updatedTemplate)\n  }\n}\n\n// 监听模板属性变化\nwatch(currentTemplate, (newTemplate) => {\n  if (newTemplate && canvasRef.value) {\n    canvasRef.value.loadTemplate(newTemplate)\n  }\n}, { deep: true })\n\n// 获取 Canvas 组件的引用\nconst canvasRef = ref(null)\n\n// 更新 Canvas 组件的引用\nconst updateCanvasRef = (el) => {\n  canvasRef.value = el\n}\n\nconst message = useMessage()\n\n// 模板表单数据\nconst templateForm = ref({\n  name: '',\n  description: ''\n})\n\n// 保存模板对话框\nconst saveTemplateDialog = ref(false)\n\n// 显示保存模板对话框\nconst showSaveTemplateDialog = ref(false)\n\n// 处理保存模板\nconst handleSaveTemplate = async (formData) => {\n  if (!canvasRef.value) return\n  \n  try {\n    // 获取当前画布的布局数据\n    const layoutData = canvasRef.value.exportLayoutData()\n    const layout = layoutData[0]\n    \n    // 构建模板数据\n    const templateData = {\n      name: formData.name,\n      description: formData.description,\n      // 布局配置\n      layout: {\n        type: layout.config.type || 'grid',\n        areas: layout.config.areas || [],\n        columns: layout.config.columns || 12,\n        gap: layout.config.gap || '20px'\n      },\n      // 组件配置\n      components: layout.config.components.map(comp => ({\n        id: comp.id,\n        type: comp.type,\n        name: comp.name,\n        area: comp.area || 'main',\n        config: {\n          fields: {}, // 清空字段数据，只保留配置\n          styles: comp.config.styles || {},\n          layout: comp.config.layout || {}\n        }\n      })),\n      // 主题配置\n      theme: {\n        colors: currentTemplate.value?.theme?.colors || {\n          primary: '#1867C0',\n          secondary: '#5CBBF6',\n          accent: '#4CAF50',\n          background: '#FFFFFF',\n          text: '#2C3E50',\n          border: '#E0E0E0'\n        },\n        typography: currentTemplate.value?.theme?.typography || {\n          body: {\n            font: 'Arial',\n            size: '14px',\n            weight: 'normal'\n          },\n          heading: {\n            font: 'Arial',\n            size: '16px',\n            weight: 'bold'\n          }\n        },\n        spacing: currentTemplate.value?.theme?.spacing || {\n          padding: '16px',\n          margin: '16px',\n          gap: '16px'\n        }\n      },\n      // TODO: 生成预览图\n      thumbnail: ''\n    }\n    \n    console.log('保存的模板数据:', templateData)\n    \n    