 const totalScore = computed(() => {\n      const score = props.completionData?.total_score || 0;\n      return Number(score.toFixed(1));\n    });\n\n    // 维度数据 - 保留整数\n    const dimensions = computed(() => {\n      const dims = props.completionData?.dimensions || {};\n      return Object.entries(dims).reduce((acc, [key, value]) => {\n        acc[key] = _objectSpread(_objectSpread({}, value), {}, {\n          score: Math.round(value.score || 0),\n          weighted_score: value.weighted_score || 0\n        });\n        return acc;\n      }, {});\n    });\n\n    // 建议\n    const suggestions = computed(() => {\n      const rawSuggestions = props.completionData?.suggestions || [];\n      // 只返回建议消息\n      return rawSuggestions.map(suggestion => suggestion.message);\n    });\n\n    // 维度名称映射\n    const getDimensionName = key => {\n      const nameMap = {\n        basic_info: '基本信息',\n        experience: '工作经历',\n        capability: '能力素质',\n        achievement: '个人成就'\n      };\n      return nameMap[key] || key;\n    };\n\n    // 进度条颜色 - 根据实际分数范围调整\n    const progressColorClass = computed(() => {\n      const score = totalScore.value;\n      if (score >= 70) return 'bg-green-500';\n      if (score >= 40) return 'bg-yellow-500';\n      return 'bg-red-500';\n    });\n\n    // 文字颜色 - 与进度条颜色对应\n    const progressTextColorClass = computed(() => {\n      const score = totalScore.value;\n      if (score >= 70) return 'text-green-600';\n      if (score >= 40) return 'text-yellow-600';\n      return 'text-red-600';\n    });\n\n    // 处理预览按钮点击\n    const handlePreview = () => {\n      showPreview.value = true;\n    };\n\n    // AI优化相关\n    const showOptimize = ref(false);\n\n    // 处理优化结果\n    const handleOptimizeApply = optimizedData => {\n      console.log('优化后的数据:', optimizedData);\n      // 这里处理优化后的数据，可能需要调用 API 更新档案\n    };\n\n    // 监听 profileData 变化，确保数据有效\n    watch(() => props.profileData, newVal => {\n      if (!newVal || Object.keys(newVal).length === 0) {\n        return;\n      }\n      // 处理数据...\n    }, {\n      immediate: true,\n      deep: true\n    });\n\n    // 处理导入按钮点击\n    const handleImportClick = () => {\n      if (!subscriptionStore.isSubscribed) {\n        router.push('/pro');\n      } else {\n        showImportDialog.value = true;\n      }\n    };\n\n    // 处理导入成功\n    const handleImportSuccess = () => {\n      showImportDialog.value = false;\n      // 触发父组件更新数据\n      emit('update');\n    };\n    const __returned__ = {\n      router,\n      subscriptionStore,\n      showProDialog,\n      showImportDialog,\n      props,\n      emit,\n      showPreview,\n      width,\n      isMobile,\n      dialogWidth,\n      totalScore,\n      dimensions,\n      suggestions,\n      getDimensionName,\n      progressColorClass,\n      progressTextColorClass,\n      handlePreview,\n      showOptimize,\n      handleOptimizeApply,\n      handleImportClick,\n      handleImportSuccess,\n      ref,\n      computed,\n      watch,\n      get useWindowSize() {\n        return useWindowSize;\n      },\n      get SparklesIcon() {\n        return SparklesIcon;\n      },\n      get ArrowDownTrayIcon() {\n        return ArrowDownTrayIcon;\n      },\n      get EyeIcon() {\n        return EyeIcon;\n      },\n      get XMarkIcon() {\n        return XMarkIcon;\n      },\n      get LightBulbIcon() {\n        return LightBulbIcon;\n      },\n      ProfilePreview,\n      AIOptimizeDialog,\n      get Dialog() {\n        return Dialog;\n      },\n      get DialogPanel() {\n        return DialogPanel;\n      },\n      get DialogTitle() {\n        return DialogTitle;\n      },\n      get TransitionChild() {\n        return TransitionChild;\n      },\n      get TransitionRoot() {\n        return TransitionRoot;\n      },\n      ResumeImport,\n      get useSubscriptionStore() {\n        return useSubscriptionStore;\n      },\n      ProDialog,\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isSc